<Window x:Class="Resocentro_Desktop.Interfaz.frmUtil.frmMedico"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="frmMedico" Height="222" Width="340"  Background="{DynamicResource Windows.Background}">
    <Grid>
        <StackPanel>
            <StackPanel Orientation="Horizontal">
                <Label Content="Tipo"/>
                <ComboBox></ComboBox>
            </StackPanel>
            <StackPanel Orientation="Horizontal">
                <Label Content="CMP"/>
                <TextBox Name="txtcmp" MinWidth="50"/>
            </StackPanel>
            <StackPanel Orientation="Horizontal">
                <Label Content="Nombres"/>
                <TextBox Name="txtnombres" MinWidth="200"/>
            </StackPanel>
            <StackPanel Orientation="Horizontal">
                <Label Content="Apellidos"/>
                <TextBox Name="txtapellidos" MinWidth="200"/>
            </StackPanel>
            <StackPanel Orientation="Horizontal">
                <Label Content="Apellidos"/>
                <ComboBox Name="u" />
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
