<Window x:Class="Resocentro_Desktop.Interfaz.Cobranza.frmSeleccionarModalidad"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Seleccionar Modalidad" Background="{DynamicResource Windows.Background}" Height="126" Width="288" WindowStyle="ToolWindow" WindowStartupLocation="CenterOwner">
    <Grid>
        <ComboBox x:Name="cboModalidad" MinHeight="23" Margin="10,22,0,0" Width="260" HorizontalAlignment="Left" FocusVisualStyle="{StaticResource MyFocusVisual}" TabIndex="5" Height="28" VerticalAlignment="Top"/>
        <Label Content="Modalidad" Padding="4" HorizontalAlignment="Left" Width="96" Height="29" VerticalAlignment="Top"/>
        <Button Content="Seleccionar" Padding="5,2" HorizontalAlignment="Left" Margin="180,64,0,0" VerticalAlignment="Top" Width="90" Click="Button_Click"/>

    </Grid>
</Window>
