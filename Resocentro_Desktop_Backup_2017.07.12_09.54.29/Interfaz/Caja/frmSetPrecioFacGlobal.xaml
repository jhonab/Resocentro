<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation" x:Class="Resocentro_Desktop.Interfaz.frmSetPrecioFacGlobal"
        Title="frmSetPrecioFacGlobal" Height="110" Width="313" Background="{DynamicResource Windows.Background}" WindowStyle="ToolWindow" WindowStartupLocation="CenterOwner">
    <Grid>
        <Label Content="Moneda" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Label Content="Precio" HorizontalAlignment="Left" Margin="10,41,0,0" VerticalAlignment="Top"/>
        <ComboBox Name="cbomoneda" HorizontalAlignment="Left" Margin="71,10,0,0" SelectedIndex="0" VerticalAlignment="Top" Width="79">
            <ComboBoxItem Content="- Seleccione -"/>
            <ComboBoxItem Content="SOLES"/>
            <ComboBoxItem Content="DOLARES"/>
        </ComboBox>
        <telerik:RadNumericUpDown Name="txtprecio" HorizontalAlignment="Left" Margin="71,41,0,0" VerticalAlignment="Top" Width="120" NullValue="0" Minimum="0" NumberDecimalDigits="2" LargeChange="1"/>
        <Button Content="Guardar"  HorizontalAlignment="Left" Margin="212,10,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
    </Grid>
</Window>
