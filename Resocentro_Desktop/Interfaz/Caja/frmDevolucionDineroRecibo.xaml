<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation" x:Class="Resocentro_Desktop.Interfaz.Caja.frmDevolucionDineroRecibo"
        Title="Devolucion Dinero" Height="246" Width="304" WindowStartupLocation="CenterOwner" Background="{DynamicResource Windows.Background}" WindowStyle="ToolWindow">
    <Grid>
        <StackPanel>
            <Label/>
            <Label Content="Monto"/>
            <telerik:RadNumericUpDown Name="txtmonto" Margin="186,0,0,0"/>
            <Label/>
            <Label Content="Motivo"/>
            <TextBox Name="txtmotivo" CharacterCasing="Upper" AcceptsReturn="True" MinHeight="80" />
            <Label/>
            <Button Content="Devolver" Click="Button_Click"/>
            <Label/>

        </StackPanel>
    </Grid>
</Window>
