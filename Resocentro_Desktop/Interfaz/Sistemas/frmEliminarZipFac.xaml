<Window x:Class="Resocentro_Desktop.Interfaz.Sistemas.frmEliminarZipFac"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="frmEliminarZipFac" Background="{DynamicResource Windows.Background}" Height="227" Width="239" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid>
        <StackPanel >
            <Label Content="Empresa"/>
            <ComboBox Name="cbosede" HorizontalAlignment="Right" Width="150" SelectedIndex="0"/>
            <Label Content="Tipo Documento"/>
            <ComboBox x:Name="cboTipoDocumento" HorizontalAlignment="Right" Width="150">
                <ComboBoxItem Content="01 - Factura"/>
                <ComboBoxItem Content="03 - Boleta"/>
            </ComboBox>            
            <Label Content="N° Documento"/>
            <TextBox Name="txtDocumento" Width="150" HorizontalAlignment="Right" />
            <Label />
            <Button Content="Eliminar" HorizontalAlignment="Right"  Grid.Row="6" VerticalAlignment="Top" Width="76" Height="31" Click="btnEliminar_Click"/>
        </StackPanel>

    </Grid>
</Window>
