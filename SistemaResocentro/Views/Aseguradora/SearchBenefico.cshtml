@{
    Layout = null;
}
<script src="~/js/script/aseguradora/searchbeneficio.js"></script>
<link href="~/css/progressbar.css" rel="stylesheet" />
<div class="row">
    <div class="col-md-12">

        <div class="form-horizontal ">
            <h4>Buscar Beneficio</h4>
            <hr />

            <input type="hidden" id="result_beneficio" />
            <input type="hidden" id="result_nombres" />
            <input type="hidden" id="idAseguradora" value="@ViewBag.idAseguradora" />
            <input type="hidden" id="idProducto" value="@ViewBag.idProducto" />
            <div class="col-md-8">
                <div class="form-group">
                    <label class="control-label col-md-2">Filtro:</label>
                    <div class="col-md-10">
                        <input type="text" class="form-control" id="filtro" />
                    </div>
                </div>
            </div>
            <div class="col-md-4">
                <div class="form-group">
                    <button class="btn btn-info btn-xs" onclick="getBeneficio()" type="button"><i class="fa fa-search fa-2x"></i></button>
                </div>
            </div>
            <div class="col-md-12">
                <div class="form-group" id="table_beneficio">
                </div>
            </div>

        </div>

    </div>

</div>
<div class="modal-footer">
    <button type="button" class="btn btn-default" data-dismiss="modal">Cerrar</button>
</div>