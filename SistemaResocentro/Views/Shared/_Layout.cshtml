    <script src="~/js/validar-input.js"></script>
    <script type="text/javascript">
        var opts = {
            "closeButton": true,
            "showDuration": "300",
            "hideDuration": "1000",
            "timeOut": "5000",
            "extendedTimeOut": "1000",
            "showEasing": "swing",
            "hideEasing": "linear",
            "showMethod": "fadeIn",
            "hideMethod": "fadeOut"
        }
        $('#modal_CambioContrasena').on('hidden.bs.modal', function (e) {
            removeHasAll("txtclave_actual");
            removeHasAll("txtclave_new");
            removeHasAll("txtclave_confirm");
            $("#txtclave_actual").val("");
            $("#txtclave_new").val("");
            $("#txtclave_confirm").val("");

        })

        $('#modal_CambioNombre').on('hidden.bs.modal', function (e) {
            location.reload();
        })

        function cambiarNombre() {
            var result = true;
            $(".has-error").each(function () {
                alert("false");
                result = false;
            });
            if (result) {
                $.ajax({
                    url: "/Home/ChangeShortName",
                    data: { name: $("#txtnombre_usersesion").val() },
                    dataType: "json",
                    type: "POST",
                    success: function (data) {
                        toastr.success('Se realizó el cambio.', 'Exito', opts);
                        $('#modal_CambioNombre').modal("hide");
                    }
                });
            }
        }

        function cambiarClave() {
            var result = true;
            $(".has-error").each(function () {
                alert("false");
                result = false;
            });
            if (result) {
                $.ajax({
                    url: "/Home/ChangeContrasena",
                    data: { clave: $("#txtclave_new").val() },
                    dataType: "json",
                    type: "POST",
                    success: function (data) {
                        toastr.success('Se realizó el cambio.', 'Exito', opts);
                        $('#modal_CambioContrasena').modal("hide");
                    }
                });
            }
        }

        function validarContrasenaActual() {
            var pass = $("#txtclave_actual").val();
            if (pass != "") {
                removeHasAll("txtclave_actual");
                $.ajax({
                    url: "/Home/ValidarContrasena",
                    data: { clave: pass },
                    dataType: "json",
                    type: "POST",
                    error: function () {
                        toastr.error('No se pudo registrar los datos', 'Error', opts);
                    },
                    success: function (data) {
                        if (data == false)
                            addError("txtclave_actual");
                        else
                            addSuccess("txtclave_actual");

                    }
                });
            }
        }
        function validarContrasenaNueva() {
            var newpass = $("#txtclave_new").val();
            var confirmpass = $("#txtclave_confirm").val();
            removeHasAll("txtclave_new");
            removeHasAll("txtclave_confirm");
            if (newpass != "" && confirmpass != "") {
                if (newpass != confirmpass) {
                    addError("txtclave_new");
                    addError("txtclave_confirm");
                } else {
                    addSuccess("txtclave_new");
                    addSuccess("txtclave_confirm");
                }
            }
        }

        function registrarIncidente() {
            var importancia = $("#cboimportancia").val();
            var aplicativo = $("#cboaplicativo").val();
            var email = $("#email_emp").val();
            var tel = $("#tel_emp").val();
            var incidente = $("#incidente_emp").val();
            if (importancia != "" && aplicativo != "" && email != "" && incidente != "") {
                $("#modal_SistemasLabel").append('<i class="fa fa-spinner fa-spin fa-3x pull-right color-green" style="color:#003471;"></i>');
                $.ajax({
                    url: "/Home/RegistrarIncidente",
                    data: { problema: incidente, importancia: importancia, aplicativo: aplicativo },
                    dataType: "json",
                    type: "POST",
                    error: function () {
                        toastr.error('No se pudo registrar los datos', 'Error', opts);
                    },
                    success: function (data) {
                        if (data == false)
                            toastr.error('No se pudo actualizar los datos', 'Error', opts);
                        else {
                            toastr.success('Se registro la solicitud Correctamente', 'Exito', opts);
                            $("#cboimportancia").val("");
                            $("#cboaplicativo").val("");
                            $("#incidente_emp").val("");
                            $("#modal_Sistemas").modal("toggle");
                        }
                    }
                });
            } else
                toastr.error('Ingrese todos los datos correctamente', 'Error', opts);
        }

        function actualizarEmpleado(tipo) {
            var valor = "";
            if (tipo == 1)
                valor = $("#email_emp").val();
            if (tipo == 2)
                valor = $("#tel_emp").val();

            $.ajax({
                url: "/Empleado/Update",
                data: { tipo: tipo, valor: valor },
                dataType: "json",
                type: "POST",
                error: function () {
                    toastr.error('No se pudo actualizar los datos', 'Error', opts);
                },
                success: function (data) {
                    if (data == false)
                        toastr.error('No se pudo actualizar los datos', 'Error', opts);
                }
            });
        }

        function RegistrarUbicacion() {
            if (navigator.geolocation) {
                navigator.geolocation.getCurrentPosition(
                    function (latitud) {
                        $.ajax({
                            url: "/Home/RegistrarUbicacion",
                            data: {
                                latitud: latitud.coords.latitude.toString(),
                                longitud: latitud.coords.longitude.toString()
                            },
                            dataType: "json",
                            type: "POST",
                            error: function () {
                                toastr.error('No se registrar ubicación', 'Error', opts);
                            },
                            success: function (data) {
                                if (data == false)
                                    toastr.error('Ubicación no registrada.', 'Error', opts);
                                else
                                    toastr.success('Ubicación registrada.', 'Exito', opts);
                            }
                        });
                    },
                    function () {
                        toastr.error('No se pudo obtener su ubicación, vuelva a intentar por favor.', 'Error', opts);
                    });
            }
        }

        $(function () {

            $("#file-1").fileinput({
                language: "es",
                uploadUrl: '@Url.Action("UploadFiles", "Account")', // you must set a valid URL here else you will get an error
                allowedFileExtensions: ['jpg', 'png'],
                overwriteInitial: false,
                maxFileSize: 1000,
                maxFilesNum: 1,
                allowedFileTypes: ['image'],
                slugCallback: function (filename) {
                    return filename.replace('(', '_').replace(']', '_');
                },
                layoutTemplates: {
                    main1: "<div class=\'input-group {class}\'>\n" +
                    "   <div class=\'input-group-btn\'>\n" +
                "       {browse}\n" +
                "       {upload}\n" +
                "       {remove}\n" +
                "   </div>\n" +
                    "   {caption}\n" +
