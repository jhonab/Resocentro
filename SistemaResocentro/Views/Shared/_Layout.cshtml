                        <input type="password" id="txtclave_actual" onchange="validarContrasenaActual()" value="" class="form-control " />
                    </div>
                    <div class="form-group">
                        <label class="control-label">Nueva Contraseña:</label>
                        <input type="password" id="txtclave_new" onchange="validarContrasenaNueva()" value="" class="form-control " />
                    </div>
                    <div class="form-group">
                        <label class="control-label">Confirmación Contraseña:</label>
                        <input type="password" id="txtclave_confirm" onchange="validarContrasenaNueva()" value="" class="form-control " />
                    </div>
                </div>

                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Cerrar</button>
                    <button type="button" class="btn btn-primary" onclick="cambiarClave()">Cambiar</button>
