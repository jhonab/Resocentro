                                        <input type="tel" id="tel_emp" placeholder="Anexo, Telefono, Celular" class="form-control" onchange="actualizarEmpleado(2);" value="@user.telefono" />
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="control-label col-md-3">Incidente:<small style="color:#ff0000">(*)</small></label>
                                    <div class="col-md-9">
                                        <textarea rows="5" maxlength="600" id="incidente_emp" placeholder="Describir Incidente" class="form-control"></textarea>
                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>
