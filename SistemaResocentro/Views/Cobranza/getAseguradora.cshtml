@model IEnumerable<SistemaResocentro.ViewModel.EmpresaSunat>
@{
    Layout = null;
}
<table class="table table-striped table-bordered table-hover">
    <thead>
        <tr>
            <th>
                RUC
            </th>
            <th>
                Razón Social
            </th>
            <th>
                Compañia
            </th>
            <th style="width:30px"></th>
        </tr>
    </thead>
    @foreach (var item in Model)
    {
        <tbody>
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.ruc)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.razonsocial)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.compania)
                </td>
                <td>
                    <button class="btn btn-info btn-xs" onclick="seleccionaEmpresa('@item.razonsocial','@item.ruc','@item.codigocompaniaseguro','@item.ubigeoId')">&nbsp;<i class="fa fa-mouse-pointer"></i></button>
                </td>
            </tr>
        </tbody>
    }

</table>
