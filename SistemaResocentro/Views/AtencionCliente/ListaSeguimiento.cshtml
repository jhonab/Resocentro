@{
    ViewBag.Title = "Seguimiento de Producción";
}


<h2>Seguimiento de Producción</h2>
<link href="~/js/datatables/datatable.css" rel="stylesheet" />
<script src="~/js/datatables/jquery.dataTables.min.js"></script>
<script src="~/js/dataTables.bootstrap.js"></script>

<script src="~/js/Datetime/moment.js"></script>
<script src="~/js/Datetime/moment-with-locales.js"></script>
<script src="~/js/Datetime/bootstrap-datetimepicker.js"></script>
<link href="~/css/Datetime/bootstrap-datetimepicker.css" rel="stylesheet" />

<link href="~/js/chosen/chosen.bootstrap.css" rel="stylesheet" />
<script src="~/js/chosen/chosen.jquery.min.js"></script>

<style>
    .table .chosen-container .chosen-results li {
        padding: 1px;
    }

    .table .chosen-container .chosen-results li {
        font-size: 10px;
    }

    .table .chosen-container-single .chosen-single {
        height: 20px;
        line-height: 20px;
    }

        .table .chosen-container-single .chosen-single div b {
            background: url(../../js/chosen/chosen-sprite.png) no-repeat 0 0px;
        }

    table tbody tr {
        cursor: pointer;
    }

    .bg-resaltar {
        background: #c5e8f7 !important;
    }

    .bg-sedacion {
        background: #EDA8A8 !important;
    }

    .bg-error {
        background: rgba(255, 0, 0, 0.3) !important;
    }

    .bg-advertencia {
