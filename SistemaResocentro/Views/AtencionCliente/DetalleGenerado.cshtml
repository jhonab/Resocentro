@model SistemaResocentro.Models.CorrelativoMotivo
@{
    ViewBag.Title = "DetalleGenerado";
}

<h2>DetalleGenerado</h2>

<div class="row">
    <div class="col-lg-12">
        <div class="well">
            

            <div class="row">
                <div class="col-lg-4">

                    <div class="form-group">
                        <label>Codigo:</label>
                        @Html.DisplayFor(model => model.idCorrelativo)
                    </div>

                    <div class="form-group">
                        <label>Motivo:</label>
                        @Html.DisplayFor(model => model.MotivoCodigo.descripcion)
                    </div>

                    <div class="form-group">
                        <label>Ultimo Cambio:</label>
                        @Html.DisplayFor(model => model.lastUpdate)
                    </div>
                    <p>
                        <a href='@Url.Action("ListaMotivos")' class="btn btn-primary"><i class="fa fa-arrow-circle-left fa-lg"></i>&nbsp;&nbsp;Regresar a la Lista</a>
                        <button id="btnregistrar" type="submit" class="btn btn-success"><i class="fa fa-check fa-lg"></i>&nbsp;&nbsp; Guardar </button>
                      
                    </p>
                </div>
            </div>
        </div>
    </div>
</div>

