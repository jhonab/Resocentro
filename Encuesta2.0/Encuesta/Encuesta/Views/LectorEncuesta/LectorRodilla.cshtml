@model Encuesta.ViewModels.Encuesta.Tipos.EncuestaRodillaViewModel

@{
    ViewBag.Titlenot = "Encuesta Rodilla";
    Encuesta.Util.Variable util = new Encuesta.Util.Variable();
}
<script src="~/js/encuestas/LectorEncuesta.js"></script>
<script src="~/js/encuestas/encuestas_ini.js"></script>
<link href="~/css/Encuesta.css" rel="stylesheet" />
<script src="../js/jquery.autosize.js"></script>
<script src="~/js/jquery-ui.min.js"></script>
<div class="row body-medico" id="div_PreEntrevista">
    <div class="col-lg-12">
        <div class="box box-primary">
            <div class="box-header">
                @{string tipo_encuesta = "";
                if (Model.tipo_encu == 5)
                { tipo_encuesta = "rodilla derecha"; }
                else if (Model.tipo_encu == 4)
                { tipo_encuesta = "rodilla izquierda"; }
                else
                { tipo_encuesta = ""; } }
                <h1 class="box-title titulo_ocultar">
                    ENCUESTA @tipo_encuesta.ToUpper()

                </h1>
            </div>
            <div class="box-body chart-responsive">
                @*Datos Examen*@
                <div class="row">

                    <div class="col-lg-12">

                        <div id="Info-examen" class="panel panel-medico">
                            <div class="panel-heading">
                                <h3 class="panel-title">Datos Examen</h3>
                                <div class="panel-options pull-right">
                                    <a href="#" data-rel="collapse"><i class="entypo-down-open"></i></a>
                                </div>
                            </div>
                            <div class="panel-body">
                                <div class="row">
                                    <div class="col-lg-12">

                                        <div class="col-sm-5 col-lg-5">
                                            @Html.Partial("_DatosExamenLector", Model._examen)
                                        </div>
                                        <div class="col-sm-5 col-lg-5">
                                            @Html.Partial("_DatosEncuestaLector", Model._encuesta)
                                        </div>
                                        <div class="col-sm-2 col-lg-2">
                                            <span>Adicional :</span>
                                            <div class="btn-group-vertical" role="group" style="margin-bottom: 10px;">

                                                @if (Model._examen.ATENCION.CITA.sedacion)
                                                {
                                                    <button class="btn btn-danger box-tools  pull-right no-print">SEDACION</button>
                                                }
                                                @if (Model._examen.ATENCION.CITA.claustrofobico)
                                                {
                                                    <button class="btn btn-info box-tools  pull-right no-print">CLAUSTROFOBICO</button>
                                                }

                                            </div>
                                            <div class="btn-group-vertical" role="group" id="div_adicional">
                                                <button class="btn btn-medico box-tools btn-sm pull-right no-print" id="btn-detalle" onclick="Mostrardetalle(false)">
                                                    <i class="fa fa-plus fa-1x"></i>&nbsp;Detalle Encuesta
                                                </button>
                                                <button class="btn btn-medico box-tools btn-sm pull-right no-print" onclick="verHistorial('@Model._examen.codigo')">
                                                    <i class="fa fa-history fa-1x"></i>&nbsp;Estudios Previos
                                                </button>

                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>


                </div>
                <div class="row" id="div_historial">
                </div>
                <div id="div-resumen">
                    <div class="pregunta">
                        @*Pregunta 1*@
                        @if (Model.p1.Value)
                        {
                            <div class="col-lg-12">
                                - <span class="resaltar">Tiene dolores en la @tipo_encuesta</span> cuando :

                                @if (Model.p1_1)
                                {
                                    <div class="col-lg-12">
                                        <span>-<span class="resaltar"> Tiene actividad física o termino de ella</span>.</span>
                                    </div>
                                }
                                @if (Model.p1_2)
                                {
                                    <div class="col-lg-12">
                                        <span>-<span class="resaltar"> Está en reposo</span>.</span>
                                    </div>
                                }
                                @if (Model.p1_3)
                                {
                                    <div class="col-lg-12">
                                        <span>-<span class="resaltar"> Sube y baja las escaleras</span>.</span>
                                    </div>
                                }
                                <div class="col-lg-12">
                                    ubicado en:
                                </div>
                                @if (Model.p1_4)
                                {
                                    <div class="col-lg-12">
                                        <span>-<span class="resaltar"> Anterior</span>.</span>
                                    </div>
                                }
                                @if (Model.p1_5)
                                {
                                    <div class="col-lg-12">
                                        <span>-<span class="resaltar"> Posterior</span>.</span>
                                    </div>
                                }
                                @if (Model.p1_6)
                                {
                                    <div class="col-lg-12">
                                        <span>-<span class="resaltar"> Medial</span>.</span>
                                    </div>
                                }
                                @if (Model.p1_7)
                                {
                                    <div class="col-lg-12">
                                        <span>-<span class="resaltar"> Lateral</span>.</span>
                                    </div>
                                }
                                @if (Model.p1_8)
                                {
                                    <div class="col-lg-12">
                                        <span>-<span class="resaltar"> @Model.p1_81</span>.</span>
                                    </div>
                                }
                                @if (Model.p1_9)
                                {
                                    <div class="col-lg-12">
                                        <span>-<span class="resaltar"> No precisa</span>.</span>
                                    </div>
                                }

                            </div>
                        }
                        @*Pregunta 2*@
                        @if (Model.p2.Value)
                        {
                            <div class="col-lg-12">
                                - Tiene sensacion de "bulto" o cuerpo extrano ubicado en
                                @if (Model.p2_4)
                                {
                                    <span class="resaltar"> Anterior</span>
                                }
                                @if (Model.p2_5)
                                {
                                    if (Model.p2_4)
                                    {
                                        <span>, </span>
                                    }
                                    <span class="resaltar"> Posterior</span>
                                }
                                @if (Model.p2_6)
                                {
                                    if (Model.p2_4 || Model.p2_5)
                                    {
                                        <span>, </span>
                                    }
                                    <span class="resaltar"> Medial</span>
                                }
                                @if (Model.p2_7)
                                {
                                    if (Model.p2_4 || Model.p2_5 || Model.p2_6)
                                    {
                                        <span>, </span>
                                    }
                                    <span class="resaltar"> Lateral</span>
                                }
                                @if (Model.p2_8)
                                {
                                    if (Model.p2_4 || Model.p2_5 || Model.p2_6 || Model.p2_7)
                                    {
                                        <span>, </span>
                                    }
                                    <span class="resaltar"> @Model.p2_81</span>
                                }
                                @if (Model.p2_9)
                                {
                                    if (Model.p2_4 || Model.p2_5 || Model.p2_6 || Model.p2_7 || Model.p2_8)
                                    {
                                        <span>, </span>
                                    }
                                    <span class="resaltar"> No precisa</span>
                                }

                            </div>
                        }
                        @*Pregunta 3*@
                        @if (Model.p3.Value)
                        {
                            <div class="col-lg-12">
                                - <span class="resaltar">Tiene sensación de inestabilidad cuando corre o camina</span>.
                            </div>
                        }
                        @*Pregunta 4*@
                        @if (Model.p4.Value)
                        {
                            <div class="col-lg-12">
                                - <span class="resaltar">Tiene sensación que la rodilla se traba</span>.
                            </div>
                        }
                        @*Pregunta 5*@
                        @if (Model.p5.Value)
                        {
                            <div class="col-lg-12">
                                - <span class="resaltar">Tiene sensación de crujido dentro de la rodilla</span>.
                            </div>
                        }

                        @*PREGUNTA 6*@
                        @if (Model.p6 != null)
                        {
                            if (Model.p6 != "")
                            {
                                <div class="col-lg-12">
                                    - Tiene antecedentes médicos:
                                    <span class="resaltar">@Model.p6</span>.
                                </div>
                            }
                        }

                        @*PREGUNTA 7*@
                        <div class="col-lg-12">
                            @if (Model.p7_2 != null)
                            {
                                if (Model.p7_2.Value)
                                {
                                    <text>- <span class="resaltar">No precisa cuando fue la primera vez</span>.</text>
                                }
                                else
                                {

                                    <text>- Lo noto </text><span class="resaltar">@Model.p7_22.</span>
                                }
                            }
                            else
                            {
                                <text>- Lo noto hace <span class="resaltar">@Model.p7_1 @util.getNombreTiempoRM(Model.p7_11)</span>.</text>
                            }
                        </div>
                        @*PREGUNTA 8*@
                        <div class="col-lg-12">
                            - Las molestias iniciaron
                            <span class="resaltar">
                                @if (Model.p8 == "1")
                                {

                                    <text>por esfuerzo,</text> <span>@Model.p8_1</span>

                                }
                                else if (Model.p8 == "2")
                                {

                                    <text>por traumatismo,</text> <span>@Model.p8_1</span>

                                }
                                else if (Model.p8 == "3")
                                {

                                    <text> sin nada en especial</text>

                                }
                                else if (Model.p8 == "4")
                                {

                                    <text>por,</text> <span>@Model.p8_1</span>

                                }
                                else { }
                            </span>.
                        </div>
                        @*PREGUNTA 9*@
                        @if (Model.p9.Value)
                        {
                            <div class="col-lg-12">
                                - <span class="resaltar">Fue operado @Model.p9_1 </span>veces de <span class="resaltar">@Model.p9_4,</span> la última hace<span class="resaltar">
                                    @Model.p9_21 @util.getNombreTiempoRM(Model.p9_22)
                                    @if (Model.p9_3 == "1")
                                    {
                                        <text>mejorando su estado</text>
                                    }
                                    else if (Model.p9_3 == "2")
                                    {
                                        <text>quedando igual</text>
                                    }
                                    else
                                    {
                                        <text>empeorando su estado</text>
                                    }
                                </span>
                                .
                            </div>
                        }
                        @*PREGUNTA 10*@
                        @if (Model.p10.Value)
                        {
                            <div class="col-lg-12">
                                - <span class="resaltar">Tiene estudios </span>previos en
                                @util.getNombreLocales(Model.p10_1)
                                hace
                                <span class="resaltar">
                                    @Model.p10_21
                                    @util.getNombreTiempoRM(Model.p10_22)
                                </span>
                                por el siguiente motivo:
                                <span class="resaltar">@Model.p10_3</span>
                                .
                            </div>
                        }
                        @*PREGUNTA 11*@
                        @if (Model.p11.Value)
                        {
                            <div class="col-lg-12">
                                - Practica<span class="resaltar">
                                    @if (Model.p11_1 == 6)
                                    {
                                        <span>@Model.p11_11</span>
                                    }
                                    else
                                    {
                                        @util.getNombreDeportes(Model.p11_1)
                                    }
                                    @util.getNombreReguralidadDeporte(Model.p11_2)
                                </span>
                                .
                            </div>
                        }

                        @*PREGUNTA 12*@
                        @if (Model.p12Acheck == false)
                        {
                            <div class="col-lg-12">- Orden médica menciona <span class="resaltar">@Model.p12A</span> .</div>
                        }
                        @if (Model.p12Bcheck == false)
                        {
                            <div class="col-lg-12">- El paciente comenta <span class="resaltar">@Model.p12B</span> .</div>
                        }
                        @*PREGUNTA 13*@
                        @if (Model.p13 != "")
                        {
                            <div class="col-lg-12">- Se desea resolver el o los siguientes problemas diagnosticados <span class="resaltar">@Model.p13</span> .</div>
                        }
                        @*PREGUNTA 14*@
                        @if (Model.p14 == "1")
                        {
                            <div class="col-lg-12">-  <span class="resaltar">Se indicó </span>que se use contraste.</div>
                        }
                        @if (Model.p14 == "2")
                        {
                            <div class="col-lg-12">- Se indicó  que <span class="resaltar">no </span>se use contraste.</div>
                        }
                        @if (Model.p14 == "3")
                        {
                            <div class="col-lg-12">- Se indicó usar contraste  <span class="resaltar">de acuerdo a hallazgos</span>.</div>
                        }
                        @*Pregunta15*@
                        @if (Model.p15)
                        {
                            <div class="col-lg-12">- <span class="resaltar">Se indicó </span> colocación de Reparo Anatómico.</div>
                        }
                        else
                        {
                            <div class="col-lg-12">- <span class="resaltar">No se indicó </span> colocación de Reparo Anatómico.</div>
                        }

                    </div>
                </div>
                <div id="div-detallado">
                    <h3></h3>
                    @*Molestias*@
                    <div class="row">

                        <div class="col-md-12">
                            <div class="">
                                <div class="">

                                    @*Molestias*@
                                    <div class="row">

                                        <div class="col-md-12">

                                            <div class="panel panel-medico" data-collapsed="1">
                                                <div class="panel-heading">
                                                    <div class="panel-title">Principales Molestias </div>
                                                    <div class="panel-options pull-right">
                                                        <a href="#" data-rel="collapse"><i class="entypo-down-open"></i></a>
                                                    </div>
                                                </div>
                                                <div class="panel-body chart-responsive">

                                                    @*Pregunta 1*@
                                                    <script type="text/javascript">
                                                        $(document).ready(function () {
                                                            //1 CHeck "En cuanto a la ubicacion del dolor" Opcion "No precisa"
                                                            $("#p1_9").change(function () {
                                                                if ($(this).is(":checked")) {
                                                                    $("#p1_4").attr("checked", false);
                                                                    $("#p1_5").attr("checked", false);
                                                                    $("#p1_6").attr("checked", false);
                                                                    $("#p1_7").attr("checked", false);
                                                                    $("#p1_8").attr("checked", false);
                                                                    $("#p1_81").hide();
                                                                }
                                                            });
                                                            $("#p1_4").change(function () {
                                                                if ($(this).is(":checked")) {
                                                                    disableCheck_precisa();
                                                                }
                                                            });
                                                            $("#p1_5").change(function () {
                                                                if ($(this).is(":checked")) {
                                                                    disableCheck_precisa();
                                                                }
                                                            });
                                                            $("#p1_6").change(function () {
                                                                if ($(this).is(":checked")) {
                                                                    disableCheck_precisa();
                                                                }
                                                            });
                                                            $("#p1_7").change(function () {
                                                                if ($(this).is(":checked")) {
                                                                    disableCheck_precisa();
                                                                }
                                                            });
                                                            $("#p1_8").change(function () {
                                                                if ($(this).is(":checked")) {
                                                                    disableCheck_precisa();
                                                                }
                                                            });

                                                            //2 CHeck "En cuanto a la ubicacion del dolor" Opcion "No precisa"
                                                            $("#p2_9").change(function () {
                                                                if ($(this).is(":checked")) {
                                                                    $("#p2_4").attr("checked", false);
                                                                    $("#p2_5").attr("checked", false);
                                                                    $("#p2_6").attr("checked", false);
                                                                    $("#p2_7").attr("checked", false);
                                                                    $("#p2_8").attr("checked", false);
                                                                    $("#p2_81").hide();
                                                                }
                                                            });
                                                            $("#p2_4").change(function () {
                                                                if ($(this).is(":checked")) {
                                                                    disableCheck_precisa_p2();
                                                                }
                                                            });
                                                            $("#p2_5").change(function () {
                                                                if ($(this).is(":checked")) {
                                                                    disableCheck_precisa_p2();
                                                                }
                                                            });
                                                            $("#p2_6").change(function () {
                                                                if ($(this).is(":checked")) {
                                                                    disableCheck_precisa_p2();
                                                                }
                                                            });
                                                            $("#p2_7").change(function () {
                                                                if ($(this).is(":checked")) {
                                                                    disableCheck_precisa_p2();
                                                                }
                                                            });
                                                            $("#p2_8").change(function () {
                                                                if ($(this).is(":checked")) {
                                                                    disableCheck_precisa_p2();
                                                                }
                                                            });

                                                        });
                                                        function disableCheck_precisa() {
                                                            $("#p1_9").attr("checked", false);
                                                        }
                                                        function disableCheck_precisa_p2() {
                                                            $("#p2_9").attr("checked", false);
                                                        }
                                                    </script>
                                                    <div class="form-group">
                                                        <div class="row">
                                                            <div class="col-md-12">
                                                                <div class="form-group">
                                                                    <div>
                                                                        <cite>1. Presenta dolor en la @tipo_encuesta?</cite><span class="asterisco">(*)</span>
                                                                        <label class="checkbox-inline">
                                                                            @Html.RadioButtonFor(model => model.p1, "true", new { @mostrar_respuesta = "rpta_p1" }) Si
                                                                        </label>
                                                                        <label class="checkbox-inline">
                                                                            @Html.RadioButtonFor(model => model.p1, "false", new { @ocultar_respuesta = "rpta_p1" }) No
                                                                        </label>


                                                                        <div class="col-md-12" id="rpta_p1">
                                                                            <div class="col-md-12" id="rpta_p1_1">
                                                                                <cite>- Cuándo?</cite>
                                                                                <div class="checkbox">
                                                                                    <label>
                                                                                        @Html.CheckBoxFor(model => model.p1_1) Dolor de mayor intensidad durante o despues de actividad física
                                                                                    </label>
                                                                                </div>
                                                                                <div class="checkbox">
                                                                                    <label>
                                                                                        @Html.CheckBoxFor(model => model.p1_2) Dolor incluso al reposo
                                                                                    </label>
                                                                                </div>
                                                                                <div class="checkbox">
                                                                                    <label>
                                                                                        @Html.CheckBoxFor(model => model.p1_3) Dolor al subir y bajar las escaleras
                                                                                    </label>
                                                                                </div>

                                                                            </div>
                                                                            <div class="col-md-12" id="rpta_p1_2">
                                                                                <cite>- En cuanto a la ubicación del dolor defina:</cite>
                                                                                <div class="checkbox">
                                                                                    <label>
                                                                                        @Html.CheckBoxFor(model => model.p1_4) Anterior
                                                                                    </label>
                                                                                </div>
                                                                                <div class="checkbox">
                                                                                    <label>
                                                                                        @Html.CheckBoxFor(model => model.p1_5) Posterior
                                                                                    </label>
                                                                                </div>
                                                                                <div class="checkbox">
                                                                                    <label>
                                                                                        @Html.CheckBoxFor(model => model.p1_6) Medial
                                                                                    </label>
                                                                                </div>
                                                                                <div class="checkbox">
                                                                                    <label>
                                                                                        @Html.CheckBoxFor(model => model.p1_7) Lateral
                                                                                    </label>
                                                                                </div>
                                                                                <div class="checkbox">
                                                                                    <label>
                                                                                        @Html.CheckBoxFor(model => model.p1_8, new { @mostrar_respuesta = "p1_81" }) Otros
                                                                                    </label>
                                                                                </div>
                                                                                @Html.TextAreaFor(model => model.p1_81, new { @placeholder = "Ingrese el dolor", @class = "form-control animate validate" })
                                                                                <div class="checkbox">
                                                                                    <label>
                                                                                        @Html.CheckBoxFor(model => model.p1_9) No precisa
                                                                                    </label>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    @*Pregunta 2*@
                                                    <div class="form-group">
                                                        <div class="row">
                                                            <div class="col-md-12">
                                                                <div class="form-group">
                                                                    <div>
                                                                        <cite>2. Sensación de "<em>bulto</em>" o cuerpo extraño en la @tipo_encuesta?</cite><span class="asterisco">(*)</span>
                                                                        <label class="checkbox-inline">
                                                                            @Html.RadioButtonFor(model => model.p2, "true", new { @mostrar_respuesta = "rpta_p2" }) Si
                                                                        </label>
                                                                        <label class="checkbox-inline">
                                                                            @Html.RadioButtonFor(model => model.p2, "false", new { @ocultar_respuesta = "rpta_p2" }) No
                                                                        </label>


                                                                        <div class="col-md-12" id="rpta_p2">
                                                                            <div class="col-md-12">
                                                                                <cite>- En cuanto a la ubicación de la sensación de "bulto" defina:</cite>
                                                                                <div class="checkbox">
                                                                                    <label>
                                                                                        @Html.CheckBoxFor(model => model.p2_4) Anterior
                                                                                    </label>
                                                                                </div>
                                                                                <div class="checkbox">
                                                                                    <label>
                                                                                        @Html.CheckBoxFor(model => model.p2_5) Posterior
                                                                                    </label>
                                                                                </div>
                                                                                <div class="checkbox">
                                                                                    <label>
                                                                                        @Html.CheckBoxFor(model => model.p2_6) Medial
                                                                                    </label>
                                                                                </div>
                                                                                <div class="checkbox">
                                                                                    <label>
                                                                                        @Html.CheckBoxFor(model => model.p2_7) Lateral
                                                                                    </label>
                                                                                </div>
                                                                                <div class="checkbox">
                                                                                    <label>
                                                                                        @Html.CheckBoxFor(model => model.p2_8, new { @mostrar_respuesta = "p2_81" }) Otros
                                                                                    </label>
                                                                                </div>
                                                                                @Html.TextAreaFor(model => model.p2_81, new { @placeholder = "Ingrese el dolor", @class = "form-control animate validate" })
                                                                                <div class="checkbox">
                                                                                    <label>
                                                                                        @Html.CheckBoxFor(model => model.p2_9) No precisa
                                                                                    </label>
                                                                                </div>

                                                                            </div>

                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    @*Pregunta 3*@
                                                    <div class="form-group">
                                                        <div class="row">
                                                            <div class="col-md-12">
                                                                <div class="form-group">
                                                                    <div>
                                                                        <cite>3. Sensación de inestabilidad de la rodilla cuando corre o camina?</cite><span class="asterisco">(*)</span>
                                                                        <label class="checkbox-inline">
                                                                            @Html.RadioButtonFor(model => model.p3, "true") Si
                                                                        </label>
                                                                        <label class="checkbox-inline">
                                                                            @Html.RadioButtonFor(model => model.p3, "false") No
                                                                        </label>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    @*Pregunta 4*@
                                                    <div class="form-group">
                                                        <div class="row">
                                                            <div class="col-md-12">
                                                                <div class="form-group">
                                                                    <div>
                                                                        <cite>4. Sensación de que la rodilla se traba?</cite><span class="asterisco">(*)</span>
                                                                        <label class="checkbox-inline">
                                                                            @Html.RadioButtonFor(model => model.p4, "true") Si
                                                                        </label>
                                                                        <label class="checkbox-inline">
                                                                            @Html.RadioButtonFor(model => model.p4, "false") No
                                                                        </label>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
