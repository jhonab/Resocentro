@model Encuesta.ViewModels.Encuesta.Tipos.EncuestaColumnaBViewModel

@{
    ViewBag.Titlenot = "Encuesta Columna";
    Encuesta.Util.Variable util = new Encuesta.Util.Variable();  
}

<script src="~/js/encuestas/LectorEncuesta.js"></script>
<script src="~/js/encuestas/encuestas_ini.js"></script>
<link href="~/css/Encuesta.css" rel="stylesheet" />
<script src="../js/jquery.autosize.js"></script>
<script src="~/js/jquery-ui.min.js"></script>
<div class="row body-medico" id="div_PreEntrevista">
    <div class="col-lg-12">
        <div class="box box-primary">
            <div class="box-header">
                @{string tipo_encuesta = "";
                if (Model.tipo_encu == 8)
                { tipo_encuesta = "columna dorsal"; }
                else if (Model.tipo_encu == 9)
                { tipo_encuesta = "columna dorso-lumbar"; }
                else if (Model.tipo_encu == 10)
                { tipo_encuesta = "columna lumbo-sacra"; }
                else
                { tipo_encuesta = ""; } }
                <h1 class="box-title titulo_ocultar">
                    ENCUESTA @tipo_encuesta.ToUpper()

                </h1>
            </div>
            <div class="box-body chart-responsive">
                @*Datos Examen*@
            <div class="row">

                <div class="col-lg-12">

                    <div id="Info-examen" class="panel panel-medico">
                        <div class="panel-heading">
                            <h3 class="panel-title">Datos Examen</h3>
                            <div class="panel-options pull-right">
                                <a href="#" data-rel="collapse"><i class="entypo-down-open"></i></a>
                            </div>
                        </div>
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-lg-12">

                                    <div class="col-sm-5 col-lg-5">
                                        @Html.Partial("_DatosExamenLector", Model._examen)
                                    </div>
                                    <div class="col-sm-5 col-lg-5">
                                        @Html.Partial("_DatosEncuestaLector", Model._encuesta)
                                    </div>
                                    <div class="col-sm-2 col-lg-2">
                                        <span>Adicional :</span>
                                        <div class="btn-group-vertical" role="group" style="margin-bottom: 10px;">

                                            @if (Model._examen.ATENCION.CITA.sedacion)
                                            {
                                                <button class="btn btn-danger box-tools  pull-right no-print">SEDACION</button>
                                            }
                                            @if (Model._examen.ATENCION.CITA.claustrofobico)
                                            {
                                                <button class="btn btn-info box-tools  pull-right no-print">CLAUSTROFOBICO</button>
                                            }

                                        </div>
                                        <div class="btn-group-vertical" role="group" id="div_adicional">
                                            <button class="btn btn-medico box-tools btn-sm pull-right no-print" id="btn-detalle" onclick="Mostrardetalle(false)">
                                                <i class="fa fa-plus fa-1x"></i>&nbsp;Detalle Encuesta
                                            </button>
                                            <button class="btn btn-medico box-tools btn-sm pull-right no-print" onclick="verHistorial('@Model._examen.codigo')">
                                                <i class="fa fa-history fa-1x"></i>&nbsp;Estudios Previos
                                            </button>

                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>


            </div>
            <div class="row" id="div_historial">
            </div>
            <div id="div-resumen">
                <div class="pregunta">
                    @*Pregunta 1*@
                    @if (Model.mol_p1.Value)
                    {
                        <div class="col-lg-12">
                            - <span class="resaltar">Tiene dolores en la @tipo_encuesta</span> ubicados en:
                            <div class="col-lg-12">
                                <span class="resaltar">
                                    @if (Model.mol_p1_7)
                                    {
                                        <text>- Escapular / Espalda alta,</text>
                                        if (Model.mol_p1_7_1 == 1)
                                        {
                                            <text>derecha</text>

                                        }
                                        else if (Model.mol_p1_7_1 == 2)
                                        {
                                            <text>izquierda</text>

                                        }
                                        else if (Model.mol_p1_7_1 == 3)
                                        {
                                            <text>bilateral</text>

                                        }
                                        else { }

                                    }
                                </span>
                                @if (Model.mol_p1_1)
                                {
                                    <text>-<span class="resaltar"> Lumbar / Dorsal</span>.</text>
                                }
                                @if (Model.mol_p1_2)
                                {
                                    <text>- <span class="resaltar">Entre los glúteos</span>.</text>
                                }
                                <div class="col-lg-12">
                                    <div class="col-lg-6">
                                        @if (Model.mol_p1_3 || Model.mol_p1_5)
                                        {
                                            <div class="col-sm-6 col-lg-6">
                                                Lado Derecho
                                                <span class="resaltar">
                                                    @if (Model.mol_p1_3)
                                                    {
                                                        <div>
                                                            - Cadera
                                                        </div>

                                                    }
                                                    @if (Model.mol_p1_5)
                                                    {
                                                        <div>
                                                            - MMII
                                                        </div>

                                                    }
                                                </span>
                                            </div>
                                        }
                                        @if (Model.mol_p1_4 || Model.mol_p1_6)
                                        {
                                            <div class="col-sm-6 col-lg-6">
                                                Lado Izquierdo
                                                <span class="resaltar">
                                                    @if (Model.mol_p1_4)
                                                    {
                                                        <div>
                                                            - Cadera
                                                        </div>

                                                    }
                                                    @if (Model.mol_p1_6)
                                                    {
                                                        <div>
                                                            - MMII
                                                        </div>

                                                    }
                                                </span>
                                            </div>
                                        }
                                    </div>
                                </div>
                                @if (Model.mol_p1_8)
                                {
                                    <text> - <span class="resaltar">@Model.mol_p1_8_1</span> </text>
                                }
                            </div>
                        </div>
                    }
                    @*Pregunta 2*@
                    @if (Model.mol_p2.Value)
                    {
                        <div class="col-lg-12">
                            - Tiene las siguientes molestias:
                            <div class="col-lg-12">
                                <div class="col-lg-12">
                                    @if (Model.mol_p2_2 || Model.mol_p2_4)
                                    {
                                        <div class="col-sm-6 col-lg-6">
                                            Lado Derecho
                                            <span class="resaltar">
                                                @if (Model.mol_p2_2)
                                                {
                                                    <div>
                                                        - Paresia o plejia en MMII
                                                    </div>

                                                }
                                                @if (Model.mol_p2_4)
                                                {
                                                    <div>
                                                        - Hipoestesia, anestesia o parestesia en el MMII
                                                    </div>

                                                }
                                            </span>
                                        </div>
                                    }
                                    @if (Model.mol_p2_1 || Model.mol_p2_3)
                                    {
                                        <div class="col-sm-6 col-lg-6">
                                            Lado Izquierdo
                                            <span class="resaltar">
                                                @if (Model.mol_p2_1)
                                                {
                                                    <div>
                                                        - Paresia o plejia en MMI
                                                    </div>

                                                }
                                                @if (Model.mol_p2_3)
                                                {
                                                    <div>
                                                        - Hipoestesia, anestesia o parestesia en el MMII
                                                    </div>

                                                }
                                            </span>

                                        </div>
                                    }
                                </div>
                                @if (Model.mol_p2_5)
                                {
                                    <div class="col-lg-12">
                                        - Tiene sensación de anestesia en:<span class="resaltar">@Model.mol_p2_5_1</span>.
                                    </div>
                                }
                                @if (Model.mol_p2_6)
                                {
                                    <div class="col-lg-12">
                                        - <span class="resaltar">@Model.mol_p2_5_1</span>.
                                    </div>
                                }
                            </div>
                        </div>
                    }
                    @*Pregunta 3*@
                    @if (Model.mol_p3_1)
                    {
                        <div class="col-lg-12">
                            - <span class="resaltar">Tiene dificultad para orinar o necesidad de sonda vesical para orinar</span>.
                        </div>
                    }
                    @if (Model.mol_p3_2)
                    {
                        <div class="col-lg-12">
                            - <span class="resaltar">@Model.mol_p3_2_1</span>.
                        </div>
                    }
                    @*PREGUNTA 4*@
                    <div class="col-lg-12">
                        - Lo noto hace <span class="resaltar">@Model.mol_p4_1 @util.getNombreTiempoRM(Model.mol_p4_2)</span>.
                    </div>
                    @*PREGUNTA 5*@
                    <div class="col-lg-12">
                        -
                        @if (Model.mol_p5.Value)
                        {
                            <span class="resaltar">Comenzó súbitamente</span><span>(de un momento a otro).</span>
                        }
                        else
                        {
                            <span class="resaltar">Comenzó gradualmente</span><span>(poco a poco).</span>
                        }
                    </div>
                    @*PREGUNTA 6*@
                    @if (Model.mol_p7 != null)
                    {
                        if (Model.mol_p7 != "")
                        {
                            <div class="col-lg-12">
                                - Tiene antecedentes traumatológicos:
                                <span class="resaltar">@Model.mol_p7.</span>
                            </div>
                        }
                    }
                    @*PREGUNTA 7*@
                    @if (Model.mol_p8 != null)
                    {
                        if (Model.mol_p8 != "")
                        {
                            <div class="col-lg-12">
                                - Tiene antecedentes médicos:
                                <span class="resaltar">@Model.mol_p8.</span>
                            </div>
                        }
                    }
                    @*PREGUNTA 8*@
                    @if (Model.pro_p1.Value)
                    {
                        <div class="col-lg-12">
                            - <span class="resaltar">Fue operado @Model.pro_p1_1</span> veces de <span class="resaltar">@Model.pro_p1_4,</span> la última hace <span class="resaltar">
                                @Model.pro_p1_21 @util.getNombreTiempoRM(Model.pro_p1_22)
                                @if (Model.pro_p1_3 == "1")
                                {
                                    <text>mejorando su estado</text>
                                }
                                else if (Model.pro_p1_3 == "2")
                                {
                                    <text>quedando igual</text>
                                }
                                else
                                {
                                    <text>empeorando su estado</text>
                                }
                            </span>
                            .
                        </div>
                    }
                    @*PREGUNTA 9*@
                    @if (Model.pro_p2.Value)
                    {
                        <div class="col-lg-12">
                            - <span class="resaltar">Tiene estudios</span> previos en
                            @util.getNombreLocales(Model.pro_p2_1)
                            hace
                            <span class="resaltar">
                                @Model.pro_p2_21
                            @util.getNombreTiempoRM(Model.pro_p2_22)
                        </span>
                        por el siguiente motivo:
                        <span class="resaltar">
                            @Model.pro_p2_3
                        @if (Model.pro_p2_4_1)
                        {
                            <text>,tiene RX</text>
                        }
                        @if (Model.pro_p2_4_2)
                        {
                            <text>,tiene TEM</text>
                        }
                    </span>
                    .
                </div>
                    }

                    @*PREGUNTA 10*@
                    @if (Model.pre_p1Acheck == false)
                    {
                        <div class="col-lg-12">- Orden médica menciona <span class="resaltar">@Model.pre_p1A</span> .</div>
                    }
                    @if (Model.pre_p1Bcheck == false)
                    {
                        <div class="col-lg-12">- El paciente comenta <span class="resaltar">@Model.pre_p1B</span> .</div>
                    }
                    @*PREGUNTA 11*@
                    @if (Model.pre_p2 != "")
                    {
                        <div class="col-lg-12">- Se desea resolver el o los siguientes problemas diagnosticados <span class="resaltar">@Model.pre_p2</span> .</div>
                    }
                    @*PREGUNTA 12*@
                    @if (Model.ind_p1 == "1")
                    {
                        <div class="col-lg-12">-  <span class="resaltar">Se indicó </span>que se use contraste.</div>
                    }
                    @if (Model.ind_p1 == "2")
                    {
                        <div class="col-lg-12">- Se indicó  que <span class="resaltar">no </span>se use contraste.</div>
                    }
                    @if (Model.ind_p1 == "3")
                    {
                        <div class="col-lg-12">- Se indicó usar contraste  <span class="resaltar">de acuerdo a hallazgos</span>.</div>
                    }


                </div>
            </div>
            <div id="div-detallado">
                <h3></h3>

                @*Molestias*@
                <div class="row">

                    <div class="col-md-12">
                        <div class="">

                            <div class="">

                                @*Molestias*@
                                <div class="row">

                                    <div class="col-md-12">

                                        <div class="panel panel-medico" data-collapsed="1">
                                            <div class="panel-heading">
                                                <div class="panel-title">Principales Molestias </div>
                                                <div class="panel-options pull-right">
                                                    <a href="#" data-rel="collapse"><i class="entypo-down-open"></i></a>
                                                </div>
                                            </div>
                                            <div class="panel-body chart-responsive">

                                                @*Pregunta 1*@
                                                <div class="form-group">
                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <div class="form-group">

                                                                <cite>1. Presenta dolor en la @tipo_encuesta?</cite><span class="asterisco">(*)</span>
                                                                <label class="checkbox-inline">
                                                                    @Html.RadioButtonFor(model => model.mol_p1, "true", new { @mostrar_respuesta = "rpta_p1" }) Si
                                                                </label>
                                                                <label class="checkbox-inline">
                                                                    @Html.RadioButtonFor(model => model.mol_p1, "false", new { @ocultar_respuesta = "rpta_p1" }) No
                                                                </label>

                                                                <div class="col-md-12" id="rpta_p1">
                                                                    <div class="row">
                                                                        <div class="col-md-12">
                                                                            <cite>Donde?</cite>
                                                                            <div class="col-md-12">
                                                                                <div class="checkbox">
                                                                                    <label>
                                                                                        @Html.CheckBoxFor(model => model.mol_p1_7, new { @mostrar_respuesta = "div_p1_7" }) Escapular / Espalda Alta
                                                                                    </label>
                                                                                </div>
                                                                                <div class="row">
                                                                                    <div class="col-md-12" id="div_p1_7">
                                                                                        <div class="radio">
                                                                                            <label>
                                                                                                @Html.RadioButtonFor(model => model.mol_p1_7_1, "1") Derecha
                                                                                            </label>
                                                                                        </div>
                                                                                        <div class="radio">
                                                                                            <label>
                                                                                                @Html.RadioButtonFor(model => model.mol_p1_7_1, "2") Izquierda
                                                                                            </label>
                                                                                        </div>
                                                                                        <div class="radio">
                                                                                            <label>
                                                                                                @Html.RadioButtonFor(model => model.mol_p1_7_1, "3") Bilateral
                                                                                            </label>
                                                                                        </div>
                                                                                    </div>
                                                                                </div>
                                                                                <div class="checkbox">
                                                                                    <label>
                                                                                        @Html.CheckBoxFor(model => model.mol_p1_1) Lumbar / Dorsal
                                                                                    </label>
                                                                                </div>
                                                                                <div class="checkbox">
                                                                                    <label>
                                                                                        @Html.CheckBoxFor(model => model.mol_p1_2) Entre los glúteos
                                                                                    </label>
                                                                                </div>
                                                                            </div>
                                                                            <div class="row">
                                                                                <div class="col-md-12">
                                                                                    <div class="col-sm-6 col-lg-6">
                                                                                        Derecha
                                                                                        <div class="checkbox">
                                                                                            <label>
                                                                                                @Html.CheckBoxFor(model => model.mol_p1_3) Cadera derecha
                                                                                            </label>
                                                                                        </div>
                                                                                        <div class="checkbox">
                                                                                            <label>
                                                                                                @Html.CheckBoxFor(model => model.mol_p1_5) Miembro inferior derecho
                                                                                            </label>
                                                                                        </div>
                                                                                    </div>
                                                                                    <div class="col-sm-6 col-lg-6">
                                                                                        Izquierda
                                                                                        <div class="checkbox">
                                                                                            <label>
                                                                                                @Html.CheckBoxFor(model => model.mol_p1_4) Cadera izquierda
                                                                                            </label>
                                                                                        </div>
                                                                                        <div class="checkbox">
                                                                                            <label>
                                                                                                @Html.CheckBoxFor(model => model.mol_p1_6) Miembro inferior izquirdo
                                                                                            </label>
                                                                                        </div>
                                                                                    </div>
                                                                                </div>
                                                                            </div>
                                                                            <div class="col-md-12">
                                                                                <div class="checkbox">
                                                                                    <label>
                                                                                        @Html.CheckBoxFor(model => model.mol_p1_8, new { @mostrar_respuesta = "mol_p1_8_1" }) Otros
                                                                                    </label>
                                                                                </div>
                                                                                @Html.TextAreaFor(model => model.mol_p1_8_1, new { @placeholder = "Especifique donde", @class = "form-control animate validate" })
                                                                            </div>

                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                @*Pregunta 2*@
                                                <div class="form-group">
                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <div class="form-group">
                                                                <div>
                                                                    <cite>2. Disminución o pérdida de fuerza u otras sensaciones?</cite><span class="asterisco">(*)</span>
                                                                    <label class="checkbox-inline">
                                                                        @Html.RadioButtonFor(model => model.mol_p2, "true", new { @mostrar_respuesta = "rpta_p2" }) Si
                                                                    </label>
                                                                    <label class="checkbox-inline">
                                                                        @Html.RadioButtonFor(model => model.mol_p2, "false", new { @ocultar_respuesta = "rpta_p2" }) No
                                                                    </label>
                                                                    <div class="col-md-12" id="rpta_p2">

                                                                        <div class="col-md-12">
                                                                            <cite>- Disminución o pérdida completa de la fuerza en miembro inferior?</cite>
                                                                            <div class="col-md-12">
                                                                                <label class="checkbox-inline">
                                                                                    @Html.CheckBoxFor(model => model.mol_p2_2) Derecha
                                                                                </label>
                                                                                <label class="checkbox-inline">
                                                                                    @Html.CheckBoxFor(model => model.mol_p2_1) Izquierda
                                                                                </label>
                                                                            </div>
                                                                        </div>

                                                                        <div class="col-md-12">
                                                                            <cite>- Sensación de adormecimiento, entumecimiento u hormigueo en el miembro inferior?</cite>
                                                                            <div class="col-md-12">
                                                                                <label class="checkbox-inline">
                                                                                    @Html.CheckBoxFor(model => model.mol_p2_4) Derecha
                                                                                </label>

                                                                                <label class="checkbox-inline">
                                                                                    @Html.CheckBoxFor(model => model.mol_p2_3) Izquierda
                                                                                </label>
                                                                            </div>
                                                                        </div>

                                                                        <div class="col-md-12">
                                                                            <div class="checkbox">
                                                                                <label>
                                                                                    @Html.CheckBoxFor(model => model.mol_p2_5, new { @mostrar_respuesta = "mol_p2_5_1" }) Sensación de anestesia en alguna parte del cuerpo
                                                                                </label>
                                                                            </div>
                                                                            @Html.TextAreaFor(model => model.mol_p2_5_1, new { @placeholder = "Especifique donde", @class = "form-control animate validate" })
                                                                            <div class="checkbox">
                                                                                <label>
                                                                                    @Html.CheckBoxFor(model => model.mol_p2_6, new { @mostrar_respuesta = "mol_p2_6_1" }) Otros
                                                                                </label>
                                                                            </div>
                                                                            @Html.TextAreaFor(model => model.mol_p2_6_1, new { @placeholder = "Especifique donde", @class = "form-control animate validate" })
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                @*Pregunta 3*@
                                                <div class="form-group">
                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <div class="form-group">
                                                                <div>
                                                                    <cite>3. Otros?</cite>
                                                                    <div class="col-md-12">
                                                                        <div class="col-md-12">
                                                                            <div class="checkbox">
                                                                                <label>
                                                                                    @Html.CheckBoxFor(model => model.mol_p3_1) Dificultada para orinar o necesidad de sonda vesical para orinar
                                                                                </label>
                                                                            </div>
                                                                            <div class="checkbox">
                                                                                <label>
                                                                                    @Html.CheckBoxFor(model => model.mol_p3_2, new { @mostrar_respuesta = "mol_p3_2_1" }) Otros
                                                                                </label>
                                                                            </div>
                                                                            @Html.TextAreaFor(model => model.mol_p3_2_1, new { @placeholder = "Especifique donde", @class = "form-control animate validate" })
                                                                        </div>

                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>


                                @*Historial Molestias*@

                                <div class="row">

                                    <div class="col-md-12">

                                        <div class="panel panel-medico" data-collapsed="1">
                                            <div class="panel-heading">
                                                <div class="panel-title">Historia de Molestias </div>
                                                <div class="panel-options pull-right">
                                                    <a href="#" data-rel="collapse"><i class="entypo-down-open"></i></a>
                                                </div>
                                            </div>
                                            <div class="panel-body chart-responsive">
                                                @*Pregunta 4*@
                                                <div class="form-group">
                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <div class="form-group">
                                                                <div>
                                                                    <cite>4. Desde hace cuánto lo ha notado por primera vez?</cite><span class="asterisco">(*)</span>
                                                                    <div class="col-md-12">
                                                                        <div class="row">
                                                                            <div class="col-sm-2 col-lg-2">
                                                                                @Html.TextBoxFor(model => model.mol_p4_1, new { @placeholder = "Cant.", @type = "number", @min = "0", @class = "form-control" })
                                                                            </div>
                                                                            <div class="col-sm-4 col-lg-4">
                                                                                @Html.DropDownListFor(model => model.mol_p4_2, (SelectList)ViewBag.tiempo, "- Seleccione -", new { @class = "form-control" })
                                                                            </div>
                                                                        </div>
                                                                    </div>

                                                                </div>

                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                @*Pregunta 5*@
                                                <div class="form-group">
                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <div class="form-group">
                                                                <div>
                                                                    <cite>5. Comó comenzaron sus molestias?</cite><span class="asterisco">(*)</span>
                                                                    <div class="col-md-12">

                                                                        <div class="radio">
                                                                            <label class="checkbox-inline">
                                                                                @Html.RadioButtonFor(model => model.mol_p5, "true") Subitamente(de un momento a otro)
                                                                            </label>

                                                                        </div>

                                                                        <div class="radio">
                                                                            <label class="checkbox-inline">
