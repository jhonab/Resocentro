@model Encuesta.ViewModels.Encuesta.Tipos.EncuestaCaderaViewModel

@{
    ViewBag.Titlenot = "Encuesta Cadera";
    Encuesta.Util.Variable util = new Encuesta.Util.Variable();   
}

<script src="~/js/encuestas/LectorEncuesta.js"></script>
<script src="~/js/encuestas/encuestas_ini.js"></script>
<link href="~/css/Encuesta.css" rel="stylesheet" />
<script src="../js/jquery.autosize.js"></script>
<script src="~/js/jquery-ui.min.js"></script>
<div class="row body-medico" id="div_PreEntrevista">
    <div class="col-lg-12">
        <div class="box box-primary">
            <div class="box-header">
                @{string tipo_encuesta = "";
                if (Model.tipo_encu == 12)
                { tipo_encuesta = "cadera derecha"; }
                else if (Model.tipo_encu == 13)
                { tipo_encuesta = "cadera izquierda"; }
                else
                { tipo_encuesta = ""; } }
                <h1 class="box-title titulo_ocultar">
                    ENCUESTA @tipo_encuesta.ToUpper()

            </h1>
        </div>
        <div class="box-body chart-responsive">
            @*Datos Examen*@
        <div class="row">

            <div class="col-lg-12">

                <div id="Info-examen" class="panel panel-medico">
                    <div class="panel-heading">
                        <h3 class="panel-title">Datos Examen</h3>
                        <div class="panel-options pull-right">
                            <a href="#" data-rel="collapse"><i class="entypo-down-open"></i></a>
                        </div>
                    </div>
                    <div class="panel-body">
                        <div class="row">
                            <div class="col-lg-12">

                                <div class="col-sm-5 col-lg-5">
                                    @Html.Partial("_DatosExamenLector", Model._examen)
                                </div>
                                <div class="col-sm-5 col-lg-5">
                                    @Html.Partial("_DatosEncuestaLector", Model._encuesta)
                                </div>
                                <div class="col-sm-2 col-lg-2">
                                    <span>Adicional :</span>
                                    <div class="btn-group-vertical" role="group" style="margin-bottom: 10px;">

                                        @if (Model._examen.ATENCION.CITA.sedacion)
                                        {
                                            <button class="btn btn-danger box-tools  pull-right no-print">SEDACION</button>
                                        }
                                        @if (Model._examen.ATENCION.CITA.claustrofobico)
                                        {
                                            <button class="btn btn-info box-tools  pull-right no-print">CLAUSTROFOBICO</button>
                                        }

                                    </div>
                                    <div class="btn-group-vertical" role="group" id="div_adicional">
                                        <button class="btn btn-medico box-tools btn-sm pull-right no-print" id="btn-detalle" onclick="Mostrardetalle(false)">
                                            <i class="fa fa-plus fa-1x"></i>&nbsp;Detalle Encuesta
                                        </button>
                                        <button class="btn btn-medico box-tools btn-sm pull-right no-print" onclick="verHistorial('@Model._examen.codigo')">
                                            <i class="fa fa-history fa-1x"></i>&nbsp;Estudios Previos
                                        </button>

                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>


        </div>
        <div class="row" id="div_historial">
        </div>
        <div id="div-resumen">
            <div class="pregunta">
                @*Pregunta 1*@
                @if (Model.p1.Value)
                {
                    <div class="col-lg-12">
                        - <span class="resaltar">Tiene dolores en el @tipo_encuesta</span> ubicados en :
                        <span class="resaltar">
                            @if (Model.p1_2)
                            {
                                <text>cadera propiamente dicha</text>
                            }
                            @if (Model.p1_1)
                            {
                                if (Model.p1_2)
                                {
                                    <text>,</text>
                                }
                                <text>inguinal</text>
                            }
                            @if (Model.p1_3)
                            {
                                if (Model.p1_2 || Model.p1_1)
                                {
                                    <text>,</text>
                                }
                                <text>región glutea</text>
                            }
                            @if (Model.p1_7)
                            {
                                if (Model.p1_2 || Model.p1_1 || Model.p1_3)
                                {
                                    <text>,</text>
                                }
                                <span>@Model.p1_71</span>
                            }
                        </span>
                        .
                    </div>
                }
                @if (Model.p1_4)
                {
                    <div class="col-lg-12">
                        - <span class="resaltar">Tiene limitación funcional</span>.
                    </div>
                }
                @if (Model.p1_5)
                {
                    <div class="col-lg-12">
                        - <span class="resaltar">Tiene crujidos</span>.
                    </div>
                }
                @if (Model.p1_6)
                {
                    <div class="col-lg-12">
                        - Ademas, <span class="resaltar">@Model.p1_61</span>.
                    </div>
                }
                @*PREGUNTA 2*@
                <div class="col-lg-12">
                    @if (Model.p2_3 != null)
                    {
                        if (Model.p2_3.Value)
                        {
                            <text>- <span class="resaltar">No precisa cuando fue la primera vez</span>.</text>
                        }
                        else
                        {

                            <text>- Lo noto </text><span class="resaltar">@Model.p2_3_1.</span>
                        }
                    }
                    else
                    {
                        <text>- Lo noto hace <span class="resaltar">@Model.p2_1 @util.getNombreTiempoRM(Model.p2_2)</span>.</text>
                    }
                </div>
                @*PREGUNTA 3*@
                <div class="col-lg-12">
                    - Las molestias iniciaron
                    <span class="resaltar">
                        @if (Model.p3 == "1")
                        {

                            <text>por esfuerzo,</text> <span>@Model.p3_1</span>

                        }
                        else if (Model.p3 == "2")
                        {

                            <text>por traumatismo,</text> <span>@Model.p3_1</span>

                        }
                        else if (Model.p3 == "3")
                        {

                            <text> sin nada en especial</text>

                        }
                        else if (Model.p3 == "4")
                        {

                            <text>por,</text> <span>@Model.p3_1</span>

                        }
                        else { }
                    </span>.
                </div>
                @*PREGUNTA 4*@
                <div class="col-lg-12">
                    -
                    @if (Model.p4 == "1")
                    {
                        <span class="resaltar">Comenzó súbitamente</span><span>(de un momento a otro),</span><span class="resaltar"> @Model.p4_1.</span>
                    }
                    else
                    {
                        <span class="resaltar">Comenzó gradualmente</span><span>(poco a poco).</span>
                    }
                </div>
                @*PREGUNTA 5*@
                @if (Model.p5 != null)
                {
                    if (Model.p5 != "")
                    {
                        <div class="col-lg-12">
                            - Tiene antecedentes traumatológicos:
                            <span class="resaltar">@Model.p5.</span>
                        </div>
                    }
                }
                @*PREGUNTA 6*@
                @if (Model.p6 != null)
                {
                    if (Model.p6 != "")
                    {
                        <div class="col-lg-12">
                            - Tiene antecedentes médicos:
                            <span class="resaltar">@Model.p6</span>.
                        </div>
                    }
                }
                @*PREGUNTA 7*@
                @if (Model.p8.Value)
                {
                    <div class="col-lg-12">
                        - <span class="resaltar">Fue operado @Model.p8_1 </span>veces de <span class="resaltar">@Model.p8_4,</span> la última hace<span class="resaltar">
                            @Model.p8_21 @util.getNombreTiempoRM(Model.p8_22)
                            @if (Model.p8_3 == "1")
                            {
                                <text>mejorando su estado</text>
                            }
                            else if (Model.p8_3 == "2")
                            {
                                <text>quedando igual</text>
                            }
                            else
                            {
                                <text>empeorando su estado</text>
                            }
                        </span>
                        .
                    </div>
                }
                @*PREGUNTA 8*@
                @if (Model.p9.Value)
                {
                    <div class="col-lg-12">
                        - <span class="resaltar">Fue infiltrado @Model.p9_1 </span>veces, la última hace <span class="resaltar">@Model.p9_21 @util.getNombreTiempoRM(Model.p9_22)</span>.
                    </div>
                }
                @*PREGUNTA 9*@
                @if (Model.p10.Value)
                {
                    <div class="col-lg-12">
                        - <span class="resaltar">Tiene estudios </span>previos en
                        @util.getNombreLocales(Model.p10_1)
                        hace
                        <span class="resaltar">
                            @Model.p10_21
                        @util.getNombreTiempoRM(Model.p10_22)
                    </span>
                    por el siguiente motivo:
                    <span class="resaltar">@Model.p10_3</span>
                    .
                </div>
                }
                @*PREGUNTA 10*@
                @if (Model.p11.Value)
                {
                    <div class="col-lg-12">
                        - Practica<span class="resaltar">
                            @if (Model.p11_11 == 6)
                            {
                                <span>@Model.p11_12</span>
                            }
                            else
                            {
                                @util.getNombreDeportes(Model.p11_11)
                            }
                            @util.getNombreReguralidadDeporte(Model.p11_2)
                        </span>
                        .
                    </div>
                }
                @*PREGUNTA 11*@
                @if (Model.p12A_2 == false)
                {
                    <div class="col-lg-12">- Orden médica menciona <span class="resaltar">@Model.p12A_1</span> .</div>
                }
                @if (Model.p12B_2 == false)
                {
                    <div class="col-lg-12">- El paciente comenta <span class="resaltar">@Model.p12B_1</span> .</div>
                }
                @*PREGUNTA 12*@
                @if (Model.p13 != "")
                {
                    <div class="col-lg-12">- Se desea resolver el o los siguientes problemas diagnosticados <span class="resaltar">@Model.p13</span> .</div>
                }
                @*PREGUNTA 13*@
                @if (Model.p14 == "1")
                {
                    <div class="col-lg-12">-  <span class="resaltar">Se indicó </span>que se use contraste.</div>
                }
                @if (Model.p14 == "2")
                {
                    <div class="col-lg-12">- Se indicó  que <span class="resaltar">no </span>se use contraste.</div>
                }
                @if (Model.p14 == "3")
                {
                    <div class="col-lg-12">- Se indicó usar contraste  <span class="resaltar">de acuerdo a hallazgos</span>.</div>
                }

                @*Pregunta15*@
                @if (Model.p15)
                {
                    <div class="col-lg-12">- <span class="resaltar">Se indicó </span> colocación de Reparo Anatómico.</div>
                }
                else
                {
                    <div class="col-lg-12">- <span class="resaltar">No se indicó </span> colocación de Reparo Anatómico.</div>
                }

            </div>
        </div>
        <div id="div-detallado">
            <h3></h3>
            @*Molestias*@
            <div class="row">

                <div class="col-md-12">
                    <div class="">

                        <div class="">

                            @*Molestias*@
                            <div class="row">

                                <div class="col-md-12">

                                    <div class="panel panel-medico" data-collapsed="1">
                                        <div class="panel-heading">
                                            <div class="panel-title">Principales Molestias </div>
                                            <div class="panel-options pull-right">
                                                <a href="#" data-rel="collapse"><i class="entypo-down-open"></i></a>
                                            </div>
                                        </div>
                                        <div class="panel-body chart-responsive">

                                            @*Pregunta 1*@
                                            <div class="form-group">
                                                <div class="row">
                                                    <div class="col-lg-8">
                                                        <div class="form-group">
                                                            <div>
                                                                <cite>1. Presenta dolor en el @tipo_encuesta?</cite><span class="asterisco">(*)</span>
                                                                <label class="checkbox-inline">
                                                                    @Html.RadioButtonFor(model => model.p1, "true", new { @mostrar_respuesta = "rpta_p1" }) Si
                                                                </label>
                                                                <label class="checkbox-inline">
                                                                    @Html.RadioButtonFor(model => model.p1, "false", new { @ocultar_respuesta = "rpta_p1" }) No
                                                                </label>


                                                                <div class="col-md-12" id="rpta_p1">
                                                                    <div class="col-md-12">
                                                                        <cite>Donde?</cite>
                                                                        <div class="checkbox">
                                                                            <label>
                                                                                @Html.CheckBoxFor(model => model.p1_2) Cadera propiamente dicha
                                                                            </label>
                                                                        </div>
                                                                        <div class="checkbox">
                                                                            <label>
                                                                                @Html.CheckBoxFor(model => model.p1_1) Inguinal
                                                                            </label>
                                                                        </div>
                                                                        <div class="checkbox">
                                                                            <label>
                                                                                @Html.CheckBoxFor(model => model.p1_3) Region Glutea
                                                                            </label>
                                                                        </div>
                                                                        <div class="checkbox">
                                                                            <label>
                                                                                @Html.CheckBoxFor(model => model.p1_7, new { @mostrar_respuesta = "p1_71" }) Otros
                                                                            </label>
                                                                        </div>
                                                                        @Html.TextAreaFor(model => model.p1_71, new { @placeholder = "Ingrese el dolor", @class = "form-control animate validate" })
                                                                    </div>

                                                                </div>

                                                                <div class="col-md-12" id="rpta_p1_1">
                                                                    <div class="checkbox">
                                                                        <label>
                                                                            @Html.CheckBoxFor(model => model.p1_4) Limitación funcional
                                                                        </label>
                                                                    </div>
                                                                    <div class="checkbox">
                                                                        <label>
                                                                            @Html.CheckBoxFor(model => model.p1_5) Crujido
                                                                        </label>
                                                                    </div>
                                                                    <div class="checkbox">
                                                                        <label>
                                                                            @Html.CheckBoxFor(model => model.p1_6, new { @mostrar_respuesta = "p1_61" }) Otros
                                                                        </label>
                                                                    </div>
                                                                    @Html.TextAreaFor(model => model.p1_61, new { @placeholder = "Ingrese el dolor", @class = "form-control animate validate" })
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>


                            @*Historial Molestias*@

                            <div class="row">

                                <div class="col-md-12">

                                    <div class="panel panel-medico" data-collapsed="1">
                                        <div class="panel-heading">
                                            <div class="panel-title">Historia de Molestias </div>
                                            <div class="panel-options pull-right">
                                                <a href="#" data-rel="collapse"><i class="entypo-down-open"></i></a>
                                            </div>
                                        </div>
                                        <div class="panel-body chart-responsive">
                                            <script type="text/javascript">
                                                $(document).ready(function () {
                                                    $("#p2_1").change(function () { $("input:radio[name='p2_3']").removeAttr('checked'); $("#p2_3_1").hide(); });
                                                    $("#p2_2").change(function () { $("input:radio[name='p2_3']").removeAttr('checked'); $("#p2_3_1").hide(); });
                                                    $("input:radio[name='p2_3']").click(function () { if ($("input:radio[name='p2_3']").is(':checked')) { $('#p2_1').val(''); $("#p2_2").val(''); } });


                                                });
                                            </script>
                                            @*Pregunta 2*@
                                            <div class="form-group">
                                                <div class="row">
                                                    <div class="col-lg-8">
                                                        <div class="form-group">
                                                            <div>
                                                                <cite>2. Desde hace cuánto lo ha notado por primera vez?</cite><span class="asterisco">(*)</span>
                                                                <div class="col-md-12">
                                                                    <div class="row">
                                                                        <div class="col-sm-2 col-lg-2">
                                                                            @Html.TextBoxFor(model => model.p2_1, new { @placeholder = "Cant.", @type = "number", @min = "0", @class = "form-control" })
                                                                        </div>
                                                                        <div class="col-sm-4 col-lg-4">
                                                                            @Html.DropDownListFor(model => model.p2_2, (SelectList)ViewBag.tiempo, "- Seleccione -", new { @class = "form-control" })
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                <div class="col-md-12">
                                                                    <div class="radio">
                                                                        <label>
                                                                            @Html.RadioButtonFor(model => model.p2_3, "true", new { @ocultar_respuesta = "p2_3_1" }) No Precisa
                                                                        </label>
                                                                    </div>
                                                                    <div class="radio">
                                                                        <label>
                                                                            @Html.RadioButtonFor(model => model.p2_3, "false", new { @mostrar_respuesta = "p2_3_1" }) Otro
                                                                        </label>
                                                                    </div>
                                                                    @Html.TextAreaFor(model => model.p2_3_1, new { @placeholder = "Especifique su respuesta", @class = "form-control animate" })
                                                                </div>
                                                            </div>

                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            @*Pregunta 3*@
                                            <div class="form-group">
                                                <div class="row">
                                                    <div class="col-lg-8">
                                                        <div class="form-group">
                                                            <div>
                                                                <cite>3. Su inicio se asocio a:</cite><span class="asterisco">(*)</span>
                                                                <div class="col-md-12">

                                                                    <div class="radio">
                                                                        <label class="checkbox-inline">
                                                                            @Html.RadioButtonFor(model => model.p3, "1", new { @mostrar_respuesta = "p3_1" }) Esfuerzo
                                                                        </label>
                                                                    </div>

                                                                    <div class="radio">
                                                                        <label class="checkbox-inline">
                                                                            @Html.RadioButtonFor(model => model.p3, "2", new { @mostrar_respuesta = "p3_1" }) Traumatismo
                                                                        </label>
                                                                    </div>
                                                                    <div class="radio">
                                                                        <label class="checkbox-inline">
                                                                            @Html.RadioButtonFor(model => model.p3, "3", new { @ocultar_respuesta = "p3_1" }) A nada en especial
                                                                        </label>
                                                                    </div>

                                                                    <div class="radio">
                                                                        <label class="checkbox-inline">
                                                                            @Html.RadioButtonFor(model => model.p3, "4", new { @mostrar_respuesta = "p3_1" }) Otros
                                                                        </label>
                                                                    </div>
                                                                    @Html.TextAreaFor(model => model.p3_1, new { @placeholder = "Especifique", @class = "form-control animate validate" })
                                                                </div>


                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            @*Pregunta 4*@
                                            <div class="form-group">
                                                <div class="row">
                                                    <div class="col-lg-8">
                                                        <div class="form-group">
                                                            <div>
                                                                <cite>4. Comó comenzaron sus molestias?</cite><span class="asterisco">(*)</span>
                                                                <div class="col-md-12">

                                                                    <div class="radio">
                                                                        <label class="checkbox-inline">
                                                                            @Html.RadioButtonFor(model => model.p4, "1", new { @mostrar_respuesta = "p4_1" }) Subitamente(de un momento a otro)
                                                                        </label>
                                                                        @Html.TextAreaFor(model => model.p4_1, new { @placeholder = "Especifique", @class = "form-control animate validate" })
                                                                    </div>

                                                                    <div class="radio">
                                                                        <label class="checkbox-inline">
                                                                            @Html.RadioButtonFor(model => model.p4, "2", new { @ocultar_respuesta = "p4_1" })Gradualmente(poco a poco)
                                                                        </label>
                                                                    </div>
                                                                </div>


                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            @*Pregunta 5*@
                                            <div class="form-group">
                                                <div class="row">
                                                    <div class="col-lg-8">
                                                        <div class="form-group">
                                                            <cite>5.  Antecedentes traumatico asociado a la @tipo_encuesta ?</cite>
                                                            <div class="col-md-12">
                                                                @Html.TextAreaFor(model => model.p5, new { @placeholder = "Especifique", @class = "form-control animate validate" })

                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            @*Pregunta 6*@
                                            <div class="form-group">
                                                <div class="row">
                                                    <div class="col-lg-8">
                                                        <div class="form-group">
                                                            <div>
                                                                <cite>6.Otros antecedentes médicos de importancia?</cite>
                                                                <div class="col-md-12">
                                                                    @Html.TextAreaFor(model => model.p6, new { @placeholder = "Especifique", @class = "form-control animate" })
                                                                </div>
                                                            </div>

                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>


                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            @*Procedimientos Relacionados*@
            <div class="row">

                <div class="col-md-12">
                    <div class="">

                        <div class="">


                            <div class="row">

                                <div class="col-md-12">

                                    <div class="panel panel-medico" data-collapsed="1">
                                        <div class="panel-heading">
                                            <div class="panel-title">Procedimientos Relacionados </div>
                                            <div class="panel-options pull-right">
                                                <a href="#" data-rel="collapse"><i class="entypo-down-open"></i></a>
                                            </div>
                                        </div>
                                        <div class="panel-body chart-responsive">
                                            @*Pregunta 7*@
                                            <div class="form-group">
                                                <div class="row">
                                                    <div class="col-md-12">
                                                        <div class="form-group">
                                                            <div>
                                                                <cite>7. Le han operado de la @tipo_encuesta antes?</cite><span class="asterisco">(*)</span>
                                                                <label class="checkbox-inline">
                                                                    @Html.RadioButtonFor(model => model.p8, "true", new { @mostrar_respuesta = "rpta_p7" }) Si
                                                                </label>
                                                                <label class="checkbox-inline">
                                                                    @Html.RadioButtonFor(model => model.p8, "false", new { @ocultar_respuesta = "rpta_p7" }) No
                                                                </label>
                                                                <div class="col-md-12" id="rpta_p7">
                                                                    <div class="col-md-12">
                                                                        <cite>Cuántas veces?</cite><span class="asterisco">(*)</span>
                                                                        <div class="col-md-12">
                                                                            <div class="row">
                                                                                <div class="col-sm-2 col-lg-2">
                                                                                    @Html.TextBoxFor(model => model.p8_1, new { @placeholder = "Cant.", @type = "number", @min = "0", @class = "form-control" })
                                                                                </div>
                                                                            </div>
                                                                        </div>

                                                                    </div>
                                                                    <div class="col-md-12">
                                                                        <cite>La última aprox. hace cuanto?</cite><span class="asterisco">(*)</span>
                                                                        <div class="col-md-12">
                                                                            <div class="row">
                                                                                <div class="col-sm-2 col-lg-2">
                                                                                    @Html.TextBoxFor(model => model.p8_21, new { @placeholder = "Cant.", @type = "number", @min = "0", @class = "form-control" })
                                                                                </div>
                                                                                <div class="col-sm-4 col-lg-4">
                                                                                    @Html.DropDownListFor(model => model.p8_22, (SelectList)ViewBag.tiempo, "- Seleccione -", new { @class = "form-control" })
                                                                                </div>
