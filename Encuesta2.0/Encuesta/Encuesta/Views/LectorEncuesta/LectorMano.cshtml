@model Encuesta.ViewModels.Encuesta.Tipos.EncuestaManoViewModel

@{
    ViewBag.Titlenot = "Encuesta Mano";
    Encuesta.Util.Variable util = new Encuesta.Util.Variable();   
}

<script src="~/js/encuestas/LectorEncuesta.js"></script>
<script src="~/js/encuestas/encuestas_ini.js"></script>
<link href="~/css/Encuesta.css" rel="stylesheet" />
<script src="~/js/jquery-ui.min.js"></script>
<script src="../js/jquery.autosize.js"></script>
<div class="row body-medico" id="div_PreEntrevista">
    <div class="col-lg-12">
        <div class="box box-primary">
            <div class="box-header">
                @{string tipo_encuesta = "";

                if (Model.tipo_encu == 22)
                { tipo_encuesta = "el dedo meñique derecho"; }
                else if (Model.tipo_encu == 23)
                { tipo_encuesta = "el dedo anular derecho"; }
                else if (Model.tipo_encu == 24)
                { tipo_encuesta = "el dedo medio derecho"; }
                else if (Model.tipo_encu == 25)
                { tipo_encuesta = "el dedo indice derecho"; }
                else if (Model.tipo_encu == 26)
                { tipo_encuesta = "el dedo pulgar derecho"; }
                else if (Model.tipo_encu == 27)
                { tipo_encuesta = "la mano derecha"; }
                else if (Model.tipo_encu == 28)
                { tipo_encuesta = "la muñeca derecha"; }
                else if (Model.tipo_encu == 29)
                { tipo_encuesta = "el dedo meñique izquierdo"; }
                else if (Model.tipo_encu == 30)
                { tipo_encuesta = "el dedo anular izquierdo"; }
                else if (Model.tipo_encu == 31)
                { tipo_encuesta = "el dedo medio izquierdo"; }
                else if (Model.tipo_encu == 32)
                { tipo_encuesta = "el dedo indice izquierdo"; }
                else if (Model.tipo_encu == 33)
                { tipo_encuesta = "el dedo pulgar izquierdo"; }
                else if (Model.tipo_encu == 34)
                { tipo_encuesta = "la mano izquierda"; }
                else if (Model.tipo_encu == 35)
                { tipo_encuesta = "la muñeca izquierda"; }

                else
                { tipo_encuesta = ""; } }
                <h1 class="box-title titulo_ocultar">
                    ENCUESTA @tipo_encuesta.ToUpper().Substring(3)

                </h1>
            </div>
            <div class="box-body chart-responsive">
                @*Datos Examen*@
            <div class="row">

                <div class="col-lg-12">

                    <div id="Info-examen" class="panel panel-medico">
                        <div class="panel-heading">
                            <h3 class="panel-title">Datos Examen</h3>
                            <div class="panel-options pull-right">
                                <a href="#" data-rel="collapse"><i class="entypo-down-open"></i></a>
                            </div>
                        </div>
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-lg-12">

                                    <div class="col-sm-5 col-lg-5">
                                        @Html.Partial("_DatosExamenLector", Model._examen)
                                    </div>
                                    <div class="col-sm-5 col-lg-5">
                                        @Html.Partial("_DatosEncuestaLector", Model._encuesta)
                                    </div>
                                    <div class="col-sm-2 col-lg-2">
                                        <span>Adicional :</span>
                                        <div class="btn-group-vertical" role="group" style="margin-bottom: 10px;">

                                            @if (Model._examen.ATENCION.CITA.sedacion)
                                            {
                                                <button class="btn btn-danger box-tools  pull-right no-print">SEDACION</button>
                                            }
                                            @if (Model._examen.ATENCION.CITA.claustrofobico)
                                            {
                                                <button class="btn btn-info box-tools  pull-right no-print">CLAUSTROFOBICO</button>
                                            }

                                        </div>
                                        <div class="btn-group-vertical" role="group" id="div_adicional">
                                            <button class="btn btn-medico box-tools btn-sm pull-right no-print" id="btn-detalle" onclick="Mostrardetalle(false)">
                                                <i class="fa fa-plus fa-1x"></i>&nbsp;Detalle Encuesta
                                            </button>
                                            <button class="btn btn-medico box-tools btn-sm pull-right no-print" onclick="verHistorial('@Model._examen.codigo')">
                                                <i class="fa fa-history fa-1x"></i>&nbsp;Estudios Previos
                                            </button>

                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>


            </div>
            <div class="row" id="div_historial">
            </div>
            <div id="div-resumen">
                <div class="pregunta">
                    @*Pregunta 1*@
                    @if (Model.p1.Value)
                    {
                        <div class="col-lg-12">
                            - <span class="resaltar">Tiene dolor en @tipo_encuesta </span>ubicado en :
                            <span class="resaltar">
                                @if (Model.p1_3)
                                {
                                    <text>dorsal</text>
                                }
                                @if (Model.p1_4)
                                {
                                    if (Model.p1_3)
                                    {
                                        <text>,</text>
                                    }
                                    <text>volar</text>
                                }
                                @if (Model.p1_5)
                                {
                                    if (Model.p1_3 || Model.p1_4)
                                    {
                                        <text>,</text>
                                    }
                                    <text>cubital</text>
                                }
                                @if (Model.p1_6)
                                {
                                    if (Model.p1_3 || Model.p1_4 || Model.p1_5)
                                    {
                                        <text>,</text>
                                    }
                                    <text>Radial</text>
                                }
                                @if (Model.p1_7)
                                {
                                    if (Model.p1_3 || Model.p1_4 || Model.p1_5 || Model.p1_6)
                                    {
                                        <text>,</text>
                                    }
                                    <span>@Model.p1_7_1</span>
                                }
                            </span>
                            .
                        </div>
                    }
                    @if (Model.p1_4)
                    {
                        <div class="col-lg-12">
                            - <span class="resaltar">Tiene limitación funcional</span>:

                            <div class="col-lg-12">
                                <span class="resaltar">
                                    @if (Model.p1_11)
                                    {
                                        <text>flexión</text>
                                    }
                                    @if (Model.p1_12)
                                    {
                                        if (Model.p1_11)
                                        {
                                            <text>,</text>
                                        }
                                        <text>extensión</text>
                                    }
                                    @if (Model.p1_13)
                                    {
                                        if (Model.p1_11 || Model.p1_12)
                                        {
                                            <text>,</text>
                                        }
                                        <text>pronación</text>
                                    }
                                    @if (Model.p1_14)
                                    {
                                        if (Model.p1_11 || Model.p1_12 || Model.p1_13)
                                        {
                                            <text>,</text>
                                        }
                                        <text>supinación</text>
                                    }
                                    @if (Model.p1_15)
                                    {
                                        if (Model.p1_11 || Model.p1_12 || Model.p1_13 || Model.p1_14)
                                        {
                                            <text>,</text>
                                        }
                                        <text>rotación</text>
                                    }
                                    @if (Model.p1_16)
                                    {
                                        if (Model.p1_11 || Model.p1_12 || Model.p1_13 || Model.p1_14 || Model.p1_15)
                                        {
                                            <text>,</text>
                                        }
                                        <span>@Model.p1_16_1</span>
                                    }.
                                </span>
                            </div>
                        </div>
                    }
                    @if (Model.p1_2)
                    {
                        <div class="col-lg-12">
                            - Tiene <span>@Model.p1_21</span>.
                        </div>
                    }
                    @*Pregunta 2*@
                    @if (Model.p2.Value)
                    {
                        <div class="col-lg-12">
                            - <span class="resaltar">Tiene crujidos</span>.
                        </div>
                    }
                    @*Pregunta 3*@
                    @if (Model.p2.Value)
                    {
                        <div class="col-lg-12">
                            - Tiene aumento de volumen o sensación de bulto en: <span class="resaltar">@Model.p3_1.</span>.
                        </div>
                    }
                    @*PREGUNTA 4*@
                    <div class="col-lg-12">
                        @if (Model.p4_3 != null)
                        {
                            if (Model.p4_3.Value)
                            {
                                <text>- <span class="resaltar">No precisa cuando fue la primera vez</span>.</text>
                            }
                            else
                            {
                                <text>- Lo noto </text><span class="resaltar">@Model.p4_3_1.</span>
                            }
                        }
                        else
                        {
                            <text>- Lo noto hace <span class="resaltar">@Model.p4_1 @util.getNombreTiempoRM(Model.p4_2)</span>.</text>

                        }
                    </div>
                    @*PREGUNTA 5*@
                    <div class="col-lg-12">
                        - Las molestias iniciaron
                        <span class="resaltar">
                            @if (Model.p5 == "1")
                            {

                                <text>por esfuerzo,</text> <span>@Model.p5_1</span>

                            }
                            else if (Model.p5 == "2")
                            {

                                <text>por traumatismo,</text> <span>@Model.p5_1</span>

                            }
                            else if (Model.p5 == "3")
                            {

                                <text> sin nada en especial</text>

                            }
                            else if (Model.p5 == "4")
                            {

                                <text>por,</text> <span>@Model.p5_1</span>

                            }
                            else { }
                        </span>.
                    </div>
                    @*PREGUNTA 6*@
                    <div class="col-lg-12">
                        @if (Model.p6 == "1")
                        {

                            <span class="resaltar">Comenzó súbitamente</span><span>(de un momento a otro), @Model.p6_1.</span>

                        }
                        else
                        {
                            <span class="resaltar">Comenzó gradualmente</span><span>(poco a poco).</span>
                        }
                    </div>
                    @*PREGUNTA 7*@
                    @if (Model.p7 != null)
                    {
                        if (Model.p7 != "")
                        {
                            <div class="col-lg-12">
                                - Tiene antecedentes relacionados:
                                <span class="resaltar">@Model.p7.</span>
                            </div>
                        }
                    }
                    @*PREGUNTA 8*@
                    @if (Model.p10.Value)
                    {
                        <div class="col-lg-12">
                            - <span class="resaltar">Fue operado @Model.p10_1 </span>veces de <span class="resaltar">@Model.p10_4, </span>la última hace <span class="resaltar">
                                @Model.p10_21 @util.getNombreTiempoRM(Model.p10_22)
                                @if (Model.p10_3 == "1")
                                {
                                    <text>mejorando su estado</text>
                                }
                                else if (Model.p10_3 == "2")
                                {
                                    <text>quedando igual</text>
                                }
                                else
                                {
                                    <text>empeorando su estado</text>
                                }
                            </span>
                            .
                        </div>
                    }
                    @*PREGUNTA 9*@
                    @if (Model.p11.Value)
                    {
                        <div class="col-lg-12">
                            - <span class="resaltar">Fue infiltrado @Model.p11_1 </span>veces, la última hace <span class="resaltar">@Model.p11_21 @util.getNombreTiempoRM(Model.p11_22)</span>.
                        </div>
                    }
                    @*PREGUNTA 10*@
                    @if (Model.p12.Value)
                    {
                        <div class="col-lg-12">
                            - <span class="resaltar">Tiene estudios </span>previos en
                            @util.getNombreLocales(Model.p12_1)
                            hace
                            <span class="resaltar">
                                @Model.p12_21
                            @util.getNombreTiempoRM(Model.p12_22)
                        </span>
                        por el siguiente motivo:
                        <span class="resaltar">@Model.p12_3</span>
                        .
                    </div>
                    }
                    @*PREGUNTA 11*@
                    @if (Model.p13.Value)
                    {
                        <div class="col-lg-12">
                            - Practica<span class="resaltar">
                                @if (Model.p13_11 == 6)
                                {
                                    <span>@Model.p13_12</span>
                                }
                                else
                                {
                                    @util.getNombreDeportes(Model.p13_11)
                                }
                                @util.getNombreReguralidadDeporte(Model.p13_2)
                            </span>
                            .
                        </div>
                    }
                    @*PREGUNTA 12*@
                    @if (Model.p14A_2 == false)
                    {
                        <div class="col-lg-12">- Orden médica menciona <span class="resaltar">@Model.p14A_1</span> .</div>
                    }
                    @if (Model.p14B_2 == false)
                    {
                        <div class="col-lg-12">- El paciente comenta <span class="resaltar">@Model.p14B_1</span> .</div>
                    }
                    @*PREGUNTA 13*@
                    @if (Model.p15 != "")
                    {
                        <div class="col-lg-12">- Se desea resolver el o los siguientes problemas diagnosticados <span class="resaltar">@Model.p15</span> .</div>
                    }
                    @*PREGUNTA 14*@
                    @if (Model.p16 == "1")
                    {
                        <div class="col-lg-12">-  <span class="resaltar">Se indicó </span>que se use contraste.</div>
                    }
                    @if (Model.p16 == "2")
                    {
                        <div class="col-lg-12">- Se indicó  que <span class="resaltar">no </span>se use contraste.</div>
                    }
                    @if (Model.p16 == "3")
                    {
                        <div class="col-lg-12">- Se indicó usar contraste  <span class="resaltar">de acuerdo a hallazgos</span>.</div>
                    }


                    @*Pregunta17*@
                    @if (Model.p17)
                    {
                        <div class="col-lg-12">- <span class="resaltar">Se indicó </span> colocación de Reparo Anatómico.</div>
                    }
                    else
                    {
                        <div class="col-lg-12">- <span class="resaltar">No se indicó </span> colocación de Reparo Anatómico.</div>
                    }



                </div>
            </div>
            <div id="div-detallado">
                <h3></h3>
                @*Molestias*@
                <div class="row">

                    <div class="col-md-12">
                        <div class=" ">
                            <div class="">

                                @*Molestias*@
                                <div class="row">

                                    <div class="col-md-12">

                                        <div class="panel panel-medico" data-collapsed="1">
                                            <div class="panel-heading">
                                                <div class="panel-title">Principales Molestias </div>

                                                <div class="panel-options pull-right">
                                                    <a href="#" data-rel="collapse"><i class="entypo-down-open"></i></a>
                                                </div>

                                            </div>
                                            <div class="panel-body chart-responsive">

                                                @*Pregunta 1*@
                                                <div class="form-group">
                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <div class="form-group">
                                                                <div>
                                                                    <cite>1. Presenta dolor en @tipo_encuesta?</cite><span class="asterisco">(*)</span>
                                                                    <label class="checkbox-inline">
                                                                        @Html.RadioButtonFor(model => model.p1, "true", new { @mostrar_respuesta = "rpta_p1" }) Si
                                                                    </label>
                                                                    <label class="checkbox-inline">
                                                                        @Html.RadioButtonFor(model => model.p1, "false", new { @ocultar_respuesta = "rpta_p1" }) No
                                                                    </label>


                                                                    <div class="col-md-12" id="rpta_p1">
                                                                        <div class="col-md-12">
                                                                            <cite>Donde?</cite>
                                                                            <div class="checkbox">
                                                                                <label>
                                                                                    @Html.CheckBoxFor(model => model.p1_3) Dorsal
                                                                                </label>
                                                                            </div>
                                                                            <div class="checkbox">
                                                                                <label>
                                                                                    @Html.CheckBoxFor(model => model.p1_4) Volar
                                                                                </label>
                                                                            </div>
                                                                            <div class="checkbox">
                                                                                <label>
                                                                                    @Html.CheckBoxFor(model => model.p1_5) Cubital
                                                                                </label>
                                                                            </div>
                                                                            <div class="checkbox">
                                                                                <label>
                                                                                    @Html.CheckBoxFor(model => model.p1_6) Radial
                                                                                </label>
                                                                            </div>
                                                                            <div class="checkbox">
                                                                                <label>
                                                                                    @Html.CheckBoxFor(model => model.p1_7, new { @mostrar_respuesta = "p1_7_1" }) Otros
                                                                                </label>
                                                                            </div>
                                                                            @Html.TextAreaFor(model => model.p1_7_1, new { @placeholder = "Ingrese el dolor", @class = "form-control animate validate" })
                                                                        </div>

                                                                    </div>

                                                                    <div class="col-md-12" id="rpta_p1_1">
                                                                        <div class="checkbox">
                                                                            <label>
                                                                                @Html.CheckBoxFor(model => model.p1_1, new { @mostrar_respuesta = "rpta_1_1" }) Limitación funcional
                                                                            </label>
                                                                        </div>
                                                                        <div class="row">
                                                                            <div class="col-md-12">
                                                                                <div class="col-md-12" id="rpta_1_1">
                                                                                    <div class="checkbox">
                                                                                        <label>
                                                                                            @Html.CheckBoxFor(model => model.p1_11) Flexión
                                                                                        </label>
                                                                                    </div>
                                                                                    <div class="checkbox">
                                                                                        <label>
                                                                                            @Html.CheckBoxFor(model => model.p1_12) Extensión
                                                                                        </label>
                                                                                    </div>
                                                                                    <div class="checkbox">
                                                                                        <label>
                                                                                            @Html.CheckBoxFor(model => model.p1_13) Pronación
                                                                                        </label>
                                                                                    </div>
                                                                                    <div class="checkbox">
                                                                                        <label>
                                                                                            @Html.CheckBoxFor(model => model.p1_14) Supinación
                                                                                        </label>
                                                                                    </div>
                                                                                    <div class="checkbox">
                                                                                        <label>
                                                                                            @Html.CheckBoxFor(model => model.p1_15) Rotación
                                                                                        </label>
                                                                                    </div>
                                                                                    <div class="checkbox">
                                                                                        <label>
                                                                                            @Html.CheckBoxFor(model => model.p1_16, new { @mostrar_respuesta = "p1_16_1" }) Otros
                                                                                        </label>
                                                                                    </div>
                                                                                    @Html.TextAreaFor(model => model.p1_16_1, new { @placeholder = "Ingrese la limitacion funcional", @class = "form-control animate validate" })
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                        <div class="checkbox">
                                                                            <label>
                                                                                @Html.CheckBoxFor(model => model.p1_2, new { @mostrar_respuesta = "p1_21" }) Otros
                                                                            </label>
                                                                        </div>
                                                                        @Html.TextAreaFor(model => model.p1_21, new { @placeholder = "Ingrese el dolor", @class = "form-control animate validate" })
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                @*Pregunta 2*@
                                                <div class="form-group">
                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <div class="form-group">
                                                                <div>
                                                                    <cite>2. Crujido?</cite><span class="asterisco">(*)</span>
                                                                    <label class="checkbox-inline">
                                                                        @Html.RadioButtonFor(model => model.p2, "true") Si
                                                                    </label>
                                                                    <label class="checkbox-inline">
                                                                        @Html.RadioButtonFor(model => model.p2, "false") No
                                                                    </label>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                @*Pregunta 3*@
                                                <div class="form-group">
                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <div class="form-group">
                                                                <div>
                                                                    <cite>3. Aumento de volumen o sensacion de bulto?</cite><span class="asterisco">(*)</span>
                                                                    <label class="checkbox-inline">
                                                                        @Html.RadioButtonFor(model => model.p3, "true", new { @mostrar_respuesta = "rpta_p3" }) Si
                                                                    </label>
                                                                    <label class="checkbox-inline">
                                                                        @Html.RadioButtonFor(model => model.p3, "false", new { @ocultar_respuesta = "rpta_p3" }) No
                                                                    </label>
                                                                    <div class="col-md-12" id="rpta_p3">
                                                                        <div class="col-md-12">
                                                                            <cite>Donde?</cite><span class="asterisco">(*)</span>
                                                                            @Html.TextAreaFor(model => model.p3_1, new { @placeholder = "Ingrese la ubicación", @class = "form-control animate validate" })
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>

                                            </div>
                                        </div>
                                    </div>
                                </div>


                                @*Historial Molestias*@

                                <div class="row">

                                    <div class="col-md-12">

                                        <div class="panel panel-medico" data-collapsed="1">
                                            <div class="panel-heading">
                                                <div class="panel-title">Historia de Molestias </div>
                                                <div class="panel-options pull-right">
                                                    <a href="#" data-rel="collapse"><i class="entypo-down-open"></i></a>
                                                </div>
                                            </div>
                                            <div class="panel-body chart-responsive">
                                                <script type="text/javascript">
                                                    $(document).ready(function () {
                                                        $("#p4_1").change(function () { $("input:radio[name='p4_3']").removeAttr('checked'); $("#p4_3_1").hide(); });
                                                        $("#p4_2").change(function () { $("input:radio[name='p4_3']").removeAttr('checked'); $("#p4_3_1").hide(); });
                                                        $("input:radio[name='p4_3']").click(function () { if ($("input:radio[name='p4_3']").is(':checked')) { $('#p4_1').val(''); $("#p4_2").val(''); } });


                                                    });
                                                </script>
                                                @*Pregunta 4*@
                                                <div class="form-group">
                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <div class="form-group">
                                                                <div>
                                                                    <cite>4. Desde hace cuánto lo ha notado por primera vez?</cite><span class="asterisco">(*)</span>
                                                                    <div class="col-md-12">
                                                                        <div class="row">
                                                                            <div class="col-sm-2 col-lg-2">
                                                                                @Html.TextBoxFor(model => model.p4_1, new { @placeholder = "Cant.", @type = "number", @min = "0", @class = "form-control" })
                                                                            </div>
                                                                            <div class="col-sm-4 col-lg-4">
                                                                                @Html.DropDownListFor(model => model.p4_2, (SelectList)ViewBag.tiempo, "- Seleccione -", new { @class = "form-control" })
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                    <div class="col-md-12">
                                                                        <div class="radio">
                                                                            <label>
                                                                                @Html.RadioButtonFor(model => model.p4_3, "true", new { @ocultar_respuesta = "p4_3_1" }) No Precisa
                                                                            </label>
                                                                        </div>
                                                                        <div class="radio">
                                                                            <label>
                                                                                @Html.RadioButtonFor(model => model.p4_3, "false", new { @mostrar_respuesta = "p4_3_1" }) Otro
                                                                            </label>
                                                                        </div>
                                                                        @Html.TextAreaFor(model => model.p4_3_1, new { @placeholder = "Especifique su respuesta", @class = "form-control animate" })
                                                                    </div>
                                                                </div>

                                                            </div>
                                                        </div>
