                                    {
                                        <text>rutinario</text>
                                    }
                                    @if (Model.p15_2)
                                    {
                                        if (Model.p15_1)
                                        {
                                            <text>,</text>
                                        }
                                        <text>tumor</text>
                                    }
                                    @if (Model.p15_3)
                                    {
                                        if (Model.p15_1 || Model.p15_2)
                                        {
                                            <text>,</text>
                                        }
                                        <text>hipófisis / region selar</text>
                                    }
                                    @if (Model.p15_4)
