@model Encuesta.ViewModels.Encuesta.Tipos.EncuestaCerebroViewModel
@{
    ViewBag.Titlenot = "Encuesta Cerebro";
    Encuesta.Util.Variable util = new Encuesta.Util.Variable();
}

<script src="~/js/encuestas/encuestas_ini.js"></script>
<script src="~/js/encuestas/LectorEncuesta.js"></script>
<link href="~/css/Encuesta.css" rel="stylesheet" />
<script src="../js/jquery.autosize.js"></script>
<script src="~/js/jquery-ui.min.js"></script>
<div class="row body-medico" id="div_PreEntrevista">
    <div class="col-lg-12">
        <div class="">
            <div class="">
                <h1 class=" titulo_ocultar">ENCUESTA CEREBRO </h1>
            </div>
            <div class=" chart-responsive">
                @*Datos Examen*@
                <div class="row">

                    <div class="col-lg-12">

                        <div id="Info-examen" class="panel panel-medico">
                            <div class="panel-heading">
                                <h3 class="panel-title">Datos Examen</h3>
                                <div class="panel-options pull-right">
                                    <a href="#" data-rel="collapse"><i class="entypo-down-open"></i></a>
                                </div>
                            </div>
                            <div class="panel-body">
                                <div class="row">
                                    <div class="col-lg-12">

                                        <div class="col-sm-5 col-lg-5">
                                            @Html.Partial("_DatosExamenLector", Model._examen)
                                        </div>
                                        <div class="col-sm-5 col-lg-5">
                                            @Html.Partial("_DatosEncuestaLector", Model._encuesta)
                                        </div>
                                        <div class="col-sm-2 col-lg-2">
                                            <span>Adicional :</span>
                                            <div class="btn-group-vertical" role="group" style="margin-bottom: 10px;">

                                                @if (Model._examen.ATENCION.CITA.sedacion)
                                                {
                                                    <button class="btn btn-danger box-tools  pull-right no-print">SEDACION</button>
                                                }
                                                @if (Model._examen.ATENCION.CITA.claustrofobico)
                                                {
                                                    <button class="btn btn-info box-tools  pull-right no-print">CLAUSTROFOBICO</button>
                                                }

                                            </div>
                                            <div class="btn-group-vertical" role="group" id="div_adicional">
                                                <button class="btn btn-medico box-tools btn-sm pull-right no-print" id="btn-detalle" onclick="Mostrardetalle(false)">
                                                    <i class="fa fa-plus fa-1x"></i>&nbsp;Detalle Encuesta
                                                </button>
                                                <button class="btn btn-medico box-tools btn-sm pull-right no-print" onclick="verHistorial('@Model._examen.codigo')">
                                                    <i class="fa fa-history fa-1x"></i>&nbsp;Estudios Previos
                                                </button>

                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>


                </div>

                <div class="row" id="div_historial">
                </div>
                <div id="div-resumen">
                    <div class="pregunta">
                        @*PREGUNTA 1*@
                        @if (Model.p1.Value)
                        {<div class="col-md-12">
                            - <span class="resaltar">
                                @if (Model.p1_1)
                                {
                                    <text>Sufre dolores de cabeza con frecuencia</text>
                                }
                                @if (Model.p1_2)
                                {
                                    if (Model.p1_1)
                                    {
                                        <text>,</text>
                                    }
                                    <text>Interrumpe su sueño</text>
                                }
                            </span>
                            @if (Model.p1_3)
                            {
                                if (Model.p1_1 || Model.p1_2)
                                {
                                    <text>,</text>
                                }
                                else
                                {
                                    <text>Tiene dolor en</text>
                                }
                                <text><label class="resaltar">@Model.p1_4</label></text>
                            }.
                        </div>
                        }
                        @*PREGUNTA 2*@
                        @if (Model.p2.Value)
                        {
                            if (Model.p2_1.Value)
                            {
                                <div class="col-md-12">- Solo <span class="resaltar">tuvo una convulsión</span>.</div>
                            }
                            else
                            {
                                <div class="col-md-12">- <span class="resaltar">Tiene convulsiones</span> desde ya hace tiempo y está en tratamiento.</div>
                            }
                        }
                        @*PREGUNTA 3*@
                        @if (Model.p3.Value)
                        {
                            <div class="col-md-12">
                                - <span class="resaltar">Se golpeó la cabeza</span>
                                @if (Model.p3_1.Value)
                                {
                                    <text>y perdio la conciencia</text>
                                    if (Model.p3_1_1.Value)
                                    {
                                        <text>inmediatamente con el trauma</text>
                                    }
                                    else
                                    {
                                        <text>minutos a horas despues del trauma</text>
                                    }

                                    if (Model.p3_1_2.Value)
                                    {
                                        <text>, duró menos de cinco minutos</text>
                                    }
                                    else
                                    {
                                        <text>, duró más de cinco minutos</text>
                                    }
                                    if (Model.p3_1_3 || Model.p3_1_4 || Model.p3_1_5)
                                    {

                                        <text>seguido de:</text>
                                        if (Model.p3_1_3)
                                        {
                                            <text>amnesia transitoria</text>
                                        }
                                        if (Model.p3_1_4)
                                        {
                                            if (Model.p3_1_3)
                                            {
                                                <text>,</text>
                                            }
                                            <text>nauseas</text>
                                        }
                                        if (Model.p3_1_5)
                                        {
                                            if (Model.p3_1_4)
                                            {
                                                <text>,</text>
                                            }
                                            <text>mareos</text>}
                                    }
                                }.
                            </div>

                        }
                        @*PREGUNTA 4*@
                        @if (Model.p4.Value)
                        {
                            if (Model.p4_1.Value)
                            {
                                <div class="col-md-12">- <span class="resaltar">Tiene mareos</span> como sensacion de que las cosas dan vueltas (vértigo).</div>
                            }
                            else
