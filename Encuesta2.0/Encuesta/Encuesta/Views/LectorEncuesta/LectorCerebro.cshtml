                                    <text>hace</text>
                                    @Model.p12_2_3 <text></text> @util.getNombreTiempoRM(Model.p12_2_4)
                                    <span class="resaltar">
                                        por el siguiente motivo:
                                        <label>@Model.p12_2_5</label>
                                    </span>
                                }
                                .
                            </div>
                        }
