                                    }
                                    @if (Model.p15_5)
                                    {
                                        if (Model.p15_1 || Model.p15_2 || Model.p15_3 || Model.p15_4)
                                        {
                                            <text>,</text>
                                        }
                                        <text>convulsiones</text>
                                    }
                                    @if (Model.p15_6)
                                    {
                                        if (Model.p15_1 || Model.p15_2 || Model.p15_3 || Model.p15_4 || Model.p15_5)
                                        {
                                            <text>,</text>
                                        }
                                        <text>vias ópticas</text>
                                    }
                                    @if (Model.p15_7)
                                    {
                                        if (Model.p15_1 || Model.p15_2 || Model.p15_3 || Model.p15_4 || Model.p15_5 || Model.p15_6)
                                        {
                                            <text>,</text>
                                        }
                                        <text>órgano de la audición y equilibrio</text>
                                    }
                                    @if (Model.p15_8)
                                    {
                                        if (Model.p15_1 || Model.p15_2 || Model.p15_3 || Model.p15_4 || Model.p15_5 || Model.p15_6 || Model.p15_7)
                                        {
                                            <text>,</text>
                                        }
                                        <text>implante coclear</text>
                                    }
                                    @if (Model.p15_9)
                                    {
                                        if (Model.p15_1 || Model.p15_2 || Model.p15_3 || Model.p15_4 || Model.p15_5 || Model.p15_6 || Model.p15_7 || Model.p15_8)
                                        {
                                            <text>,</text>
                                        }
                                        <text>macizo facial</text>
                                    }
                                    @if (Model.p15_10)
                                    {
                                        if (Model.p15_1 || Model.p15_2 || Model.p15_3 || Model.p15_4 || Model.p15_5 || Model.p15_6 || Model.p15_7 || Model.p15_8 || Model.p15_9)
                                        {
                                            <text>,</text>
                                        }
                                        <text>parkinsonismo</text>
                                    }
                                    @if (Model.p15_11)
                                    {
                                        if (Model.p15_1 || Model.p15_2 || Model.p15_3 || Model.p15_4 || Model.p15_5 || Model.p15_6 || Model.p15_7 || Model.p15_8 || Model.p15_9 || Model.p15_10)
                                        {
                                            <text>,</text>
                                        }
                                        <text>esterotaxia</text>
                                    }
                                    @if (Model.p15_12)
                                    {
                                        if (Model.p15_1 || Model.p15_2 || Model.p15_3 || Model.p15_4 || Model.p15_5 || Model.p15_6 || Model.p15_7 || Model.p15_8 || Model.p15_9 || Model.p15_10 || Model.p15_11)
                                        {
                                            <text>,</text>
                                        }
                                        <text>protocolo investigación:</text><label>@Model.p15_12_1</label>
                                    }
                                    @if (Model.p15_13)
                                    {
                                        if (Model.p15_1 || Model.p15_2 || Model.p15_3 || Model.p15_4 || Model.p15_5 || Model.p15_6 || Model.p15_7 || Model.p15_8 || Model.p15_9 || Model.p15_10 || Model.p15_11 || Model.p15_12)
                                        {
                                            <text>,</text>
                                        }
                                        <label>@Model.p15_13_1</label>
                                    }
                                </span>
                                .
                            </div>
                        }
                        @*PREGUNTA 16*@
                        @if (Model.p16_1 || Model.p16_2 || Model.p16_3 || Model.p16_4 || Model.p16_5 || Model.p16_6 || Model.p16_7 || Model.p16_8 || Model.p16_9 || Model.p16_10)
                        {
