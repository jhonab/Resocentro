                                    <text>acción de otra persona o fuerza extrema</text>
                                }
                                @if (Model.p5_2)
                                {
                                    if (Model.p5_1)
                                    {
                                        <text>,</text>
                                    }
                                    <text>pérdida de conciencia previa</text>
                                }
                                @if (Model.p5_3)
                                {
                                    if (Model.p5_2)
                                    {
                                        <text>,</text>
                                    }
                                    <text>sensación de mareo o inestabilidad</text>
                                }
                                @if (Model.p5_4)
                                {
                                    if (Model.p5_3)
                                    {
                                        <text>,</text>
                                    }
                                    <label class="resaltar">@Model.p5_4_1;</label>
