                                <span class="resaltar">
                                    @if (Model.p16_1)
                                    {
                                        <text>angio RM arterial</text>
                                    }
                                    @if (Model.p16_2)
                                    {
                                        if (Model.p16_1)
                                        {
                                            <text>,</text>
                                        }
                                        <text>angio RM venosa</text>
                                    }
                                    @if (Model.p16_3)
                                    {
                                        if (Model.p16_1 || Model.p16_2)
                                        {
                                            <text>,</text>
