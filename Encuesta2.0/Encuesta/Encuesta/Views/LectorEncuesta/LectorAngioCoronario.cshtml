@model Encuesta.ViewModels.Encuesta.Tipos.EncuestaAngioCoronarioViewModel

@{
    ViewBag.Titlenot = "Encuesta Angio TEM Coronario";
    Encuesta.Util.Variable util = new Encuesta.Util.Variable();   
}

<script src="~/js/encuestas/LectorEncuesta.js"></script>
<script src="~/js/encuestas/encuestas_ini.js"></script>
<link href="~/css/Encuesta.css" rel="stylesheet" />
<script src="../js/jquery.autosize.js"></script>
<script src="~/js/jquery-ui.min.js"></script>
<div class="row body-medico" id="div_PreEntrevista">
    <div class="col-lg-12">
        <div class="box box-primary">
            <div class="box-header">
                <h1 class="box-title titulo_ocultar">
                    ENCUESTA ANGIO TEM CORONARIO

                </h1>
            </div>
            <div class="box-body chart-responsive">
                @*Datos Examen*@
            <div class="row">

                <div class="col-lg-12">

                    <div id="Info-examen" class="panel panel-medico">
                        <div class="panel-heading">
                            <h3 class="panel-title">Datos Examen</h3>
                            <div class="panel-options pull-right">
                                <a href="#" data-rel="collapse"><i class="entypo-down-open"></i></a>
                            </div>
                        </div>
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-lg-12">

                                    <div class="col-sm-5 col-lg-5">
                                        @Html.Partial("_DatosExamenLector", Model._examen)
                                    </div>
                                    <div class="col-sm-5 col-lg-5">
                                        @Html.Partial("_DatosEncuestaLector", Model._encuesta)
                                    </div>
                                    <div class="col-sm-2 col-lg-2">
                                        <span>Adicional :</span>
                                        <div class="btn-group-vertical" role="group" style="margin-bottom: 10px;">

                                            @if (Model._examen.ATENCION.CITA.sedacion)
                                            {
                                                <button class="btn btn-danger box-tools  pull-right no-print">SEDACION</button>
                                            }
                                            @if (Model._examen.ATENCION.CITA.claustrofobico)
                                            {
                                                <button class="btn btn-info box-tools  pull-right no-print">CLAUSTROFOBICO</button>
                                            }

                                        </div>
                                        <div class="btn-group-vertical" role="group" id="div_adicional">
                                            <button class="btn btn-medico box-tools btn-sm pull-right no-print" id="btn-detalle" onclick="Mostrardetalle(false)">
                                                <i class="fa fa-plus fa-1x"></i>&nbsp;Detalle Encuesta
                                            </button>
                                            <button class="btn btn-medico box-tools btn-sm pull-right no-print" onclick="verHistorial('@Model._examen.codigo')">
                                                <i class="fa fa-history fa-1x"></i>&nbsp;Estudios Previos
                                            </button>

                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>


            </div>
            <div class="row" id="div_historial">
            </div>
            <div id="div-resumen">
                <div class="pregunta">
                    @*Pregunta 1*@
                    @if (Model.p1.Value)
                    {
                        <div class="col-lg-12">
                            - <span class="resaltar">Tiene diagnóstico </span>desde
                            <span class="resaltar">@Model.p1_2 @util.getNombreTiempoRM(Model.p1_1)</span>,
                            <span class="resaltar">@Model.p1_3</span>
                            .
                        </div>
                    }
                    @*Pregunta 2*@
                    @if (Model.p2.Value)
                    {
                        <div class="col-lg-12">
                            - <span class="resaltar">Tiene molestias </span>en:
                            <span class="resaltar">@Model.p2_1</span>
                            .
                        </div>
                    }
                    @*Pregunta 3*@
                    @if (Model.p3.Value)
                    {
                        <div class="col-lg-12">
                            - <span class="">EKG </span>:
                            <span class="resaltar">@Model.p3_1</span>
                            .
                        </div>
                    }
                    @*Pregunta 4*@
                    @if (Model.p4.Value)
                    {
                        <div class="col-lg-12">
                            - <span class="">Prueba de esfuerzo </span>:
                            <span class="resaltar">@Model.p4_1</span>
                            .
                        </div>
                    }
                    @*Pregunta 5*@
                    @if (Model.p5.Value)
                    {
                        <div class="col-lg-12">
                            - <span class="">Ecocardiografía </span>:
                            <span class="resaltar">@Model.p5_1</span>
                            .
                        </div>
                    }
                    @*Pregunta 6*@
                    @if (Model.p6.Value)
                    {
                        <div class="col-lg-12">
                            - <span class="">Perfusión miocárdiaca de esfuerzo </span>:
                            <span class="resaltar">@Model.p6_1</span>
                            .
                        </div>
                    }
                    @*Pregunta 7*@
                    @if (Model.p6.Value)
                    {
                        <div class="col-lg-12">
                            - <span class="resaltar">Tiene cirugía </span>por:
                            @if (Model.p7_1)
                            {
                                <span class="resaltar">By pass</span>
                            }
                            @if (Model.p7_2)
                            {
                                if (Model.p7_1)
                                {
                                    <span>,</span>
                                }
                                <span class="resaltar">Stent</span>
                            }
                            @if (Model.p7_1)
                            {
                                if (Model.p7_1 || Model.p7_2)
                                {
                                    <span>,</span>
                                }
                                <span class="resaltar">@Model.p7_4</span>
                            }
                            .
                        </div>
                    }
                    @*Pregunta 8*@
                    @if (Model.p8 != null && Model.p8 != "")
                    {
                        <div class="col-lg-12">
                            - <span class="">Toma los siguientes medicamentos </span>:
                            <span class="resaltar">@Model.p8</span>
                            .
                        </div>
                    }
                    @*Pregunta 9*@
                    @if (Model.p9 != null && Model.p9 != "")
                    {
                        <div class="col-lg-12">
                            - <span class="">Enfermedad coronaria </span>:
                            <span class="resaltar">@Model.p9</span>
                            .
                        </div>
                    }
                    @*Pregunta 10*@
                    @if (Model.p10 != null && Model.p10 != "")
                    {
                        <div class="col-lg-12">
                            - <span class="">Permeabilidad de by pass </span>:
                            <span class="resaltar">@Model.p10</span>
                            .
                        </div>
                    }
                    @*Pregunta 11*@
                    @if (Model.p11 != null && Model.p11 != "")
                    {
                        <div class="col-lg-12">
                            - <span class="">Permeabilidad de by stent </span>:
                            <span class="resaltar">@Model.p11</span>
                            .
                        </div>
                    }
                    @*Pregunta 12*@
                    @if (Model.p12 != null && Model.p12 != "")
                    {
                        <div class="col-lg-12">
                            - <span class="">Otros </span>:
                            <span class="resaltar">@Model.p12</span>
                            .
                        </div>
                    }
                    @*PREGUNTA 13*@
                    @if (Model.p13 == 1)
                    {
                        <div class="col-lg-12">-  <span class="resaltar">Se indicó </span>usar atenolol en <span class="resaltar">50mg </span>.</div>
                    }
                    @if (Model.p13 == 2)
                    {
                        <div class="col-lg-12">- <span class="resaltar">Se indicó </span>usar atenolol <span class="resaltar">100mg </span>.</div>
                    }
                    @if (Model.p13 == 3)
                    {
                        <div class="col-lg-12">- SSe indicó  que <span class="resaltar">no</span>se use atenolol.</div>
                    }
                </div>
            </div>
