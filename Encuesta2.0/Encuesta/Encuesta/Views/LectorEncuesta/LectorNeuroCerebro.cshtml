                                                            <div class="form-group">
                                                                <div>
                                                                    <cite>10. Tumor cerebral, evolucion</cite>
                                                                    <div class="col-md-12">
                                                                        <div class="col-md-12">

                                                                            @Html.TextAreaFor(model => model.p10, new { @placeholder = "Especifique", @class = "form-control animate validate" })
                                                                        </div>
                                                                    </div>

                                                                </div>
                                                            </div>
