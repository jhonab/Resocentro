@model Encuesta.ViewModels.Encuesta.Tipos.EncuestaNeuroCerebroViewModel

@{
    ViewBag.Titlenot = "Encuesta Neuro Cerebro";
    Encuesta.Util.Variable util = new Encuesta.Util.Variable();   
}

<script src="~/js/encuestas/LectorEncuesta.js"></script>
<script src="~/js/encuestas/encuestas_ini.js"></script>
<link href="~/css/Encuesta.css" rel="stylesheet" />
<script src="../js/jquery.autosize.js"></script>
<script src="~/js/jquery-ui.min.js"></script>
<div class="row body-medico" id="div_PreEntrevista">
    <div class="col-lg-12">
        <div class="box box-primary">
            <div class="box-header">
                <h1 class="box-title titulo_ocultar">
                    ENCUESTA ANGIO NEURO CEREBRO - COLUMNA

                </h1>
            </div>
            <div class="box-body chart-responsive">
                @*Datos Examen*@
            <div class="row">

                <div class="col-lg-12">

                    <div id="Info-examen" class="panel panel-medico">
                        <div class="panel-heading">
                            <h3 class="panel-title">Datos Examen</h3>
                            <div class="panel-options pull-right">
                                <a href="#" data-rel="collapse"><i class="entypo-down-open"></i></a>
                            </div>
                        </div>
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-lg-12">

                                    <div class="col-sm-5 col-lg-5">
                                        @Html.Partial("_DatosExamenLector", Model._examen)
                                    </div>
                                    <div class="col-sm-5 col-lg-5">
                                        @Html.Partial("_DatosEncuestaLector", Model._encuesta)
                                    </div>
                                    <div class="col-sm-2 col-lg-2">
                                        <span>Adicional :</span>
                                        <div class="btn-group-vertical" role="group" style="margin-bottom: 10px;">

                                            @if (Model._examen.ATENCION.CITA.sedacion)
                                            {
                                                <button class="btn btn-danger box-tools  pull-right no-print">SEDACION</button>
                                            }
                                            @if (Model._examen.ATENCION.CITA.claustrofobico)
                                            {
                                                <button class="btn btn-info box-tools  pull-right no-print">CLAUSTROFOBICO</button>
                                            }

                                        </div>
                                        <div class="btn-group-vertical" role="group" id="div_adicional">
                                            <button class="btn btn-medico box-tools btn-sm pull-right no-print" id="btn-detalle" onclick="Mostrardetalle(false)">
                                                <i class="fa fa-plus fa-1x"></i>&nbsp;Detalle Encuesta
                                            </button>
                                            <button class="btn btn-medico box-tools btn-sm pull-right no-print" onclick="verHistorial('@Model._examen.codigo')">
                                                <i class="fa fa-history fa-1x"></i>&nbsp;Estudios Previos
                                            </button>

                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>


            </div>
            <div class="row" id="div_historial">
            </div>
            <div id="div-resumen">
                <div class="pregunta">
                    @*Pregunta 1*@
                    @if (Model.p1.Value)
                    {
                        <div class="col-lg-12">
                            - <span class="resaltar">Tiene diagnóstico </span>desde
                            <span class="resaltar">@Model.p1_2 @util.getNombreTiempoRM(Model.p1_1)</span>,
                            <span class="resaltar">@Model.p1_3</span>
                            .
                        </div>
                    }
                    @*Pregunta 2*@
                    @if (Model.p2.Value)
                    {
                        <div class="col-lg-12">
                            - <span class="resaltar">Tiene molestias </span>:
                            <span class="resaltar">@Model.p2_1</span>
                            .
                        </div>
                    }
                    @*PREGUNTA 3*@
                    @if (Model.p3.Value)
                    {
                        <div class="col-lg-12">
                            - Tiene antecedentes traumatológicos:
                            <span class="resaltar">@Model.p3_2.</span>
                        </div>
                    }
                    @*Pregunta 4*@
                    @if (Model.p4.Value)
                    {
                        <div class="col-lg-12">
                            - <span class="resaltar">Tiene cirugias </span>:
                            <span class="resaltar">@Model.p4_1</span>
                            @if (Model.p5 == 1)
                            {
                                <span class="resaltar">el resultado fue bueno</span>
                            }
                            @if (Model.p5 == 2)
                            {
                                <span class="resaltar">el resultado fue estable</span>
                            }
                            @if (Model.p5 == 3)
                            {
                                <span class="resaltar">el resultado fue malo</span>
                            }
                            .
                        </div>
                    }
                    @*Pregunta 6*@
                    @if (Model.p6.Value)
                    {
                        <div class="col-lg-12">
                            - <span class="resaltar">Tiene estudios </span>:
                            <span class="resaltar">@Model.p6_1</span>
                            .
                        </div>
                    }
                    @*Pregunta 7*@
                    @if (Model.p7_1)
                    {
                        <div class="col-lg-12">
                            - Segmento a evaluar <span class="resaltar">Cerebro</span>.

                        </div>
                    }
                    @*Pregunta 7*@
                    @if (Model.p7_2)
                    {
                        <div class="col-lg-12">
                            - Segmento a evaluar <span class="resaltar">Columna Cervical</span>.

                        </div>
                    }
                    @*Pregunta 7*@
                    @if (Model.p7_3)
                    {
                        <div class="col-lg-12">
                            - Segmento a evaluar <span class="resaltar">Columna Dorsal</span>.

                        </div>
                    }
                    @*Pregunta 7*@
                    @if (Model.p7_4)
                    {
                        <div class="col-lg-12">
                            - Segmento a evaluar <span class="resaltar">Columna Lumbosacra</span>.

                        </div>
                    }
                    @*Pregunta 7*@
                    @if (Model.p7_5)
                    {
                        <div class="col-lg-12">
                            - Segmento a evaluar <span class="resaltar">Columna Sacrocoxis</span>.

                        </div>
                    }
                    @*PREGUNTA 8*@
                    @if (Model.p8 != "")
                    {
                        <div class="col-lg-12">- ACV isquemia / hemorraguia: <span class="resaltar">@Model.p8</span> .</div>
                    }
                    @*PREGUNTA 9*@
                    @if (Model.p9 != "")
                    {
                        <div class="col-lg-12">- Aneurisma cerebral: <span class="resaltar">@Model.p9</span> .</div>
                    }
                    @*PREGUNTA 10*@
                    @if (Model.p10 != "")
                    {
                        <div class="col-lg-12">- Tumor cerebral, evolucion: <span class="resaltar">@Model.p10</span> .</div>
                    }
                    @*PREGUNTA 11*@
                    @if (Model.p11 != "")
                    {
                        <div class="col-lg-12">- Fractura cráneo, evolucion: <span class="resaltar">@Model.p11</span> .</div>
                    }
                    @*PREGUNTA 12*@
                    @if (Model.p12 != "")
                    {
                        <div class="col-lg-12">- Fractura vertebral, evolucion: <span class="resaltar">@Model.p12</span> .</div>
                    }
                    @*PREGUNTA 13*@
                    @if (Model.p13 != "")
                    {
                        <div class="col-lg-12">- HNP: <span class="resaltar">@Model.p13</span> .</div>
                    }
                    @*PREGUNTA 14*@
                    @if (Model.p14 != "")
                    {
                        <div class="col-lg-12">- Tumor espinal: <span class="resaltar">@Model.p14</span> .</div>
                    }
                    @*PREGUNTA 15*@
                    @if (Model.p15 != "")
                    {
                        <div class="col-lg-12">- Fijacion interna espinal: <span class="resaltar">@Model.p15</span> .</div>
                    }
                    @*PREGUNTA 16*@
                    @if (Model.p16 != "")
                    {
                        <div class="col-lg-12">- Además, <span class="resaltar">@Model.p16</span> .</div>
                    }

                    @*PREGUNTA 17*@
                    @if (Model.p17_1 || Model.p17_2 || Model.p17_3 || Model.p17_4 || Model.p17_5)
                    {
                        <div class="col-lg-12">
                            - Se solicitó realizar los siguientes protocolos:
                            <span class="resaltar">
                                @if (Model.p17_1)
                                {
                                    <text>Cerebro Simple</text>
                                }
                                @if (Model.p17_2)
                                {
                                    if (Model.p17_1)
                                    {
                                        <text>,</text>
                                    }
                                    <text>Cerebro con contraste</text>
                                }
                                @if (Model.p17_3)
                                {
                                    if (Model.p17_1 || Model.p17_2)
                                    {
                                        <text>,</text>
                                    }
                                    <text>AngioTEM cerebral</text>
                                }
                                @if (Model.p17_4)
                                {
                                    if (Model.p17_1 || Model.p17_2 || Model.p17_3)
                                    {
                                        <text>,</text>
                                    }
                                    <text>Columna sin contraste</text>
                                }
                                @if (Model.p17_5)
                                {
                                    if (Model.p17_1 || Model.p17_2 || Model.p17_3 || Model.p17_4)
                                    {
                                        <text>,</text>
                                    }
                                    <text>Columna con contraste</text>
                                }

                            </span>
                            .
                        </div>
                    }


                </div>
            </div>
            <div id="div-detallado">
                <h3></h3>

                @*Molestias*@
                <div class="row">

                    <div class="col-md-12">
                        <div class="">
                            <div class="">

                                @*Molestias*@
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="panel panel-medico" data-collapsed="1">
                                            <div class="panel-heading">
                                                <div class="panel-title">Principales Molestias </div>
                                                <div class="panel-options pull-right">
                                                    <a href="#" data-rel="collapse"><i class="entypo-down-open"></i></a>
                                                </div>
                                            </div>
                                            <div class="panel-body chart-responsive">

                                                @*Pregunta 1*@
                                                <div class="form-group">
                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <div class="form-group">
                                                                <div>
                                                                    <cite>1. Tiene diagnóstico?</cite><span class="asterisco">(*)</span>
                                                                    <label class="checkbox-inline">
                                                                        @Html.RadioButtonFor(model => model.p1, "true", new { @mostrar_respuesta = "rpta_p1" }) Si
                                                                    </label>
                                                                    <label class="checkbox-inline">
                                                                        @Html.RadioButtonFor(model => model.p1, "false", new { @ocultar_respuesta = "rpta_p1" }) No
                                                                    </label>
                                                                    <div class="col-md-12" id="rpta_p1">
                                                                        <div class="col-md-12">
                                                                            <div class="row">
                                                                                <div class="col-sm-5 col-lg-5">
                                                                                    @Html.DropDownListFor(model => model.p1_1, (SelectList)ViewBag.tiempo, "- Seleccione -", new { @class = "form-control" })
                                                                                </div>
                                                                                <div class="col-sm-2 col-lg-2">
                                                                                    @Html.TextBoxFor(model => model.p1_2, new { @placeholder = "Año", @type = "number", @min = "0", @class = "form-control" })
                                                                                </div>
                                                                            </div>
                                                                        </div>

                                                                        <div class="col-md-12">

                                                                            @Html.TextAreaFor(model => model.p1_3, new { @placeholder = "Ingrese el diagnóstico", @class = "form-control animate validate" })
                                                                        </div>
                                                                    </div>

                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                @*Pregunta 2*@
                                                <div class="form-group">
                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <div class="form-group">
                                                                <div>
                                                                    <cite>2. Presenta alguna molestia?</cite><span class="asterisco">(*)</span>
                                                                    <label class="checkbox-inline">
                                                                        @Html.RadioButtonFor(model => model.p2, "true", new { @mostrar_respuesta = "rpta_p2" }) Si
                                                                    </label>
                                                                    <label class="checkbox-inline">
                                                                        @Html.RadioButtonFor(model => model.p2, "false", new { @ocultar_respuesta = "rpta_p2" }) No
                                                                    </label>


                                                                    <div class="col-md-12" id="rpta_p2">
                                                                        <div class="col-md-12">

                                                                            @Html.TextAreaFor(model => model.p2_1, new { @placeholder = "Ingrese la molestia ", @class = "form-control animate validate" })
                                                                        </div>
                                                                    </div>

                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                @*Procedimientos Relacionados*@
                <div class="row">

                    <div class="col-md-12">
                        <div class="">
                            <div class="">
                                <div class="row">

                                    <div class="col-md-12">

                                        <div class="panel panel-medico" data-collapsed="1">
                                            <div class="panel-heading">
                                                <div class="panel-title">Procedimientos Relacionados </div>
                                                <div class="panel-options pull-right">
                                                    <a href="#" data-rel="collapse"><i class="entypo-down-open"></i></a>
                                                </div>
                                            </div>
                                            <div class="panel-body chart-responsive">
                                                @*Pregunta 3*@
                                                <div class="form-group">
                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <div class="form-group">
                                                                <div>
                                                                    <cite>3. Antecedentes traumáticos?</cite><span class="asterisco">(*)</span>
                                                                    <label class="checkbox-inline">
                                                                        @Html.RadioButtonFor(model => model.p3, "true", new { @mostrar_respuesta = "rpta_p3" }) Si
                                                                    </label>
                                                                    <label class="checkbox-inline">
                                                                        @Html.RadioButtonFor(model => model.p3, "false", new { @ocultar_respuesta = "rpta_p3" }) No
                                                                    </label>
                                                                    <div class="col-md-12" id="rpta_p3">
                                                                        <div class="col-md-12">
                                                                            @Html.TextAreaFor(model => model.p3_2, new { @placeholder = "Ingrese el resultado", @class = "form-control animate validate" })
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                @*Pregunta 4*@
                                                <div class="form-group">
                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <div class="form-group">
                                                                <div>
                                                                    <cite>4. Cirugias previas?</cite><span class="asterisco">(*)</span>
                                                                    <label class="checkbox-inline">
