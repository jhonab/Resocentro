@model Encuesta.ViewModels.Encuesta.Tipos.EncuestaOncologicoViewModel

@{
    ViewBag.Titlenot = "Encuesta Oncológico";
    Encuesta.Util.Variable util = new Encuesta.Util.Variable();   
}

<script src="~/js/encuestas/LectorEncuesta.js"></script>
<script src="~/js/encuestas/encuestas_ini.js"></script>
<link href="~/css/Encuesta.css" rel="stylesheet" />
<script src="../js/jquery.autosize.js"></script>
<script src="~/js/jquery-ui.min.js"></script>
<div class="row body-medico" id="div_PreEntrevista">
    <div class="col-lg-12">
        <div class="box box-primary">
            <div class="box-header">
                @{string tipo_encuesta = "ONCOLÓGICO";
                }
                <h1 class="box-title titulo_ocultar">
                    ENCUESTA @tipo_encuesta.ToUpper()

                </h1>
            </div>
            <div class="box-body chart-responsive">
                @*Datos Examen*@
                <div class="row">

                    <div class="col-lg-12">

                        <div id="Info-examen" class="panel panel-medico">
                            <div class="panel-heading">
                                <h3 class="panel-title">Datos Examen</h3>
                                <div class="panel-options pull-right">
                                    <a href="#" data-rel="collapse"><i class="entypo-down-open"></i></a>
                                </div>
                            </div>
                            <div class="panel-body">
                                <div class="row">
                                    <div class="col-lg-12">

                                        <div class="col-sm-5 col-lg-5">
                                            @Html.Partial("_DatosExamenLector", Model._examen)
                                        </div>
                                        <div class="col-sm-5 col-lg-5">
                                            @Html.Partial("_DatosEncuestaLector", Model._encuesta)
                                        </div>
                                        <div class="col-sm-2 col-lg-2">
                                            <span>Adicional :</span>
                                            <div class="btn-group-vertical" role="group" style="margin-bottom: 10px;">

                                                @if (Model._examen.ATENCION.CITA.sedacion)
                                                {
                                                    <button class="btn btn-danger box-tools  pull-right no-print">SEDACION</button>
                                                }
                                                @if (Model._examen.ATENCION.CITA.claustrofobico)
                                                {
                                                    <button class="btn btn-info box-tools  pull-right no-print">CLAUSTROFOBICO</button>
                                                }

                                            </div>
                                            <div class="btn-group-vertical" role="group" id="div_adicional">
                                                <button class="btn btn-medico box-tools btn-sm pull-right no-print" id="btn-detalle" onclick="Mostrardetalle(false)">
                                                    <i class="fa fa-plus fa-1x"></i>&nbsp;Detalle Encuesta
                                                </button>
                                                <button class="btn btn-medico box-tools btn-sm pull-right no-print" onclick="verHistorial('@Model._examen.codigo')">
                                                    <i class="fa fa-history fa-1x"></i>&nbsp;Estudios Previos
                                                </button>

                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>


                </div>

                <div class="row" id="div_historial">
                </div>
                <div id="div-resumen">
                    <div class="pregunta">
                        @*Pregunta 1*@
                        @if (Model.p1.Value)
                        {
                            <div class="col-lg-12">
                                - <span class="resaltar">Tiene molestias</span> :
                                <span class="resaltar">@Model.p1_1</span>
                                .
                            </div>
                        }
                        @*Pregunta 2*@
                        @if (Model.p2.Value)
                        {
                            <div class="col-lg-12">
                                - <span class="resaltar">Tiene diagnóstico</span>.
                            </div>
                            <div class="col-lg-12">
                                @if (Model.p2_1.Value)
                                {
                                    <span>- Tiene cáncer</span>
                                    <span class="resaltar">
                                        @if (Model.p2_1_1 == 1)
                                        {
                                            <span>de mama</span>
                                        }
                                        else if (Model.p2_1_1 == 2)
                                        {
                                            <span>de pulmón</span>
                                        }
                                        else if (Model.p2_1_1 == 3)
                                        {
                                            <span>de estómago</span>
                                        }
                                        else if (Model.p2_1_1 == 4)
                                        {
                                            <span>de colon</span>
                                        }
                                        else if (Model.p2_1_1 == 5)
                                        {
                                            <span>de riñón</span>
                                        }
                                        else if (Model.p2_1_1 == 6)
                                        {
                                            <span>de próstata</span>
                                        }
                                        else if (Model.p2_1_1 == 7)
                                        {
                                            <span>de ovario</span>
                                        }
                                        else if (Model.p2_1_1 == 8)
                                        {
                                            <span class="resaltar">de @Model.p2_1_2</span>
                                        }
                                    </span>
                                    <span>.</span>
                                }
                            </div>
                            <div class="col-lg-12">
                                - <span>Anatomía Patológica </span><span class="resaltar">@Model.p2_2</span>.

                            </div>
                            <div class="col-lg-12">
                                - <span>Grado Histológico </span><span class="resaltar">@Model.p2_3</span>.

                            </div>
                        }
                        @*Pregunta 3*@
                        <div class="col-lg-12">
                            <span>- El motivo del estudio es</span>
                            <span class="resaltar">
                                @if (Model.p3 == 1)
                                {
                                    <span>diagnóstico</span>
                                }
                                else if (Model.p3 == 2)
                                {
                                    <span>estadiaje</span>
                                }
                                else if (Model.p3 == 3)
                                {
                                    <span>reestadiaje</span>
                                }
                                else if (Model.p3 == 5)
                                {
                                    <span>sospecha de recurrencia</span>
                                }
                                else if (Model.p3 == 4)
                                {
                                    <span class="resaltar">@Model.p3_1</span>
                                }
                            </span>
                            <span>.</span>

                        </div>
                        @*Pregunta 4*@
                        <div class="col-lg-12">
                            @if (Model.p4.Value)
                            {
                                <span>- Tiene a familiares con cancer: </span>
                                <span class="resaltar">@Model.p4_1</span>
                                <span>.</span>
                            }
                        </div>
                        @*Pregunta 5*@
                        <div class="col-lg-12">
                            @if (Model.p5.Value)
                            {
                                <span>- Tiene Qx: </span>
                                if (Model.p5_1.Value)
                                {
                                    <div class="col-lg-12">
                                        <span>- Histerectomía hace </span><span class="resaltar">@Model.p5_1_2 @util.getNombreTiempoRM(Model.p5_1_1)</span>.
                                    </div>
                                }
                                if (Model.p5_2.Value)
                                {
                                    <div class="col-lg-12">
                                        <span>- Prostatectomía hace </span><span class="resaltar">@Model.p5_2_2 @util.getNombreTiempoRM(Model.p5_2_1)</span>.
                                    </div>
                                }
                                if (Model.p5_3.Value)
                                {
                                    <div class="col-lg-12">
                                        <span>- Colecistectomía hace </span><span class="resaltar">@Model.p5_3_2 @util.getNombreTiempoRM(Model.p5_3_1)</span>.
                                    </div>
                                }
                                if (Model.p5_4 != "")
                                {
                                    <div class="col-lg-12">
                                        <span>- </span><span class="resaltar">@Model.p5_4 </span>.
                                    </div>
                                }


                            }
                        </div>
                        @*Pregunta 6*@
                        @if (Model.p6.Value)
                        {
                            <div class="col-lg-12">
                                <span>- Se realizó quimioterapia hace </span><span class="resaltar">@Model.p6_3 @util.getNombreTiempoRM(Model.p6_2)</span>
                                @if (Model.p6_1.Value)
                                {
                                    <span class="resaltar">y la termino </span>
                                }
                                else
                                {
                                    <span class="resaltar">y no la termino </span>
                                }
                                .
                            </div>
                        }
                        @*Pregunta 7*@
                        @if (Model.p7.Value)
                        {
                            <div class="col-lg-12">
                                <span>- Se realizó radioterapia hace </span><span class="resaltar">@Model.p7_3 @util.getNombreTiempoRM(Model.p7_2)</span>
                                @if (Model.p7_1.Value)
                                {
                                    <span class="resaltar">y la termino </span>
                                }
                                else
                                {
                                    <span class="resaltar">y no la termino </span>
                                }
                                .
                            </div>
                        }
                        @*PREGUNTA 8*@
                        @if (Model.p8.Value)
                        {
                            <div class="col-lg-12">
                                - <span class="resaltar">Tiene estudios </span>previos en
                                @util.getNombreLocales(Model.p8_1)
                                hace
                                <span class="resaltar">
                                    @Model.p8_3
                                @util.getNombreTiempoRM(Model.p8_4)
                            </span>
                            por el siguiente motivo:
                            <span class="resaltar">@Model.p8_5</span>
                            .
                        </div>
                        }

                        @*PREGUNTA 9*@
                        @if (Model.p9 != null)
                        {
                            if (Model.p9 != "")
                            {
                                <div class="col-lg-12">- Marcadores tumorales: <span class="resaltar">@Model.p9</span> .</div>
                            }
                        }


                        @*Pregunta10*@
                        @if (Model.p10)
                        {
                            <div class="col-lg-12">- <span class="resaltar">Se indicó </span> colocación de Reparo Anatómico.</div>
                        }
                        else
                        {
                            <div class="col-lg-12">- <span class="resaltar">No se indicó </span> colocación de Reparo Anatómico.</div>
                        }

                    </div>
                </div>
                <div id="div-detallado">
                    <h3></h3>
                    @*Molestias*@
                    <div class="row">

                        <div class="col-md-12">
                            <div class=" ">

                                <div class="">

                                    @*Molestias*@
                                    <div class="row">
                                        <div class="col-md-12">
                                            <div class="panel panel-medico" data-collapsed="1">
                                                <div class="panel-heading">
                                                    <div class="panel-title">Principales Molestias </div>
                                                    <div class="panel-options pull-right">
                                                        <a href="#" data-rel="collapse"><i class="entypo-down-open"></i></a>
                                                    </div>
                                                </div>
                                                <div class="panel-body chart-responsive">

                                                    @*Pregunta 1*@
                                                    <div class="form-group">
                                                        <div class="row">
                                                            <div class="col-md-12">
                                                                <div class="form-group">
                                                                    <div>
                                                                        <cite>1. Tiene molestias?</cite><span class="asterisco">(*)</span>
                                                                        <label class="checkbox-inline">
                                                                            @Html.RadioButtonFor(model => model.p1, "true", new { @mostrar_respuesta = "rpta_p1" }) Si
                                                                        </label>
                                                                        <label class="checkbox-inline">
                                                                            @Html.RadioButtonFor(model => model.p1, "false", new { @ocultar_respuesta = "rpta_p1" }) No
                                                                        </label>
                                                                        <div class="col-md-12" id="rpta_p1">
                                                                            @Html.TextAreaFor(model => model.p1_1, new { @placeholder = "Ingrese las molestias", @class = "form-control animate validate" })
                                                                        </div>

                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    @*Pregunta 2*@
                                                    <div class="form-group">
                                                        <div class="row">
                                                            <div class="col-md-12">
                                                                <div class="form-group">
                                                                    <div>
                                                                        <cite>2. Tiene diagnóstico?</cite><span class="asterisco">(*)</span>
                                                                        <label class="checkbox-inline">
                                                                            @Html.RadioButtonFor(model => model.p2, "true", new { @mostrar_respuesta = "rpta_p2" }) Si
                                                                        </label>
                                                                        <label class="checkbox-inline">
                                                                            @Html.RadioButtonFor(model => model.p2, "false", new { @ocultar_respuesta = "rpta_p2" }) No
                                                                        </label>


                                                                        <div class="col-md-12" id="rpta_p2">
                                                                            <div class="col-md-12">
                                                                                <cite>Cáncer?</cite><span class="asterisco">(*)</span>
                                                                                <label class="checkbox-inline">
                                                                                    @Html.RadioButtonFor(model => model.p2_1, "true", new { @mostrar_respuesta = "rpta_p2_1" }) Si
                                                                                </label>
                                                                                <label class="checkbox-inline">
                                                                                    @Html.RadioButtonFor(model => model.p2_1, "false", new { @ocultar_respuesta = "rpta_p2_1" }) No
                                                                                </label>
                                                                                <div class="col-md-12" id="rpta_p2_1">
                                                                                    <div class="radio">
                                                                                        <label>
                                                                                            @Html.RadioButtonFor(model => model.p2_1_1, "1", new { @ocultar_respuesta = "p2_1_2" }) Mama
                                                                                        </label>
                                                                                    </div>
                                                                                    <div class="radio">
                                                                                        <label>
                                                                                            @Html.RadioButtonFor(model => model.p2_1_1, "2", new { @ocultar_respuesta = "p2_1_2" }) Pulmón
                                                                                        </label>
                                                                                    </div>
                                                                                    <div class="radio">
                                                                                        <label>
                                                                                            @Html.RadioButtonFor(model => model.p2_1_1, "3", new { @ocultar_respuesta = "p2_1_2" }) Estómago
                                                                                        </label>
                                                                                    </div>
                                                                                    <div class="radio">
                                                                                        <label>
                                                                                            @Html.RadioButtonFor(model => model.p2_1_1, "4", new { @ocultar_respuesta = "p2_1_2" }) Colon
                                                                                        </label>
                                                                                    </div>
                                                                                    <div class="radio">
                                                                                        <label>
                                                                                            @Html.RadioButtonFor(model => model.p2_1_1, "5", new { @ocultar_respuesta = "p2_1_2" }) Riñón
                                                                                        </label>
                                                                                    </div>
                                                                                    <div class="radio">
                                                                                        <label>
                                                                                            @Html.RadioButtonFor(model => model.p2_1_1, "6", new { @ocultar_respuesta = "p2_1_2" }) Próstata
                                                                                        </label>
                                                                                    </div>
                                                                                    <div class="radio">
                                                                                        <label>
                                                                                            @Html.RadioButtonFor(model => model.p2_1_1, "7", new { @ocultar_respuesta = "p2_1_2" }) Ovario
                                                                                        </label>
                                                                                    </div>
                                                                                    <div class="radio">
                                                                                        <label>
                                                                                            @Html.RadioButtonFor(model => model.p2_1_1, "8", new { @mostrar_respuesta = "p2_1_2" }) Otro
                                                                                        </label>
                                                                                    </div>
                                                                                    @Html.TextAreaFor(model => model.p2_1_2, new { @placeholder = "Ingrese el tipo de cáncer", @class = "form-control animate validate" })
                                                                                </div>
                                                                            </div>
                                                                            <div class="col-md-12">
                                                                                <cite>Anatomía Patológica</cite><span class="asterisco">(*)</span>
                                                                                <div class="col-md-12">
                                                                                    @Html.TextAreaFor(model => model.p2_2, new { @placeholder = "Ingrese la anatimía patológica", @class = "form-control animate validate" })
                                                                                </div>
                                                                            </div>
                                                                            <div class="col-md-12">
                                                                                <cite>Grado Histológico</cite><span class="asterisco">(*)</span>
                                                                                <div class="col-md-12">
                                                                                    @Html.TextAreaFor(model => model.p2_3, new { @placeholder = "Ingrese el grado histológico", @class = "form-control animate validate" })
                                                                                </div>
                                                                            </div>
                                                                        </div>

                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    @*Pregunta 3*@
                                                    <div class="form-group">
                                                        <div class="row">
                                                            <div class="col-md-12">
                                                                <div class="form-group">
                                                                    <div>
                                                                        <cite>3. Cuál es el motivo del estudio?</cite><span class="asterisco">(*)</span>

                                                                        <div class="col-md-12">
                                                                            <div class="col-md-12">
                                                                                <div class="radio">
                                                                                    <label>
                                                                                        @Html.RadioButtonFor(model => model.p3, "1", new { @ocultar_respuesta = "p3_1" }) Diagnóstico
                                                                                    </label>
                                                                                </div>
                                                                                <div class="radio">
                                                                                    <label>
                                                                                        @Html.RadioButtonFor(model => model.p3, "2", new { @ocultar_respuesta = "p3_1" }) Estadiaje
                                                                                    </label>
                                                                                </div>
                                                                                <div class="radio">
                                                                                    <label>
                                                                                        @Html.RadioButtonFor(model => model.p3, "3", new { @ocultar_respuesta = "p3_1" }) Reestadiaje
                                                                                    </label>
                                                                                </div>
                                                                                <div class="radio">
                                                                                    <label>
                                                                                        @Html.RadioButtonFor(model => model.p3, "5", new { @ocultar_respuesta = "p3_1" }) Sospecha de recurrencia
                                                                                    </label>
                                                                                </div>
                                                                                <div class="radio">
                                                                                    <label>
                                                                                        @Html.RadioButtonFor(model => model.p3, "4", new { @mostrar_respuesta = "p3_1" }) Otro
                                                                                    </label>
                                                                                </div>
                                                                                @Html.TextAreaFor(model => model.p3_1, new { @placeholder = "Ingrese el motivo", @class = "form-control animate validate" })
                                                                            </div>
                                                                        </div>

                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>


                                    @*Antecedentes Oncológicos*@

                                    <div class="row">

                                        <div class="col-md-12">

                                            <div class="panel panel-medico" data-collapsed="1">
                                                <div class="panel-heading">
                                                    <div class="panel-title">Antecedentes Oncológicos </div>
                                                    <div class="panel-options pull-right">
                                                        <a href="#" data-rel="collapse"><i class="entypo-down-open"></i></a>
                                                    </div>
                                                </div>
                                                <div class="panel-body chart-responsive">
                                                    @*Pregunta 4*@
                                                    <div class="form-group">
                                                        <div class="row">
                                                            <div class="col-md-12">
                                                                <div class="form-group">
                                                                    <div>
                                                                        <cite>4. Algun familiar con cáncer?</cite><span class="asterisco">(*)</span>
                                                                        <label class="checkbox-inline">
                                                                            @Html.RadioButtonFor(model => model.p4, "true", new { @mostrar_respuesta = "rpta_p4" }) Si
                                                                        </label>
                                                                        <label class="checkbox-inline">
                                                                            @Html.RadioButtonFor(model => model.p4, "false", new { @ocultar_respuesta = "rpta_p4" }) No
                                                                        </label>
                                                                        <div class="col-md-12" id="rpta_p4">
                                                                            <div class="row">
                                                                                @Html.TextBoxFor(model => model.p4_1, new { @placeholder = "Especifique", @class = "form-control animate validate" })
                                                                            </div>
                                                                        </div>
                                                                    </div>

                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>


                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    @*Procedimientos Relacionados*@
                    <div class="row">

                        <div class="col-md-12">
                            <div class=" ">
                                <div class="">


                                    <div class="row">

                                        <div class="col-md-12">

                                            <div class="panel panel-medico" data-collapsed="1">
                                                <div class="panel-heading">
                                                    <div class="panel-title">Procedimientos Relacionados </div>
                                                    <div class="panel-options pull-right">
                                                        <a href="#" data-rel="collapse"><i class="entypo-down-open"></i></a>
                                                    </div>
                                                </div>
                                                <div class="panel-body chart-responsive">
                                                    @*Pregunta 5*@
                                                    <div class="form-group">
                                                        <div class="row">
                                                            <div class="col-md-12">
                                                                <div class="form-group">
                                                                    <div>
                                                                        <cite>5. Cirugías previas?</cite><span class="asterisco">(*)</span>
                                                                        <label class="checkbox-inline">
                                                                            @Html.RadioButtonFor(model => model.p5, "true", new { @mostrar_respuesta = "rpta_p5" }) Si
                                                                        </label>
                                                                        <label class="checkbox-inline">
                                                                            @Html.RadioButtonFor(model => model.p5, "false", new { @ocultar_respuesta = "rpta_p5" }) No
                                                                        </label>
                                                                        <div class="col-md-12" id="rpta_p5">

                                                                            <div class="row">
                                                                                <div class="col-md-12">
                                                                                    <cite>Histerectomía</cite><span class="asterisco">(*)</span>
                                                                                    <label class="checkbox-inline">
                                                                                        @Html.RadioButtonFor(model => model.p5_1, "true", new { @mostrar_respuesta = "rpta_p5_1" }) Si
                                                                                    </label>
                                                                                    <label class="checkbox-inline">
                                                                                        @Html.RadioButtonFor(model => model.p5_1, "false", new { @ocultar_respuesta = "rpta_p5_1" }) No
                                                                                    </label>
                                                                                    <div class="col-md-12" id="rpta_p5_1">
                                                                                        <div class="row">
                                                                                            <div class="col-sm-2 col-lg-2">
                                                                                                @Html.TextBoxFor(model => model.p5_1_2, new { @placeholder = "Cant.", @type = "number", @min = "0", @class = "form-control" })
                                                                                            </div>
                                                                                            <div class="col-sm-5 col-lg-5">
                                                                                                @Html.DropDownListFor(model => model.p5_1_1, (SelectList)ViewBag.tiempo, "- Seleccione -", new { @class = "form-control" })
                                                                                            </div>

                                                                                        </div>
                                                                                    </div>

                                                                                </div>
                                                                            </div>

                                                                            <div class="row">
                                                                                <div class="col-md-12">
                                                                                    <cite>Prostatectomía</cite><span class="asterisco">(*)</span>
                                                                                    <label class="checkbox-inline">
                                                                                        @Html.RadioButtonFor(model => model.p5_2, "true", new { @mostrar_respuesta = "rpta_p5_2" }) Si
                                                                                    </label>
                                                                                    <label class="checkbox-inline">
                                                                                        @Html.RadioButtonFor(model => model.p5_2, "false", new { @ocultar_respuesta = "rpta_p5_2" }) No
                                                                                    </label>
                                                                                    <div class="col-md-12" id="rpta_p5_2">
                                                                                        <div class="row">
                                                                                            <div class="col-sm-2 col-lg-2">
                                                                                                @Html.TextBoxFor(model => model.p5_2_2, new { @placeholder = "Cant.", @type = "number", @min = "0", @class = "form-control" })
                                                                                            </div>
                                                                                            <div class="col-sm-5 col-lg-5">
                                                                                                @Html.DropDownListFor(model => model.p5_2_1, (SelectList)ViewBag.tiempo, "- Seleccione -", new { @class = "form-control" })
                                                                                            </div>
                                                                                        </div>
                                                                                    </div>

                                                                                </div>
