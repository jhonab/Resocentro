@model Encuesta.ViewModels.Encuesta.Tipos.EncuestaAngioAortaViewModel

@{
    ViewBag.Titlenot = "Encuesta Angio TEM Aorta y Carótidas";
    Encuesta.Util.Variable util = new Encuesta.Util.Variable(); 
}

<script src="~/js/encuestas/LectorEncuesta.js"></script>
<script src="~/js/encuestas/encuestas_ini.js"></script>
<link href="~/css/Encuesta.css" rel="stylesheet" />
<script src="../js/jquery.autosize.js"></script>
<script src="~/js/jquery-ui.min.js"></script>
<div class="row body-medico" id="div_PreEntrevista">
    <div class="col-lg-12">
        <div class="box box-primary">
            <div class="box-header">
                <h1 class="box-title titulo_ocultar">
                    ENCUESTA ANGIO TEM AORTA Y CARÓTIDAS

                </h1>
            </div>
            <div class="box-body chart-responsive">
                @*Datos Examen*@
            <div class="row">

                <div class="col-lg-12">

                    <div id="Info-examen" class="panel panel-medico">
                        <div class="panel-heading">
                            <h3 class="panel-title">Datos Examen</h3>
                            <div class="panel-options pull-right">
                                <a href="#" data-rel="collapse"><i class="entypo-down-open"></i></a>
                            </div>
                        </div>
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-lg-12">

                                    <div class="col-sm-5 col-lg-5">
                                        @Html.Partial("_DatosExamenLector", Model._examen)
                                    </div>
                                    <div class="col-sm-5 col-lg-5">
                                        @Html.Partial("_DatosEncuestaLector", Model._encuesta)
                                    </div>
                                    <div class="col-sm-2 col-lg-2">
                                        <span>Adicional :</span>
                                        <div class="btn-group-vertical" role="group" style="margin-bottom: 10px;">

                                            @if (Model._examen.ATENCION.CITA.sedacion)
                                            {
                                                <button class="btn btn-danger box-tools  pull-right no-print">SEDACION</button>
                                            }
                                            @if (Model._examen.ATENCION.CITA.claustrofobico)
                                            {
                                                <button class="btn btn-info box-tools  pull-right no-print">CLAUSTROFOBICO</button>
                                            }

                                        </div>
                                        <div class="btn-group-vertical" role="group" id="div_adicional">
                                            <button class="btn btn-medico box-tools btn-sm pull-right no-print" id="btn-detalle" onclick="Mostrardetalle(false)">
                                                <i class="fa fa-plus fa-1x"></i>&nbsp;Detalle Encuesta
                                            </button>
                                            <button class="btn btn-medico box-tools btn-sm pull-right no-print" onclick="verHistorial('@Model._examen.codigo')">
                                                <i class="fa fa-history fa-1x"></i>&nbsp;Estudios Previos
                                            </button>

                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>


            </div>
            <div class="row" id="div_historial">
            </div>
            <div id="div-resumen">
                <div class="pregunta">
                    @*Pregunta 1*@
                    @if (Model.p1.Value)
                    {
                        <div class="col-lg-12">
                            - <span class="resaltar">Tiene diagnóstico </span>desde
                            <span class="resaltar">@Model.p1_2 @util.getNombreTiempoRM(Model.p1_1)</span>,
                            <span class="resaltar">@Model.p1_3</span>
                            .
                        </div>
                    }
                    @*Pregunta 2*@
                    @if (Model.p2.Value)
                    {
                        <div class="col-lg-12">
                            - <span class="resaltar">Tiene molestias </span>en:
                            <span class="resaltar">@Model.p2_1</span>
                            .
                        </div>
                    }
                    @*Pregunta 3*@
                    @if (Model.p3.Value)
                    {
                        <div class="col-lg-12">
                            - <span class="">EcoDoppler </span>:
                            <span class="resaltar">@Model.p3_1</span>
                            .
                        </div>
                    }
                    @*Pregunta 4*@
                    @if (Model.p4.Value)
                    {
                        <div class="col-lg-12">
                            - <span class="">Angiotem </span>:
                            <span class="resaltar">@Model.p4_1</span>
                            .
                        </div>
                    }
                    @*Pregunta 5*@
                    @if (Model.p5_3 != null && Model.p5_3 != "")
                    {
                        <div class="col-lg-12">
                            - <span class="resaltar">Tiene cirugía </span>por:S
                            <span class="resaltar">@Model.p5_3</span>
                            .
                        </div>
                    }

                    @*PREGUNTA 6*@
                    @if (Model.p7_1 == false)
                    {
                        <div class="col-lg-12">- Orden médica menciona <span class="resaltar">@Model.p7</span> .</div>
                    }
                    @if (Model.p8_1 == false)
                    {
                        <div class="col-lg-12">- El paciente comenta <span class="resaltar">@Model.p8</span> .</div>
                    }
                    @*PREGUNTA 7*@
                    @if (Model.p9 != "")
                    {
                        <div class="col-lg-12">- Se desea resolver el o los siguientes problemas diagnosticados <span class="resaltar">@Model.p9</span> .</div>
                    }

                    @*PREGUNTA 13*@
                    @if (Model.p11_1 || Model.p11_2 || Model.p11_3 || Model.p11_4 || Model.p11_5)
                    {
                        <div class="col-lg-12">
                            - Se indicaron los procedimientos:
                            @if (Model.p11_1)
                            {
                                <div class="col-lg-12">-  <span class="resaltar">Protocolo estándar angiotem aorta torácica </span>.</div>
                            }
                            @if (Model.p11_2)
                            {
                                <div class="col-lg-12">-  <span class="resaltar">Protocolo estándar angiotem aorta torácica y abdominal </span>.</div>
                            }
                            @if (Model.p11_3)
                            {
                                <div class="col-lg-12">-  <span class="resaltar">Protocolo estándar angiotem aorta abdominal e iliacas </span>.</div>
                            }
                            @if (Model.p11_4)
                            {
                                <div class="col-lg-12">-  <span class="resaltar">Protocolo estándar angiotem aorterias carótidas </span>.</div>
                            }
                            @if (Model.p11_5)
                            {
                                <div class="col-lg-12">-  <span class="resaltar">Protocolo estándar angiotem miembros inferiores </span>.</div>
                            }
                        </div>
                    }
                </div>
            </div>
            <div id="div-detallado">
                <h3></h3>

                @*Molestias*@
                <div class="row">

                    <div class="col-md-12">
                        <div>
                            <div>

                                @*Molestias*@
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="panel panel-medico" data-collapsed="1">
                                            <div class="panel-heading">
                                                <h2 class="panel-title">Principales Molestias </h2>
                                                <div class="panel-options pull-right">
                                                    <a href="#" data-rel="collapse"><i class="entypo-down-open"></i></a>
                                                </div>
                                            </div>
                                            <div class="panel-body ">

                                                @*Pregunta 1*@
                                                <div class="form-group">
                                                    <div class="row">
                                                        <div class="col-lg-8">
                                                            <div class="form-group">
                                                                <div>
                                                                    <cite>1. Tiene diagnóstico?</cite><span class="asterisco">(*)</span>
                                                                    <label class="checkbox-inline">
                                                                        @Html.RadioButtonFor(model => model.p1, "true", new { @mostrar_respuesta = "rpta_p1" }) Si
                                                                    </label>
                                                                    <label class="checkbox-inline">
                                                                        @Html.RadioButtonFor(model => model.p1, "false", new { @ocultar_respuesta = "rpta_p1" }) No
                                                                    </label>
                                                                    <div class="col-md-12" id="rpta_p1">
                                                                        <div class="col-md-12">
                                                                            <div class="row">
                                                                                <div class="col-sm-5 col-lg-5">
                                                                                    @Html.DropDownListFor(model => model.p1_1, (SelectList)ViewBag.tiempo, "- Seleccione -", new { @class = "form-control" })
                                                                                </div>
                                                                                <div class="col-sm-2 col-lg-2">
                                                                                    @Html.TextBoxFor(model => model.p1_2, new { @placeholder = "Cant.", @type = "number", @min = "0", @class = "form-control" })
                                                                                </div>
                                                                            </div>
                                                                        </div>

                                                                        <div class="col-md-12">

                                                                            @Html.TextAreaFor(model => model.p1_3, new { @placeholder = "Ingrese el diagnóstico", @class = "form-control animate validate" })
                                                                        </div>
                                                                    </div>

                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                @*Pregunta 2*@
                                                <div class="form-group">
                                                    <div class="row">
                                                        <div class="col-lg-8">
                                                            <div class="form-group">
                                                                <div>
                                                                    <cite>2. Presenta alguna molestia?</cite><span class="asterisco">(*)</span>
                                                                    <label class="checkbox-inline">
                                                                        @Html.RadioButtonFor(model => model.p2, "true", new { @mostrar_respuesta = "rpta_p2" }) Si
                                                                    </label>
                                                                    <label class="checkbox-inline">
                                                                        @Html.RadioButtonFor(model => model.p2, "false", new { @ocultar_respuesta = "rpta_p2" }) No
                                                                    </label>


                                                                    <div class="col-md-12" id="rpta_p2">
                                                                        <div class="col-md-12">

                                                                            @Html.TextAreaFor(model => model.p2_1, new { @placeholder = "Ingrese la molestia ", @class = "form-control animate validate" })
                                                                        </div>
                                                                    </div>

                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                @*Procedimientos Relacionados*@
                <div class="row">

                    <div class="col-md-12">
                        <div>
                            <div>


                                <div class="row">

                                    <div class="col-md-12">

                                        <div class="panel panel-medico" data-collapsed="1">
                                            <div class="panel-heading">
