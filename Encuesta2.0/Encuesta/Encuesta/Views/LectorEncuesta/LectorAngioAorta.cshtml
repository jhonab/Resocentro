                                            <div class="panel-body ">
                                                @*Pregunta 3*@
                                                <div class="form-group">
                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <div class="form-group">
                                                                <div>
                                                                    <cite>3. EcoDoppler?</cite><span class="asterisco">(*)</span>
                                                                    <label class="checkbox-inline">
                                                                        @Html.RadioButtonFor(model => model.p3, "true", new { @mostrar_respuesta = "rpta_p3" }) Si
                                                                    </label>
                                                                    <label class="checkbox-inline">
                                                                        @Html.RadioButtonFor(model => model.p3, "false", new { @ocultar_respuesta = "rpta_p3" }) No
                                                                    </label>
                                                                    <div class="col-md-12" id="rpta_p3">
                                                                        <div class="col-md-12">
                                                                            @Html.TextAreaFor(model => model.p3_1, new { @placeholder = "Ingrese el resultado", @class = "form-control animate validate" })
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                @*Pregunta 4*@
                                                <div class="form-group">
                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <div class="form-group">
                                                                <div>
                                                                    <cite>4. Angiotem?</cite><span class="asterisco">(*)</span>
                                                                    <label class="checkbox-inline">
                                                                        @Html.RadioButtonFor(model => model.p4, "true", new { @mostrar_respuesta = "rpta_p4" }) Si
                                                                    </label>
                                                                    <label class="checkbox-inline">
                                                                        @Html.RadioButtonFor(model => model.p4, "false", new { @ocultar_respuesta = "rpta_p4" }) No
                                                                    </label>
                                                                    <div class="col-md-12" id="rpta_p4">
                                                                        <div class="col-md-12">
                                                                            @Html.TextAreaFor(model => model.p4_1, new { @placeholder = "Ingrese el resultado", @class = "form-control animate validate" })
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                @*Pregunta 5*@
                                                <div class="form-group">
                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <div class="form-group">
                                                                <div>
                                                                    <cite>5. Cirugías previas?</cite>

                                                                    <div class="col-md-12">
                                                                        <div class="col-md-12">
                                                                            @Html.TextAreaFor(model => model.p5_3, new { @placeholder = "Ingrese el motivo", @class = "form-control animate validate" })
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>

                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                @*Preguntas a Resolver*@
                <div class="row">

                    <div class="col-md-12">

                        <div>
                            <div>
                                <div class="row">

                                    <div class="col-md-12">

                                        <div class="panel panel-medico" data-collapsed="1">
                                            <div class="panel-heading">
                                                <h2 class="panel-title">Preguntas a Resolver </h2>
                                                <div class="panel-options pull-right">
                                                    <a href="#" data-rel="collapse"><i class="entypo-down-open"></i></a>
                                                </div>
                                            </div>
                                            <div class="panel-body ">
                                                @*Pregunta 6-A*@
                                                <div class="form-group">
                                                    <div class="row">
                                                        <div class="col-lg-8">
                                                            <div class="form-group">
                                                                <div>
                                                                    <cite>6-A. Ingresar lo que dice la orden médica?</cite><span class="asterisco">(*)</span>

                                                                    <div class="col-md-12">
                                                                        @Html.TextAreaFor(model => model.p7, new { @placeholder = "Ingrese lo que dice la orden médica", @class = "form-control animate validate" })
