                                                             <div class="col-lg-8">
                                                                 <div class="form-group">
                                                                     <div>
                                                                         <cite>7. Aneurisma?</cite>
                                                                         <div class="col-md-12">
                                                                             <div class="col-md-12">

                                                                                 @Html.TextAreaFor(model => model.p8, new { @placeholder = "Especifique", @class = "form-control animate validate" })
                                                                             </div>
                                                                         </div>

                                                                     </div>
                                                                 </div>
                                                             </div>
                                                         </div>
                                                     </div>
                                                    Pregunta 8
                                                     <div class="form-group">
                                                         <div class="row">
                                                             <div class="col-lg-8">
                                                                 <div class="form-group">
                                                                     <div>
                                                                         <cite>9. Disección?</cite>
                                                                         <div class="col-md-12">
                                                                             <div class="col-md-12">

                                                                                 @Html.TextAreaFor(model => model.p9, new { @placeholder = "Especifique", @class = "form-control animate validate" })
                                                                             </div>
                                                                         </div>

                                                                     </div>
                                                                 </div>
                                                             </div>
                                                         </div>
                                                     </div>
                                                     Pregunta 9
                                                     <div class="form-group">
