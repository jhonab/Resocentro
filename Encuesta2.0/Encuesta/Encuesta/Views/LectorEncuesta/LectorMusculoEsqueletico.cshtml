@model Encuesta.ViewModels.Encuesta.Tipos.EncuestaMusculoEsqueleticoViewModel

@{
    ViewBag.Titlenot = "Encuesta Músculo Esquelético";
    Encuesta.Util.Variable util = new Encuesta.Util.Variable();   
}

<script src="~/js/encuestas/LectorEncuesta.js"></script>
<script src="~/js/encuestas/encuestas_ini.js"></script>
<link href="~/css/Encuesta.css" rel="stylesheet" />
<script src="../js/jquery.autosize.js"></script>
<script src="~/js/jquery-ui.min.js"></script>
<div class="row body-medico" id="div_PreEntrevista">
    <div class="col-lg-12">
        <div class="box box-primary">
            <div class="box-header">
                <h1 class="box-title titulo_ocultar">
                    ENCUESTA MÚSCULO ESQUELETICO

                </h1>
            </div>
            <div class="box-body chart-responsive">
                @*Datos Examen*@
            <div class="row">

                <div class="col-lg-12">

                    <div id="Info-examen" class="panel panel-medico">
                        <div class="panel-heading">
                            <h3 class="panel-title">Datos Examen</h3>
                            <div class="panel-options pull-right">
                                <a href="#" data-rel="collapse"><i class="entypo-down-open"></i></a>
                            </div>
                        </div>
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-lg-12">

                                    <div class="col-sm-5 col-lg-5">
                                        @Html.Partial("_DatosExamenLector", Model._examen)
                                    </div>
                                    <div class="col-sm-5 col-lg-5">
                                        @Html.Partial("_DatosEncuestaLector", Model._encuesta)
                                    </div>
                                    <div class="col-sm-2 col-lg-2">
                                        <span>Adicional :</span>
                                        <div class="btn-group-vertical" role="group" style="margin-bottom: 10px;">

                                            @if (Model._examen.ATENCION.CITA.sedacion)
                                            {
                                                <button class="btn btn-danger box-tools  pull-right no-print">SEDACION</button>
                                            }
                                            @if (Model._examen.ATENCION.CITA.claustrofobico)
                                            {
                                                <button class="btn btn-info box-tools  pull-right no-print">CLAUSTROFOBICO</button>
                                            }

                                        </div>
                                        <div class="btn-group-vertical" role="group" id="div_adicional">
                                            <button class="btn btn-medico box-tools btn-sm pull-right no-print" id="btn-detalle" onclick="Mostrardetalle(false)">
                                                <i class="fa fa-plus fa-1x"></i>&nbsp;Detalle Encuesta
                                            </button>
                                            <button class="btn btn-medico box-tools btn-sm pull-right no-print" onclick="verHistorial('@Model._examen.codigo')">
                                                <i class="fa fa-history fa-1x"></i>&nbsp;Estudios Previos
                                            </button>

                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>


            </div>
            <div class="row" id="div_historial">
            </div>
            <div id="div-resumen">
                <div class="pregunta">
                    @*Pregunta 1*@
                    @if (Model.p1.Value)
                    {
                        <div class="col-lg-12">
                            - <span class="resaltar">Tiene diagnóstico </span>desde
                            <span class="resaltar">@Model.p1_2 @util.getNombreTiempoRM(Model.p1_1)</span>,
                            <span class="resaltar">@Model.p1_3</span>
                            .
                        </div>
                    }
                    @*Pregunta 2*@
                    @if (Model.p2.Value)
                    {
                        <div class="col-lg-12">
                            - <span class="resaltar">Tiene molestias </span>:
                            <span class="resaltar">@Model.p2_1</span>
                            .
                        </div>
                    }

                    @*PREGUNTA 3*@
                    <div class="col-lg-12">
                        - Lo noto hace <span class="resaltar">@Model.p3_1 @util.getNombreTiempoRM(int.Parse(Model.p3_2))</span>.
                    </div>
                    @*PREGUNTA 4*@
                    <div class="col-lg-12">
                        - Las molestias iniciaron
                        <span class="resaltar">
                            @if (Model.p4 == 1)
                            {

                                <text>por esfuerzo,</text> <span>@Model.p4_1</span>

                            }
                            else if (Model.p4 == 2)
                            {

                                <text>por traumatismo,</text> <span>@Model.p4_1</span>

                            }
                            else if (Model.p4 == 3)
                            {

                                <text> sin nada en especial</text>

                            }
                            else if (Model.p4 == 4)
                            {

                                <text>por,</text> <span>@Model.p4_1</span>

                            }
                            else { }
                        </span>.
                    </div>
                    @*PREGUNTA 5*@
                    <div class="col-lg-12">
                        -
                        @if (Model.p5.Value)
                        {
                            <span class="resaltar">Comenzó súbitamente</span><span>(de un momento a otro),</span><span class="resaltar"> @Model.p4_1.</span>
                        }
                        else
                        {
                            <span class="resaltar">Comenzó gradualmente</span><span>(poco a poco).</span>
                        }
                    </div>
                    @*PREGUNTA 6*@
                    @if (Model.p6 != null)
                    {
                        if (Model.p6 != "")
                        {
                            <div class="col-lg-12">
                                - Tiene antecedentes traumatológicos:
                                <span class="resaltar">@Model.p6.</span>
                            </div>
                        }
                    }
                    @*Pregunta 7*@
                    @if (Model.p7.Value)
                    {
                        <div class="col-lg-12">
                            - <span class="resaltar">Tiene cirugias </span> hace
                            <span class="resaltar">@Model.p7_2 @util.getNombreTiempoRM(Model.p7_1)</span>,
                            <span class="resaltar">@Model.p7_3</span>
                            .
                        </div>
                    }
                    @*Pregunta 8*@
                    @if (Model.p8.Value)
                    {
                        <div class="col-lg-12">
                            - <span class="resaltar">Tiene inmovilización </span> desde
                            <span class="resaltar">@Model.p8_1_1 @util.getNombreTiempoRM(int.Parse(Model.p8_1))</span>,hasta
                            <span class="resaltar">@Model.p8_2_1 @util.getNombreTiempoRM(int.Parse(Model.p8_2))</span>
                            .
                        </div>
                    }
                    @*Pregunta 9*@
                    @if (Model.p9.Value)
                    {
                        <div class="col-lg-12">
                            - <span class="resaltar">Tiene estudios </span> previos en
                            <span class="resaltar">@Model.p8_1_1 @util.getNombreLocales(Model.p9_1)</span>
                            .
                        </div>
                    }


                    @*PREGUNTA 10*@
                    @if (Model.p10_1 == false)
                    {
                        <div class="col-lg-12">- Orden médica menciona <span class="resaltar">@Model.p10</span> .</div>
                    }
                    @if (Model.p11_1 == false)
                    {
                        <div class="col-lg-12">- El paciente comenta <span class="resaltar">@Model.p11</span> .</div>
                    }

                    @*PREGUNTA 11*@
                    @if (Model.p12 != "")
                    {
                        <div class="col-lg-12">- Se desea resolver el o los siguientes problemas diagnosticados <span class="resaltar">@Model.p12</span> .</div>
                    }
                    @*PREGUNTA 12*@
                    @if (Model.p13 != "")
                    {
                        <div class="col-lg-12">- Se realizo el examen de: <span class="resaltar">@Model.p13</span> .</div>
                    }
                    @*PREGUNTA 13*@
                    @if (Model.p14 == 1)
                    {
                        <div class="col-lg-12">-  <span class="resaltar">Se indicó </span>que se use contraste.</div>
                    }
                    @if (Model.p14 == 2)
                    {
                        <div class="col-lg-12">- Se indicó  que <span class="resaltar">no </span>se use contraste.</div>
                    }
                    @if (Model.p14 == 3)
                    {
                        <div class="col-lg-12">- Se indicó usar contraste  <span class="resaltar">de acuerdo a hallazgos</span>.</div>
                    }




                </div>
            </div>
            <div id="div-detallado">
                <h3></h3>
                @*Molestias*@
                <div class="row">

                    <div class="col-md-12">
                        <div class=" ">
                            <div class="">

                                @*Molestias*@
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="panel panel-medico" data-collapsed="1">
                                            <div class="panel-heading">
                                                <div class="panel-title">Principales Molestias </div>
                                                <div class="panel-options pull-right">
                                                    <a href="#" data-rel="collapse"><i class="entypo-down-open"></i></a>
                                                </div>
                                            </div>
                                            <div class="panel-body chart-responsive">

                                                @*Pregunta 1*@
                                                <div class="form-group">
                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <div class="form-group">
                                                                <div>
                                                                    <cite>1. Tiene diagnóstico?</cite><span class="asterisco">(*)</span>
                                                                    <label class="checkbox-inline">
                                                                        @Html.RadioButtonFor(model => model.p1, "true", new { @mostrar_respuesta = "rpta_p1" }) Si
                                                                    </label>
                                                                    <label class="checkbox-inline">
                                                                        @Html.RadioButtonFor(model => model.p1, "false", new { @ocultar_respuesta = "rpta_p1" }) No
                                                                    </label>
                                                                    <div class="col-md-12" id="rpta_p1">
                                                                        <div class="col-md-12">
                                                                            <div class="row">
                                                                                <div class="col-sm-5 col-lg-5">
                                                                                    @Html.DropDownListFor(model => model.p1_1, (SelectList)ViewBag.tiempo, "- Seleccione -", new { @class = "form-control" })
                                                                                </div>
                                                                                <div class="col-sm-2 col-lg-2">
                                                                                    @Html.TextBoxFor(model => model.p1_2, new { @placeholder = "Año", @type = "number", @min = "0", @class = "form-control" })
                                                                                </div>
                                                                            </div>
                                                                        </div>

                                                                        <div class="col-md-12">

                                                                            @Html.TextAreaFor(model => model.p1_3, new { @placeholder = "Ingrese el diagnóstico", @class = "form-control animate validate" })
                                                                        </div>
                                                                    </div>

                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                @*Pregunta 2*@
                                                <div class="form-group">
                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <div class="form-group">
                                                                <div>
                                                                    <cite>2. Presenta alguna molestia?</cite><span class="asterisco">(*)</span>
                                                                    <label class="checkbox-inline">
                                                                        @Html.RadioButtonFor(model => model.p2, "true", new { @mostrar_respuesta = "rpta_p2" }) Si
                                                                    </label>
                                                                    <label class="checkbox-inline">
                                                                        @Html.RadioButtonFor(model => model.p2, "false", new { @ocultar_respuesta = "rpta_p2" }) No
                                                                    </label>


                                                                    <div class="col-md-12" id="rpta_p2">
                                                                        <div class="col-md-12">

                                                                            @Html.TextAreaFor(model => model.p2_1, new { @placeholder = "Ingrese el diagnostico", @class = "form-control animate validate" })
                                                                        </div>
                                                                    </div>

                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>

                                            </div>
                                        </div>
                                    </div>
                                </div>
                                @*Historial Molestias*@

                                <div class="row">

                                    <div class="col-md-12">

                                        <div class="panel panel-medico" data-collapsed="1">
                                            <div class="panel-heading">
                                                <div class="panel-title">Historia de Molestias </div>
                                                <div class="panel-options pull-right">
                                                    <a href="#" data-rel="collapse"><i class="entypo-down-open"></i></a>
                                                </div>
                                            </div>
                                            <div class="panel-body chart-responsive">

                                                @*Pregunta 3*@
                                                <div class="form-group">
                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <div class="form-group">
                                                                <div>
                                                                    <cite>3. Desde hace cuánto lo ha notado por primera vez?</cite><span class="asterisco">(*)</span>
                                                                    <div class="col-md-12">
                                                                        <div class="row">
                                                                            <div class="col-sm-2 col-lg-2">
                                                                                @Html.TextBoxFor(model => model.p3_1, new { @placeholder = "Cant.", @type = "number", @min = "0", @class = "form-control" })
                                                                            </div>
                                                                            <div class="col-sm-4 col-lg-4">
                                                                                @Html.DropDownListFor(model => model.p3_2, (SelectList)ViewBag.tiempo, "- Seleccione -", new { @class = "form-control" })
                                                                            </div>
                                                                        </div>
                                                                    </div>

                                                                </div>

                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                @*Pregunta 4*@
                                                <div class="form-group">
                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <div class="form-group">
                                                                <div>
                                                                    <cite>4. Su inicio se asocio a:</cite><span class="asterisco">(*)</span>
                                                                    <div class="col-md-12">

                                                                        <div class="radio">
                                                                            <label class="checkbox-inline">
                                                                                @Html.RadioButtonFor(model => model.p4, "1", new { @mostrar_respuesta = "p4_1" }) Esfuerzo
                                                                            </label>
                                                                        </div>

                                                                        <div class="radio">
                                                                            <label class="checkbox-inline">
                                                                                @Html.RadioButtonFor(model => model.p4, "2", new { @mostrar_respuesta = "p4_1" }) Traumatismo
                                                                            </label>
                                                                        </div>
                                                                        <div class="radio">
                                                                            <label class="checkbox-inline">
                                                                                @Html.RadioButtonFor(model => model.p4, "3", new { @ocultar_respuesta = "p4_1" }) A nada en especial
                                                                            </label>
                                                                        </div>

                                                                        <div class="radio">
                                                                            <label class="checkbox-inline">
                                                                                @Html.RadioButtonFor(model => model.p4, "4", new { @mostrar_respuesta = "p4_1" }) Otros
                                                                            </label>
                                                                        </div>
                                                                        @Html.TextAreaFor(model => model.p4_1, new { @placeholder = "Especifique", @class = "form-control animate validate" })
                                                                    </div>


                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                @*Pregunta 5*@
                                                <div class="form-group">
                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <div class="form-group">
                                                                <div>
                                                                    <cite>5. Cómo comenzaron sus molestias?</cite><span class="asterisco">(*)</span>
                                                                    <div class="col-md-12">

                                                                        <div class="radio">
                                                                            <label class="checkbox-inline">
                                                                                @Html.RadioButtonFor(model => model.p5, "true", new { @mostrar_respuesta = "p5_1" }) Subitamente(de un momento a otro)
                                                                            </label>
                                                                            @Html.TextAreaFor(model => model.p5_1, new { @placeholder = "Especifique", @class = "form-control animate validate" })
                                                                        </div>

                                                                        <div class="radio">
                                                                            <label class="checkbox-inline">
                                                                                @Html.RadioButtonFor(model => model.p5, "false", new { @ocultar_respuesta = "p5_1" })Gradualmente(poco a poco)
                                                                            </label>
                                                                        </div>
                                                                    </div>


                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                @*Pregunta 6*@
                                                <div class="form-group">
                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <div class="form-group">
                                                                <cite>6. Antecedente de traumatismo?</cite>
                                                                <div class="col-md-12">
                                                                    @Html.TextAreaFor(model => model.p6, new { @placeholder = "Especifique", @class = "form-control animate validate" })

                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>


                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                @*Procedimientos Relacionados*@
                <div class="row">

                    <div class="col-md-12">
                        <div class=" ">
                            <div class="">
                                <div class="row">

                                    <div class="col-md-12">

                                        <div class="panel panel-medico" data-collapsed="1">
                                            <div class="panel-heading">
                                                <div class="panel-title">Procedimientos Relacionados </div>
                                                <div class="panel-options pull-right">
                                                    <a href="#" data-rel="collapse"><i class="entypo-down-open"></i></a>
                                                </div>
                                            </div>
                                            <div class="panel-body chart-responsive">
                                                @*Pregunta 7*@
                                                <div class="form-group">
                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <div class="form-group">
                                                                <div>
                                                                    <cite>7. Ha tenido una cirugia antes?</cite><span class="asterisco">(*)</span>
                                                                    <label class="checkbox-inline">
                                                                        @Html.RadioButtonFor(model => model.p7, "true", new { @mostrar_respuesta = "rpta_p7" }) Si
                                                                    </label>
                                                                    <label class="checkbox-inline">
                                                                        @Html.RadioButtonFor(model => model.p7, "false", new { @ocultar_respuesta = "rpta_p7" }) No
                                                                    </label>
                                                                    <div class="col-md-12" id="rpta_p7">
                                                                        <div class="col-md-12">
                                                                            <div class="row">
                                                                                <div class="col-sm-5 col-lg-5">
                                                                                    @Html.DropDownListFor(model => model.p7_1, (SelectList)ViewBag.tiempo, "- Seleccione -", new { @class = "form-control" })
                                                                                </div>
                                                                                <div class="col-sm-2 col-lg-2">
                                                                                    @Html.TextBoxFor(model => model.p7_2, new { @placeholder = "Año", @type = "number", @min = "0", @class = "form-control" })
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                        <div class="col-md-12">
                                                                            @Html.TextAreaFor(model => model.p7_3, new { @placeholder = "Ingrese cuales fueron", @class = "form-control animate validate" })
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                @*Pregunta 8*@
                                                <div class="form-group">
                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <div class="form-group">
                                                                <div>
                                                                    <cite>8. Tiene alguna inmovilización?</cite><span class="asterisco">(*)</span>
                                                                    <label class="checkbox-inline">
                                                                        @Html.RadioButtonFor(model => model.p8, "true", new { @mostrar_respuesta = "rpta_p8" }) Si
                                                                    </label>
                                                                    <label class="checkbox-inline">
                                                                        @Html.RadioButtonFor(model => model.p8, "false", new { @ocultar_respuesta = "rpta_p8" }) No
                                                                    </label>
                                                                    <div class="col-md-12" id="rpta_p8">
                                                                        <div class="col-md-12">
                                                                            <cite>Desde cuándo?</cite>
                                                                            <div class="row">
                                                                                <div class="col-sm-5 col-lg-5">
                                                                                    @Html.DropDownListFor(model => model.p8_1, (SelectList)ViewBag.tiempo, "- Seleccione -", new { @class = "form-control" })
                                                                                </div>
                                                                                <div class="col-sm-2 col-lg-2">
                                                                                    @Html.TextBoxFor(model => model.p8_1_1, new { @placeholder = "Año", @type = "number", @min = "0", @class = "form-control" })
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                        <div class="col-md-12">
                                                                            <cite>hasta cuándo?</cite>
                                                                            <div class="row">
                                                                                <div class="col-sm-5 col-lg-5">
                                                                                    @Html.DropDownListFor(model => model.p8_2, (SelectList)ViewBag.tiempo, "- Seleccione -", new { @class = "form-control" })
                                                                                </div>
                                                                                <div class="col-sm-2 col-lg-2">
                                                                                    @Html.TextBoxFor(model => model.p8_2_1, new { @placeholder = "Año", @type = "number", @min = "0", @class = "form-control" })
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                @*Pregunta 9*@
                                                <div class="form-group">
                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <div class="form-group">
                                                                <div>
                                                                    <cite>9. Tiene estudios previos de resonancia o tomografía?</cite><span class="asterisco">(*)</span>
                                                                    <label class="checkbox-inline">
                                                                        @Html.RadioButtonFor(model => model.p9, "true", new { @mostrar_respuesta = "rpta_p4" }) Si
                                                                    </label>
                                                                    <label class="checkbox-inline">
                                                                        @Html.RadioButtonFor(model => model.p9, "false", new { @ocultar_respuesta = "rpta_p4" }) No
                                                                    </label>
                                                                    <div class="col-md-12" id="rpta_p4">
                                                                        <div class="col-md-12">
                                                                            <cite>El último donde?</cite><span class="asterisco">(*)</span>
                                                                            <div class="col-md-12">
                                                                                <div class="row">
                                                                                    <div class="col-sm-4 col-lg-4">
                                                                                        @Html.DropDownListFor(model => model.p9_1, (SelectList)ViewBag.locales, "- Seleccione -", new { @class = "form-control" })
                                                                                    </div>
                                                                                    <div class="col-sm-12 col-md-12">
                                                                                        <div class="row">
                                                                                            <div class="col-md-12">
                                                                                                <cite>Compromiso del paciente a traer el estudio en <strong><em>MENOS DE 12 HORAS</em></strong>:</cite><span class="asterisco">(*)</span>
                                                                                                <div class="radio">
                                                                                                    <label>
                                                                                                        @Html.RadioButtonFor(model => model.p9_3, "true") Si
                                                                                                    </label>
                                                                                                </div>
                                                                                                <div class="radio">
                                                                                                    <label>
                                                                                                        @Html.RadioButtonFor(model => model.p9_3, "false")no
                                                                                                    </label>
                                                                                                </div>
                                                                                            </div>
                                                                                        </div>
                                                                                    </div>
                                                                                </div>

                                                                            </div>

                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                @*Preguntas a Resolver*@
                <div class="row">

                    <div class="col-md-12">

                        <div class=" ">
                            <div class="">
                                <div class="row">

                                    <div class="col-md-12">

                                        <div class="panel panel-medico" data-collapsed="1">
