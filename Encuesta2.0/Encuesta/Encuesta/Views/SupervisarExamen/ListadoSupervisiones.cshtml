                                    @if (!item.p1 == null)
                                    {
                                        <span><i class="fa fa-minus fa-2x alert-info"></i></span>
                                    }
                                    else
                                    {
                                        if (item.p1.Value)
                                        {
                                            <i class="fa fa-check fa-2x alert-success"></i>
                                        }
                                        else
                                        {
                                            <span class="alert-danger">
                                                @Html.DisplayFor(modelItem => item.p1_motivo)
                                            </span>
                                        }
                                    }
                                </td>
                                <td class="text-center hor-centro">
                                    @if (!item.p2 == null)
