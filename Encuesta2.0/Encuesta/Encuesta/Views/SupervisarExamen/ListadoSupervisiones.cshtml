                                        }
                                        else
                                        {
                                            <span class="alert-danger">
                                                @Html.DisplayFor(modelItem => item.p3_inf_motivo)
                                            </span>
                                        }
                                    }
                                </td>
                                <td class="text-center hor-centro">
                                    @if (!item.dx_pre_inf == null)
                                    {
                                        <span><i class="fa fa-minus fa-2x alert-info"></i></span>
                                    }
                                    else
                                    {
                                        if (item.dx_pre_inf.Value)
                                        {
                                            <i class="fa fa-check fa-2x alert-success"></i>
                                        }
                                        else
                                        {
                                            <i class="fa fa-times fa-2x alert-danger"></i>
                                        }
                                    }
                                </td>
                                <td class="text-center hor-centro">
                                    @Html.DisplayFor(modelItem => item.informante)
                                </td>




                                <td class="text-center hor-centro">
                                    @if (!item.p1_val == null)
                                    {
                                        <span><i class="fa fa-minus fa-2x alert-info"></i></span>
                                    }
                                    else
                                    {
                                        if (item.p1_val.Value)
                                        {
                                            <i class="fa fa-check fa-2x alert-success"></i>
                                        }
                                        else
                                        {
                                            <span class="alert-danger">
                                                @Html.DisplayFor(modelItem => item.p1_val_motivo)
                                            </span>
                                        }
                                    }
                                </td>
                                <td class="text-center hor-centro">
                                    @if (!item.p2_val == null)
                                    {
                                        <span><i class="fa fa-minus fa-2x alert-info"></i></span>
                                    }
                                    else
                                    {
                                        if (item.p2_val.Value)
                                        {
                                            <i class="fa fa-check fa-2x alert-success"></i>
                                        }
                                        else
                                        {
                                            <span class="alert-danger">
                                                @Html.DisplayFor(modelItem => item.p2_val_motivo)
                                            </span>
                                        }
                                    }
                                </td>
                                <td class="text-center hor-centro">
                                    @if (!item.p3_val == null)
                                    {
                                        <span><i class="fa fa-minus fa-2x alert-info"></i></span>
                                    }
                                    else
                                    {
                                        if (item.p3_val.Value)
                                        {
                                            <i class="fa fa-check fa-2x alert-success"></i>
                                        }
                                        else
                                        {
                                            <span class="alert-danger">
                                                @Html.DisplayFor(modelItem => item.p3_val_motivo)
                                            </span>
                                        }
                                    }
                                </td>
                                <td class="text-center hor-centro">
                                    @if (!item.dx_pre_val == null)
                                    {
                                        <span><i class="fa fa-minus fa-2x alert-info"></i></span>
                                    }
                                    else
                                    {
                                        if (item.dx_pre_val.Value)
                                        {
                                            <i class="fa fa-check fa-2x alert-success"></i>
                                        }
                                        else
                                        {
