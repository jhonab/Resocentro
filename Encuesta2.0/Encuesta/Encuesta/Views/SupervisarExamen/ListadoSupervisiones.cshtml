                                        }
                                    }
                                </td>
                                <td class="text-center hor-centro">
                                    @if (!item.p3 == null)
                                    {
                                        <span><i class="fa fa-minus fa-2x alert-info"></i></span>
                                    }
                                    else
                                    {
                                        if (item.p3.Value)
                                        {
                                            <i class="fa fa-check fa-2x alert-success"></i>
                                        }
                                        else
                                        {
                                            <span class="alert-danger">
                                                @Html.DisplayFor(modelItem => item.p3_motivo)
                                            </span>
                                        }
                                    }
                                </td>
                                <td class="text-center hor-centro">
                                    @if (item.dx_pre != "")
                                    {
                                        <span class="alert-danger">
                                            @Html.DisplayFor(modelItem => item.dx_pre)
                                        </span>
                                    }
                                </td>
                                <td class="text-center hor-centro">
                                    @Html.DisplayFor(modelItem => item.supervisor)
                                </td>




                                <td class="text-center hor-centro">
                                    @if (!item.p1_inf == null)
                                    {
                                        <span><i class="fa fa-minus fa-2x alert-info"></i></span>
