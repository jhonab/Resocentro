                            <th class="bg-yellow hor-centro text-center">
                                Informante
                            </th>

                            <th class="bg-green hor-centro text-center">
                                Encuesta
                            </th>
                            <th class="bg-green hor-centro text-center">
                                Imagen
                            </th>
                            <th class="bg-green hor-centro text-center">
                                Objetivo Clínico
                            </th>
                            <th class="bg-green hor-centro text-center">
                                Dx
                            </th>
                            <th class="bg-green hor-centro text-center">
                                Validador
                            </th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td class=" hor-centro">
                                    @item.fecha_registro.ToShortDateString()
                                </td>
                                <td class=" hor-centro">
                                    @Html.DisplayFor(modelItem => item.numeroexamen)
                                </td>
                                <td class=" hor-centro">
                                    @Html.DisplayFor(modelItem => item.encuestador)
                                </td>
                                <td class=" hor-centro">
                                    @Html.DisplayFor(modelItem => item.tecnologo)
                                </td>
