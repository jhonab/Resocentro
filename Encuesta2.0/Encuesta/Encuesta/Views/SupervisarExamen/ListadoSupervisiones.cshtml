</style>
<script type="text/javascript">
    $(function () {
        $('.table').DataTable({});
    });
    function cargarreporte() {
        var inicio = $("#inicio").val();
        var fin = $("#inicio").val();
        window.location.href = ('@Url.Action("ListadoSupervisiones", "SupervisarExamen")' + '?inicio=' + inicio+"&fin="+fin);
    }
</script><div class="row">
    <div class="col-md-12">
