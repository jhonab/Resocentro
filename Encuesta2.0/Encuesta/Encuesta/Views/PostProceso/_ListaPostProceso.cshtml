@model IEnumerable<Encuesta.Models.POSTPROCESO>
<script type="text/javascript">

    $(document).ready(function () {
        $('#table_tecnicas').dataTable({
            "columnDefs": [
            { "targets": [3], "visible": false, "searchable": false },
            { "targets": [2], "searchable": false, }

            ]
        });
    });

</script>

<table class="table table-striped table-bordered table-hover" id="table_tecnicas">
    <thead>
        <tr>
            <th>Técnica</th>
            <th>Cant.</th>
            <th></th>
            <th>codigo</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td style="padding: 1px!important">@Html.DisplayFor(modelItem => item.descripcion)</td>
                <td style="padding: 1px!important">@Html.DisplayFor(modelItem => item.cantidadplaca)</td>
                <td class="table_id" style="padding: 1px!important">
                    <button type="button" class="btn btn-danger btn-circle" onclick="eliminarTecnica(@item.codigopostproceso)"><i class="fa fa-times"></i></button>
                </td>

                <td style="padding: 1px!important">@Html.DisplayFor(modelItem => item.codigopostproceso)</td>
            </tr>
        }

    </tbody>
</table>
