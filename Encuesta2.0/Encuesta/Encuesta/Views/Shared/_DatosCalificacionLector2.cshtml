                    $.ajax({
                        async: false,
                        cache: false,
                        url: '@Url.Action("VerificarCalificacion", "SupervisarExamen")',
                        data: { examen: examen, tipo: 1 },
                        dataType: "json",
                        type: "POST",
                        success: function (data) {
                            if (data.result.trim() == "0") {
                                xxx = "Médico Informante\n Falta Terminar de Calificar el Examen";
                            }
                        },
                        error: function () {
                            xxx = 'False';
                        }
                    });
                } else {

                    $.ajax({
                        async: false,
                        cache: false,
                        url: '@Url.Action("VerificarCalificacion", "SupervisarExamen")',
                        data: { examen: examen, tipo: 2 },
                        dataType: "json",
                        type: "POST",
                        success: function (data) {
                            if (data.result.trim() == "0") {
                                xxx = "Médico Validador\n Falta Terminar de Calificar el Examen";
                            }
                        },
                        error: function () {
                            xxx = 'False';
                        }
                    });
