                                                                            <label>
                                                                                @Html.CheckBoxFor(model => model.p1_5) Rotación Interna
                                                                            </label>
                                                                        </div>
                                                                        <div class="checkbox">
                                                                            <label>
                                                                                @Html.CheckBoxFor(model => model.p1_6) Rotación Externa
                                                                            </label>
                                                                        </div>
