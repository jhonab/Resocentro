                                                                                <div class="col-md-12">
                                                                                    <cite>El último donde?</cite><span class="asterisco">(*)</span>
                                                                                    <div class="col-md-12">
                                                                                        <div class="row">
                                                                                            <div class="col-sm-4 col-lg-4">
                                                                                                @Html.DropDownListFor(model => model.pro_p2_1, (SelectList)ViewBag.locales, "- Seleccione -", new { @class = "form-control", @mostrar_respuesta = "rpta_p9_1", @mostrar_respuesta_value = "9" })
