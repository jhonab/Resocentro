@model Encuesta.ViewModels.Encuesta.Tipos.EncuestaColumnaAViewModel

@{
    ViewBag.Title = "<i class='fa fa-edit'></i> Encuesta";
    ViewBag.Titlenot = "Encuesta Columna";
}

<script src="~/js/encuestas/validacion/RM/columnaA_val.js"></script>
<script src="~/js/encuestas/encuestas_ini.js"></script>
<link href="~/css/Encuesta.css" rel="stylesheet" />
<script src="../js/jquery.autosize.js"></script>
<script src="~/js/jquery-ui.min.js"></script>
<script type="text/javascript">
    $(document).ready(function () {
        $('.animate').autosize();
    });
</script>
<div class="row" id="div_PreEntrevista">
    <div class="col-md-12">
        <div class="">
            <div class="">
                @{string tipo_encuesta = "";
                  if (Model.tipo_encu == 6)
                  { tipo_encuesta = "columna cervical"; }
                  else if (Model.tipo_encu == 7)
                  { tipo_encuesta = "columna cervico-dorsal"; }
                  else
                  { tipo_encuesta = ""; } }
                <h1 class=" titulo_ocultar">ENCUESTA @tipo_encuesta.ToUpper()

                </h1>
            </div>
            <div class="chart-responsive">@*Datos Examen*@
                 <div class="row">

                    <div class="col-md-12">

                        <div class="panel panel-dark" data-collapsed="0">
                            <div class="panel-heading">
                                <h3 class="panel-title">Datos Examen</h3>
                                <div class="box-tools pull-right">
                                    <div class="panel-options pull-right">
                                        <a href="#" data-rel="collapse"><i class="entypo-down-open"></i></a>
                                    </div>
                                </div>
                            </div>
                            <div class="panel-body">
                                <div class="row">
                                    <div class="col-md-12">

                                        <div class="col-xs-5 col-sm-5 col-lg-5">
                                            @Html.Partial("_DatosExamen", Model._examen)
                                        </div>
                                        <div class="col-xs-5 col-sm-5 col-lg-5">
                                            @Html.Partial("_DatosPaciente", Model._paciente)
                                        </div>

                                        <div class="col-xs-2 col-sm-2 col-lg-2">
                                            <div class="btn-group-vertical">
                                                <button class="btn btn-primary box-tools pull-right no-print" onclick="cambiarEncuesta()">
                                                    <i class="fa fa-undo fa-2x"></i>
                                                </button>
                                                <button class="btn btn-info box-tools pull-right no-print" onclick="verHistorial('@Model._examen.codigo')" >
                                                    <i class="fa fa-history fa-2x"></i>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>


                    </div>
                </div>
                <div class="row" id="div_historial">                    
                </div>
                @using (Html.BeginForm(null, null, FormMethod.Post, new { id = "frm_encuesta" }))
                {  
                    @Html.AntiForgeryToken()
                    @Html.ValidationSummary(true, "", new { @class="alert alert-info"})

                    @Html.HiddenFor(y => y.numeroexamen);
                    @Html.HiddenFor(y => y.equipoAsignado);
@*Molestias*@
                    <div class="row">

                        <div class="col-md-12">
                            <div class="">
                             
                                <div class="">

                                    @*Molestias*@
                                    <div class="row">

                                        <div class="col-md-12">

                                            <div class="panel panel-dark" data-collapsed="0">
                                                <div class="panel-heading">
                                                    <div class="panel-title">Principales Molestias </div>
                                                    <div class="panel-options pull-right">
                                                        <a href="#" data-rel="collapse"><i class="entypo-down-open"></i></a>
                                                    </div>
                                                </div>
                                                <div class="panel-body chart-responsive">

                                                    @*Pregunta 1*@
                                                    <div class="form-group">
                                                        <div class="row">
                                                            <div class="col-md-12">
                                                                <div class="form-group">

                                                                    <cite>1. Presenta dolor en la @tipo_encuesta?</cite><span class="asterisco">(*)</span>
                                                                    <label class="checkbox-inline">
                                                                        @Html.RadioButtonFor(model => model.mol_p1, "true", new { @mostrar_respuesta = "rpta_p1" }) Si
                                                                    </label>
                                                                    <label class="checkbox-inline">
                                                                        @Html.RadioButtonFor(model => model.mol_p1, "false", new { @ocultar_respuesta = "rpta_p1" }) No
                                                                    </label>


                                                                    <div class="col-md-12" id="rpta_p1">
                                                                        <div class="row">
                                                                            <div class="col-md-12">
                                                                                <cite>Donde?</cite>
                                                                                <div class="checkbox">
                                                                                    <label>
                                                                                        @Html.CheckBoxFor(model => model.mol_p1_1) En cuello, en la base del cuello o en la nuca
                                                                                    </label>
                                                                                </div>
                                                                                <div class="col-sm-6 col-lg-6">
                                                                                    - Derecha
                                                                                    <div class="checkbox">
                                                                                        <label>
                                                                                            @Html.CheckBoxFor(model => model.mol_p1_3) Hombro derecho
                                                                                        </label>
                                                                                    </div>
                                                                                    <div class="checkbox">
                                                                                        <label>
                                                                                            @Html.CheckBoxFor(model => model.mol_p1_5) Brazo derecho
                                                                                        </label>
                                                                                    </div>
                                                                                    <div class="checkbox">
                                                                                        <label>
                                                                                            @Html.CheckBoxFor(model => model.mol_p1_7) Antebrazo derecho
                                                                                        </label>
                                                                                    </div>
                                                                                    <div class="checkbox">
                                                                                        <label>
                                                                                            @Html.CheckBoxFor(model => model.mol_p1_9) Mano derecha
                                                                                        </label>
                                                                                    </div>
                                                                                </div>
                                                                                <div class="col-sm-6 col-lg-6">
                                                                                    - Izquierda
                                                                                    <div class="checkbox">
                                                                                        <label>
                                                                                            @Html.CheckBoxFor(model => model.mol_p1_2) Hombro izquierdo
                                                                                        </label>
                                                                                    </div>
                                                                                    <div class="checkbox">
                                                                                        <label>
                                                                                            @Html.CheckBoxFor(model => model.mol_p1_4) Brazo izquierdo
                                                                                        </label>
                                                                                    </div>
                                                                                    <div class="checkbox">
                                                                                        <label>
                                                                                            @Html.CheckBoxFor(model => model.mol_p1_6) Antebrazo izquierdo
                                                                                        </label>
                                                                                    </div>
                                                                                    <div class="checkbox">
                                                                                        <label>
                                                                                            @Html.CheckBoxFor(model => model.mol_p1_8) Mano izquierda
                                                                                        </label>
                                                                                    </div>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    @*Pregunta 2*@
                                                    <div class="form-group">
                                                        <div class="row">
                                                            <div class="col-md-12">
                                                                <div class="form-group">
                                                                    <div>
                                                                        <cite>2. Disminución o pérdida de fuerza u otras sensaciones?</cite><span class="asterisco">(*)</span>
                                                                        <label class="checkbox-inline">
                                                                            @Html.RadioButtonFor(model => model.mol_p2, "true", new { @mostrar_respuesta = "rpta_p2" }) Si
                                                                        </label>
                                                                        <label class="checkbox-inline">
                                                                            @Html.RadioButtonFor(model => model.mol_p2, "false", new { @ocultar_respuesta = "rpta_p2" }) No
                                                                        </label>
                                                                        <div class="col-md-12" id="rpta_p2">

                                                                            <div class="col-md-12">
                                                                                <cite>- Disminución o pérdida completa de la fuerza en miembro superior?</cite>
                                                                                <div class="col-md-12">
                                                                                    <label class="checkbox-inline">
                                                                                        @Html.CheckBoxFor(model => model.mol_p2_2) Derecha
                                                                                    </label>
                                                                                    <label class="checkbox-inline">
                                                                                        @Html.CheckBoxFor(model => model.mol_p2_1) Izquierda
                                                                                    </label>
                                                                                </div>
                                                                            </div>

                                                                            <div class="col-md-12">
                                                                                <cite>- Sensación de adormecimiento, entumecimiento u hormigueo en el miembro superior?</cite>
                                                                                <div class="col-md-12">
                                                                                    <label class="checkbox-inline">
                                                                                        @Html.CheckBoxFor(model => model.mol_p2_4) Derecha
                                                                                    </label>

                                                                                    <label class="checkbox-inline">
                                                                                        @Html.CheckBoxFor(model => model.mol_p2_3) Izquierda
                                                                                    </label>
                                                                                </div>
                                                                            </div>

                                                                            <div class="col-md-12">
                                                                                <div class="checkbox">
                                                                                    <label>
                                                                                        @Html.CheckBoxFor(model => model.mol_p2_5, new { @mostrar_respuesta = "mol_p2_5_1" }) Sensación de anestesia en alguna parte del cuerpo
                                                                                    </label>
                                                                                </div>
                                                                                @Html.TextAreaFor(model => model.mol_p2_5_1, new { @placeholder = "Especifique donde", @class = "form-control animate validate" })
                                                                                <div class="checkbox">
                                                                                    <label>
                                                                                        @Html.CheckBoxFor(model => model.mol_p2_6, new { @mostrar_respuesta = "mol_p2_6_1" }) Otros                                                                               
                                                                                    </label>
                                                                                </div>
                                                                                @Html.TextAreaFor(model => model.mol_p2_6_1, new { @placeholder = "Especifique donde", @class = "form-control animate validate" })
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    @*Pregunta 3*@
                                                    <div class="form-group">
                                                        <div class="row">
                                                            <div class="col-md-12">
                                                                <div class="form-group">
                                                                    <div>
                                                                        <cite>3. Otros?</cite>
                                                                        <div class="col-md-12">
                                                                            <div class="col-md-12">
                                                                                <div class="checkbox">
                                                                                    <label>
                                                                                        @Html.CheckBoxFor(model => model.mol_p3_1) Mareos asociados a los movimientos de la cabeza
                                                                                    </label>
                                                                                </div>
                                                                                <div class="checkbox">
                                                                                    <label>
                                                                                        @Html.CheckBoxFor(model => model.mol_p3_2) Mareos asociados a los cambios de posición del cuerpo
                                                                                    </label>
                                                                                </div>
                                                                                <div class="checkbox">
                                                                                    <label>
                                                                                        @Html.CheckBoxFor(model => model.mol_p3_3) Mareos que no se asocian a ninguna actividad específica
                                                                                    </label>
                                                                                </div>
                                                                            </div>

                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>


                                    @*Historial Molestias*@

                                    <div class="row">

                                        <div class="col-md-12">

                                            <div class="panel panel-dark" data-collapsed="0">
                                                <div class="panel-heading">
                                                    <div class="panel-title">Historia de Molestias </div>
                                                    <div class="panel-options pull-right">
                                                        <a href="#" data-rel="collapse"><i class="entypo-down-open"></i></a>
                                                    </div>
                                                </div>
                                                <div class="panel-body chart-responsive">
                                                    @*Pregunta 4*@
                                                    <div class="form-group">
                                                        <div class="row">
                                                            <div class="col-md-12">
                                                                <div class="form-group">
                                                                    <div>
                                                                        <cite>4. Desde hace cuánto lo ha notado por primera vez?</cite><span class="asterisco">(*)</span>
                                                                        <div class="col-md-12">
                                                                            <div class="row">
                                                                                <div class="col-sm-2 col-lg-2">
                                                                                    @Html.TextBoxFor(model => model.mol_p4_1, new { @placeholder = "Cant.", @type = "number", @min = "0", @class = "form-control" })
                                                                                </div>
                                                                                <div class="col-sm-4 col-lg-4">
                                                                                    @Html.DropDownListFor(model => model.mol_p4_2, (SelectList)ViewBag.tiempo, "- Seleccione -", new { @class = "form-control" })
                                                                                </div>
                                                                            </div>
                                                                        </div>

                                                                    </div>

                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    @*Pregunta 5*@
                                                    <div class="form-group">
                                                        <div class="row">
                                                            <div class="col-md-12">
                                                                <div class="form-group">
                                                                    <div>
                                                                        <cite>5. Comó comenzaron sus molestias?</cite><span class="asterisco">(*)</span>
                                                                        <div class="col-md-12">

                                                                            <div class="radio">
                                                                                <label class="checkbox-inline">
                                                                                    @Html.RadioButtonFor(model => model.mol_p5, "true", new { @mostrar_respuesta = "mol_p5_1" }) Subitamente(de un momento a otro)
                                                                                </label>
                                                                                @Html.TextAreaFor(model => model.mol_p5_1, new { @placeholder = "Especifique", @class = "form-control animate validate" })
                                                                            </div>

                                                                            <div class="radio">
                                                                                <label class="checkbox-inline">
                                                                                    @Html.RadioButtonFor(model => model.mol_p5, "false", new { @ocultar_respuesta = "mol_p5_1" })Gradualmente(poco a poco)
                                                                                </label>
                                                                            </div>
                                                                        </div>


                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    @*Pregunta 6*@
                                                    <div class="form-group">
                                                        <div class="row">
                                                            <div class="col-md-12">
                                                                <div class="form-group">
                                                                    <cite>6. Tiene otros antecedentes traumáticos de la columna?</cite>
                                                                    <div class="col-md-12">
                                                                        @Html.TextAreaFor(model => model.mol_p7, new { @placeholder = "Especifique", @class = "form-control animate validate" })

                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    @*Pregunta 7*@
                                                    <div class="form-group">
                                                        <div class="row">
                                                            <div class="col-md-12">
                                                                <div class="form-group">
                                                                    <div>
                                                                        <cite>7. Otros antecedentes médicos de importancia?</cite>
                                                                        <div class="col-md-12">
                                                                            @Html.TextAreaFor(model => model.mol_p8, new { @placeholder = "Especifique", @class = "form-control animate" })
                                                                        </div>
                                                                    </div>

                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>


                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

@*Procedimientos Relacionados*@
                    <div class="row">

                        <div class="col-md-12">
                            <div class="">                               
                                <div class="">
                                    <div class="row">

                                        <div class="col-md-12">

                                            <div class="panel panel-dark" data-collapsed="0">
                                                <div class="panel-heading">
                                                    <div class="panel-title">Procedimientos Relacionados </div>
