<script type="text/javascript">
    function EditExamen() {
        var examen = $("#modal_nexamen").val();
        if ($('#alert_2').length != 0)
            $('#alert_2').remove();
        if (examen == "")
            $("#2").append("<div id='alert_2' class='alert alert-danger'>Ingrese un N° Examen valido</div>")
        else {
            $('#modalExamen').modal('toggle');
            window.location.href = ('@Url.Action("UpdateExamen", "RealizarExamen")?examen=' + examen);
        }
    }
</script>
