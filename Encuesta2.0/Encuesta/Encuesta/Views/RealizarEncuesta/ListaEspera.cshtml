                             { "sName": "isvip", "bVisible": false },//8
                             { "sName": "sedacion", "bVisible": false },//9
                             { "sName": "asginado", "bVisible": false },//10
                             { "sName": "escaneados", "bVisible": false },//11
                             { "sName": "encuesta", "bVisible": false },//12
                             { "sName": "carta", "bVisible": false },//13
                             { "sName": "tipo_encuesta", "bVisible": false },//14
                             { "sName": "isAyer", "bVisible": false },//15

            ],
            "createdRow": function (row, aData, dataIndex) {


                if (aData[15] == "True") {
                    $(row).addClass('warning');
                }
                else {
                    $('td:eq(3)', row).html((aData[4]).substring(11));
                }
                // si es Sedacion
                if (aData[9] == "True") {
                    $(row).addClass('danger alert-danger');
                }
                // si es VIP
                if (aData[8] == "True") {
                    $('td:eq(1)', row).html(aData[1] + ' <i class="fa fa-star fa-lg fa-spin" style="color: #F8DB07;"></i>');
                }
                // minutos trascurridos
                //$('td:eq(4)', row).addClass('texto-medio');
                //if (parseInt(aData[4]) > 40) {
                //    $('td:eq(4)', row).addClass('tiempo_excedente');
                //}
                //else if (parseInt(aData[4]) > 20) {
                //    $('td:eq(4)', row).addClass('tiempo_intermedio');
                //}
                //else {
                //    $('td:eq(4)', row).addClass('tiempo_correcto');
                //}

                // Adjuntos
                var adjuntos = "";
                if (aData[11] == "True" || aData[13] == "True") {
                    adjuntos = ('<button type="button" onclick="DescargarAdjunto(' + aData[0] + ',' + aData[11].toString().toLowerCase() + ',' + aData[13].toString().toLowerCase() + ');" class="btn btn-primary btn-xs "><i class="fa fa-paperclip fa-lg"></i> </button>&nbsp;&nbsp;&nbsp;&nbsp;');

                }

                // realizar encuesta
                if (aData[12] == "False") {//si esta listo para encuestar
                    if (aData[14] == "True") {
                        $('td:eq(6)', row).append('<button type="button" id="btnEncuesta" onclick="RealizarEncuesta(' + aData[0] + ',' + "'" + aData[5] + "'" + ');" class="btn btn-primary "><i class="fa fa-edit fa-2x"></i></button>');
                    } else {
                        $('td:eq(6)', row).append('<button type="button" id="btnEncuesta" onclick="RealizarEncuesta(' + aData[0] + ',' + "'" + aData[5] + "'" + ');" class="btn btn-success " ><i class="fa fa-edit fa-2x"></i></button>');

                    }
                } else {
                    $('td:eq(6)', row).append('<button type="button" onclick="reasignar(' + aData[0] + ');" class="btn btn-default  btn-xs"><i class="fa fa-fire fa-lg"></i></button>');
                }



                //agregarboton de informacion Paciente
                $('td:eq(1)', row).append("<br/>" + adjuntos + '<button type="button" onclick="cargarDatosExamen(' + aData[0] + ')"  class="btn btn-outline btn-info btn-xs" ><i class="fa fa-info-circle fa-lg"></i> Info</button>');



                //agregar botn para Cancelar
                $('td:eq(0)', row).append('<br /><button type="button" onclick="ModalCancelarExamen(' + aData[0] + ')"  class="btn btn-outline  btn-danger btn-xs"><i class="fa fa-times-circle fa-lg "></i> Cancelar</button>');
            }
        });
        $("#div_time").html('<p class="pull-right">Last Refresh : <b>' + myDate.toLocaleTimeString() + '</b></p>');
        $('#myDataTable').css("width", "100%!important");
        //setInterval(function () {
        //    table.ajax.reload();
        //    $('#myDataTable').width("100%");
        //    var _date = new Date();
        //    $("#div_time").html('<p class="pull-right">Last Refresh : <b>' + _date.toLocaleTimeString() + '</b></p>');
        //}, 30000);
        getSucursales(table);//tipo filtro
        //getEquipo(table, "2");//tipo Modal


    });
    function cargarTabla() {

        table.ajax.reload();

        var _date = new Date();
        $("#div_time").html('<p class="pull-right">Last Refresh : <b>' + _date.toLocaleTimeString() + '</b></p>');
    }

    function ModalCancelarExamen(examen) {
        $('#inputhidden_id').val(examen);
        $('#modal_Cancelar').modal('toggle');
    }

    function RealizarEncuesta(examen, estudio) {
        @{ Encuesta.Member.CustomMembershipUser user = Membership.GetUser(true) as Encuesta.Member.CustomMembershipUser;}
        if (estudio.toLowerCase().includes("artro"))
            if (!('@user.ProviderUserKey.ToString().ToUpper()' == 'JCCH25' || '@user.ProviderUserKey.ToString().ToUpper()' == 'GDCV18')) {
                toastr.error("La sesión iniciada no es de un médico que realice el procedimiento.", opts);
                return;
            }
        window.location.href = ('@Url.Action("RealizarEncuesta", "RealizarEncuesta")' + '?examen=' + examen);
    }

    function getSucursales(table) {
        $.ajax({
            url: "@Url.Action("getSedes", "RealizarEncuesta")",
            data: {},
            dataType: "json",
            type: "POST",
            error: function () {
                toastr.error("error cargar sedes", opts);
            },
            success: function (data) {
                var items = "";
                items += "<option value='' >- Todos -</option>";
                $.each(data, function (i, item) {
                    items += "<option value=\"" + item.Value + "\">" + item.Text + "</option>";
                });
                $("#lstsede").html(items);
                $("#lstsede").change(function () {
                    var texto = $("#lstsede option:selected").html();
                    if ($("#lstsede").val() != '') {
                        $('#myDataTable').dataTable().fnFilter(texto);
                    }
                    else {
                        $('#myDataTable').dataTable().fnFilter('');
                    }
                });
                var parameter = $.get("sede");
                if (parameter != null) {
                    $("#lstsede").val(parameter);
                    var texto = $("#lstsede option:selected").html()
                    $('#myDataTable').dataTable().fnFilter(texto);
                }



            }
        });
    }

    function DescargarAdjunto(examen, doc, carta) {
        if (doc) {
            window.open('@Url.Action("Getfile", "Adjuntos")' + '?examen=' + examen + '&tipo=1');
        }
        if (carta) {
            window.open('@Url.Action("Getfile", "Adjuntos")' + '?examen=' + examen + '&tipo=2');
        }
    }

    function cargarDatosExamen(examen) {
        $.ajax({
            url: "@Url.Action("DatosExamen", "RealizarEncuesta")",
            data: { examen: examen },
            dataType: "json",
            type: "POST",
            error: function () {
                toastr.error('Ocurrio un problema y <b>no</b> se enviaron los datos al sistema', opts);
            },
            success: function (data) {
                $('#myModal .modal-body').html(data.mensaje);
                $('#myModal').modal('toggle');
            }
        });

    }
    function CalculateDateDiff(dateFrom, dateTo) {
        var difference = (dateTo - dateFrom);

        var years = Math.floor(difference / (1000 * 60 * 60 * 24 * 365));
        difference -= years * (1000 * 60 * 60 * 24 * 365);
        var months = Math.floor(difference / (1000 * 60 * 60 * 24 * 30.4375));

        var dif = '';
        if (years > 0)
            dif = years + ' años ';

        if (months > 0) {
            if (years > 0) dif += ' y ';
            dif += months + ' meses';
        }

        return dif;
    }




</script>


