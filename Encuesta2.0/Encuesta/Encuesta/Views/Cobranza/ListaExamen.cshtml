@model IEnumerable<Encuesta.ViewModels.Tecnologo.RealizarExamenViewModel>
@{
    ViewBag.Title = "<i class='fa fa-shopping-cart'></i> Cobranza";
    ViewBag.Titlenot = "Cobranza";
}

<!-- DataTables JavaScript -->


<script src="~/js/plugins/dataTables/jquery.dataTables.js"></script>
<script src="~/js/plugins/dataTables/dataTables.bootstrap.js"></script>

<script src="~/js/getUrl.js"></script>
<script src="~/js/jquery-ui.min.js"></script>
<script>
    var table;
    var myDate = new Date();

    $(document).ready(function () {
        $("#msj").fadeOut()//ocultar el msj
        table = $('#myDataTable').DataTable({
            "bServerSide": true,
            "sAjaxSource": "@Url.Action("ListaAsync", "Cobranza")",
            //"bProcessing": true,
            "iDisplayLength": 20,
            "lengthMenu": [[20, 25, 50, -1], [20, 25, 50, "All"]],
            "aoColumns": [
                             { "sName": "N Examen" },//0
                             { "sName": "Paciente" },//1
                             { "sName": "HCita" },//2
                             { "sName": "HAdmin." },//3
                             { "sName": "Min." },//4
                             { "sName": "Concicion" },//5
                             { "sName": "Estudio" },//6
                             { "sName": "Equipo Asignado" },//7
                             {
                                 "sName": "Acciones",
                                 "mRender": function (data, type, full) {
                                     return "";
                                 }
                             , "bVisible": false
                             },//8
                             { "sName": "isvip", "bVisible": false },//9
                             { "sName": "validado", "bVisible": false },//10 Supervisado
                             { "sName": "validacion", "bVisible": false },//11 Solicita validacionTenologo
                             { "sName": "sedacion", "bVisible": false },//12
                             { "sName": "isIniciado", "bVisible": false },//13 enviado a la modalidad
                             { "sName": "isActivado", "bVisible": false },//14 si ya paso mas de 15 min para solicitar supervision
                             { "sName": "minrestante", "bVisible": false }//15 mint faltantes
            ],
            "createdRow": function (row, aData, dataIndex) {
                //celdad a ocultar
                /* $('td:eq(5)', row).addClass('ocultar1');
                 $('td:eq(5)', row).css('d', 'd');
                 */

                var isVisibleContraste = true;
                if (aData[16] == "True") {
                    isVisibleContraste = false;
                } else {
                    isVisibleContraste = true;
                }
                // si es una validacion pendiente
                var fec_admi = new Date((aData[3]).substring(6, 10), parseInt((aData[3]).substring(3, 5)) - 1, (aData[3]).substring(0, 2), 0, 0, 0);
                var fec_now = new Date(new Date().getFullYear(), new Date().getMonth(), new Date().getDate(), 0, 0, 0);
                if (fec_admi < fec_now) {
                    $(row).addClass('warning');
                }
                else {
                    $('td:eq(3)', row).html((aData[3]).substring(11));
                }
                // si es Sedacion
                if (aData[12] == "True") {
                    $(row).addClass('danger alert-danger');
                }
                // si es VIP
                if (aData[9] == "True") {
                    $('td:eq(1)', row).html(aData[1] + '&nbsp;&nbsp;&nbsp; <i class="fa fa-star fa-lg fa-spin" style="color: #F8DB07;"></i>');
                }
                // si esta SUpervisado
                if (aData[10] == "True") {
                    if (isVisibleContraste) {
                        $('td:eq(0)', row).html(aData[0] + '&nbsp;&nbsp;&nbsp;<i class="fa fa-check fa-lg " style="color: #47DB3B"></i>');
                    }
                    else {
                        $('td:eq(0)', row).html(aData[0] + '&nbsp;&nbsp;&nbsp;<i class="fa fa-eyedropper fa-lg " style="color: #ff9600"></i>');
                    }
                }
                    // si esta esperando ser SUpervisado
                else if (aData[11] == "True") {
                    $('td:eq(0)', row).html(aData[0] + '&nbsp;&nbsp;&nbsp;<i class="fa fa-spinner fa-lg fa-spin" style="color: #F8A003"></i>');
                }
                    // si estaenviado a la modalidad
                else if (aData[13] == "True") {
                    $('td:eq(0)', row).html(aData[0] + '&nbsp;&nbsp;&nbsp;<i class="fa fa-joomla fa-lg fa-spin" style="color: #078EF8"></i>');

                    /*if (aData[14] == "True") {
                        $('td:eq(8)', row).html('<input type="button" onclick="SolicitarSupervision(' + aData[0] + ')"  class="btn btn-primary" value="Supervisión"/>');
                    }
                    else {
                        $('td:eq(8)', row).html("Faltan : "+ aData[0] +" .min")
                   
                    }*/
                }
                    // si aun no lo revisan
                else {
                    $('td:eq(0)', row).html(aData[0]);//mostrar solo en N Examen
                }

                // minutos trascurridos
                if (parseInt(aData[4]) > 40) {
                    $('td:eq(4)', row).addClass('tiempo_excedente');
                }
                else if (parseInt(aData[4]) > 20) {
                    $('td:eq(4)', row).addClass('tiempo_intermedio');
                }
                else {
                    $('td:eq(4)', row).addClass('tiempo_correcto');
                }

            }
        });

        $("#div_time").html('<p class="pull-right">Last Refresh : <b>' + myDate.toLocaleTimeString() + '</b></p>');
        $('#myDataTable').css("width", "100%!important");
        setInterval(function () {
            table.ajax.reload();
            $('#myDataTable').width("100%");
            var _date = new Date();
            $("#div_time").html('<p class="pull-right">Last Refresh : <b>' + _date.toLocaleTimeString() + '</b></p>');
        }, 30000);
        //getEquipo(table, "2");//tipo Modal


    });



    function CalculateDateDiff(dateFrom, dateTo) {
        var difference = (dateTo - dateFrom);

        var years = Math.floor(difference / (1000 * 60 * 60 * 24 * 365));
        difference -= years * (1000 * 60 * 60 * 24 * 365);
        var months = Math.floor(difference / (1000 * 60 * 60 * 24 * 30.4375));

        var dif = '';
        if (years > 0)
            dif = years + ' años ';

        if (months > 0) {
            if (years > 0) dif += ' y ';
            dif += months + ' meses';
        }

        return dif;
    }


    

</script>

<div class="row">
    <div class="col-lg-12">
        <div class="box box-primary">
            <div class="box-header">
                <h2 class="box-title">Lista de Examenes </h2>
            </div>
            <div class="box-body chart-responsive">

                <div class="panel panel-primary">
                    <div class="panel-heading">
                        Lista de Examenes 
                   
                    </div>

                    <!-- /.panel-heading -->
                    <div class="panel-body">
                        <div class="alert" id="msj">
                        </div>
                        <div class="row">
                            <div class="col-lg-4">
                                <label>Leyenda</label>
                            </div>
                            <div id="div_time" class="col-lg-8">
                            </div>

                        </div>
                        <div class="row">
                            
                            <div class="col-lg-10">
                                <div class="col-lg-2"><i style="color: #FFF" class='fa fa-stop fa-2x '></i><b> &nbsp;Recien Encuestado</b></div>
                                <div class="col-lg-2"><i style="color: #078EF8" class='fa fa-joomla fa-2x fa-spin'></i><b> &nbsp;Adquision  Imagenes</b></div>
                                <div class="col-lg-2"><i style="color: #F8A003" class='fa fa-spinner fa-2x fa-spin'></i><b> &nbsp;Esperando Supervisión</b></div>
                                <div class="col-lg-2"><i style="color: #ff9600" class='fa fa-eyedropper fa-2x'></i><b> &nbsp;Esperando Enfermera</b></div>
                                <div class="col-lg-2"><i style="color: #47DB3B" class='fa fa-check fa-2x'></i><b> &nbsp;Esperando Tecnólogo</b></div>

                            </div>
                        </div>
                    <br />
                    <div class="table-responsive">
                        <table id="myDataTable" class="table table-striped table-bordered table-hover">
                            <thead>
                                <tr>
                                    <th>N° Examen</th>
                                    <th>Paciente</th>
                                    <th>H. Cita</th>
                                    <th>H. Admin.</th>
                                    <th>Min. </th>
                                    <th>Cond.</th>
                                    <th>Estudio</th>
                                    <th>Equipo Asignado</th>
                                    <th></th>
                            </thead>
                            <tbody>
                            </tbody>
                        </table>

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
</div>

<!-- Modal Datos -->
<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title" id="myModalLabel">Datos de Examen</h4>
            </div>
            <div class="modal-body">
            </div>

            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Cerrar</button>
            </div>
        </div>
        <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
</div>
<script type="text/javascript">
    function EditExamen() {
        var examen = $("#modal_nexamen").val();
        if ($('#alert_2').length != 0)
            $('#alert_2').remove();
        if (examen == "")
            $("#2").append("<div id='alert_2' class='alert alert-danger'>Ingrese un N° Examen valido</div>")
        else {
            $('#modalExamen').modal('toggle');
            window.location.href = ('@Url.Action("UpdateExamen", "RealizarExamen")?examen=' + examen);
        }
    }
</script>
<!-- Modal Buscar Examen-->
<div class="modal fade" id="modalExamen" tabindex="-1" role="dialog" aria-labelledby="modalExamenLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title" id="modalExamenLabel">Ingrese Número de Examen</h4>
            </div>
            <div class="modal-body" id="2">
                <div class="input-group custom-search-form">
                    <input type="number" class="form-control" id="modal_nexamen" placeholder="N° Examen...">
                    <span class="input-group-btn">
                        <button class="btn btn-default" type="button" onclick="EditExamen();">
                            <i class="fa fa-search"></i>
                        </button>
                    </span>
                </div>
            </div>

            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Cerrar</button>
            </div>
        </div>
        <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
</div>

<script type="text/javascript">
    function ActualizarDatos() {
        var nexa = ($('#modalidad_examen').val());
        var fec = ($('#modal_Modalidad_Fecha').val());
        var talla = ($('#modal_Modalidad_talla').val());
        var pes = ($('#modal_Modalidad_Peso').val());
        $.ajax({
            url: "@Url.Action("ActualizarDatosAdmision", "RealizarExamen")",
            data: { nexamen: nexa, talla: talla, peso: pes, fecha: fec },
            dataType: "json",
            type: "POST",
            error: function () {
                mostrarmensaje(4, 'Ocurrio un problema y <b>no</b> se enviaron los datos al sistemas');
            },
            success: function (data) {
                EnviarModalidad(nexa);
                $('#modal_Modalidad').modal('toggle');
            }
        });
    }
</script>

<!-- Modal Datos -->
<div class="modal fade" id="modal_Modalidad" tabindex="-1" role="dialog" aria-labelledby="modal_ModalidadLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title" id="modal_ModalidadLabel">Confirmar Datos de Examen</h4>
            </div>
            <div class="modal-body">
            </div>

            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Cerrar</button>
                <button type="button" class="btn btn-primary" onclick="ActualizarDatos();">Enviar</button>
            </div>
        </div>
        <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
</div>
